EbaumComment.prototype.handleClick=function(a){var a=a||window.event,a=a.target||a.srcElement,b=a.id.split("-"),c=b[1];switch(b[0]){case "addComment":this.relay(CommentEvent.ADD,a);break;case "addReply":this.relay(CommentEvent.ADD_REPLY,a);break;case "reply":this.replyBtn(c);break;case "ban":this.banBtn(a);break;case "confirmBan":this.confirmBan(a);break;case "delete":this.deleteBtn(a);break;case "addCommentText":jQuery(a).val()=="Add your comment here. 300 character Max."&&this.clearInput(a);break;
