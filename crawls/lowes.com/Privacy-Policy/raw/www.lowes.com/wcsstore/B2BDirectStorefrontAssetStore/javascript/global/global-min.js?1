").length){return"compare";}else{if($(".product-listing").length){return"list";}else{if($("#cbc-anon-landing").length){return"cbc";}else{return false;}}}}}}}},fireCoremetrics:function(eventType,catEntryId,price,clickType){if(!Lowes.User.isZippedIn()){return false;}var cmObject={},cmCategory="MAP Product Add to Cart Process";switch(clickType||this.getClickType()){case"detail":cmCategory+=" - Detail Page";if($(".button.add").length){cmObject[2]=$(".button.add").attr("id").split("_")[1];}else{cmCategory="";}break;case"list":cmCategory+=" - List Page";if(eventType=="Finish"){cmObject[2]=catEntryId;}else{cmCategory="";}break;case"quickview":cmCategory+=" - List Page - Quickview";cmObject[2]=catEntryId;break;default:cmCategory="";break;}if(typeof price!="undefined"&&price.length&&eventType=="Finish"){cmObject[3]="MAP Item Price "+Lowes.Utils.formatCurrency(parseFloat(price));}if(typeof cmCategory!="undefined"&&cmCategory.length){cmObject[1]=(typeof(cmProductviewData)!="undefined")?cmProductviewData.pagename:(typeof(cmPageviewData)!="undefined"?cmPageviewData.pageName:"undefined");cmCreateConversionEventTag(cmCategory,eventType,"MAP Item","",cmAttrCreator(cmObject));}},redirectToCart:function(itemData){var cartParams="";$.each(itemData,function(key,val){if(key!="isAjaxItemAdd"){cartParams+="&"+key+"="+val;}});addToCartURL+=cartParams;window.location=addToCartURL;}};}());if($.inArray($("body").attr("id"),Lowes.MiniCart.excludedPageIds)==-1&&!$("#error-page").length){Lowes.MiniCart.init();}