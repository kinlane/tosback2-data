ocess";switch(w||this.getClickType()){case"detail":q+=" - Detail Page";if($(".button.add").length){v[2]=$(".button.add").attr("id").split("_")[1];}else{q="";}break;case"list":q+=" - List Page";if(r=="Finish"){v[2]=u;}else{q="";}break;case"quickview":q+=" - List Page - Quickview";v[2]=u;break;default:q="";break;}if(typeof s!="undefined"&&s.length&&r=="Finish"){v[3]="MAP Item Price "+Lowes.Utils.formatCurrency(parseFloat(s));}if(typeof q!="undefined"&&q.length){v[1]=(typeof(cmProductviewData)!="undefined")?cmProductviewData.pagename:(typeof(cmPageviewData)!="undefined"?cmPageviewData.pageName:"undefined");cmCreateConversionEventTag(q,r,"MAP Item","",cmAttrCreator(v));}},redirectToCart:function(q){var r="";$.each(q,function(s,u){if(s!="isAjaxItemAdd"){r+="&"+s+"="+u;}});o+=r;window.location=o;}};}());if($.inArray($("body").attr("id"),Lowes.MiniCart.excludedPageIds)==-1&&!$("#error-page").length){Lowes.MiniCart.init();}