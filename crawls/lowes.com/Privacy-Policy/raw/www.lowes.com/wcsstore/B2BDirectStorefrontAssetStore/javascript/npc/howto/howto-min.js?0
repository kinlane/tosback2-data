(function($){$.widget("ui.expando",{options:{smallClass:"ui-expando-small",toggleButtonClass:"ui-size-toggle",easingOut:"easeInOutExpo",easingIn:"easeInOutExpo",expanderMargin:"20px",duration:1200,largeWidth:"90%",smallWidth:"50%"},_setOption:function(key,value){$.Widget.prototype._setOption.apply(this,arguments)},_create:function(){var $expander=this.element.find("div:first"),that=this;$expander.append('\x3cdiv class\x3d"'+this.options.toggleButtonClass+'"\x3e\x3c/div\x3e');this.element.delegate(".ui-size-toggle",
"click.expando",function(){var isSmall=$expander.hasClass(that.options.smallClass),margins=that.element.hasClass("ui-expando-left")?"marginRight":"marginLeft",widthVal=isSmall?that.options.largeWidth:that.options.smallWidth,easing=isSmall?that.options.easingIn:that.options.easingOut,animation={width:widthVal};animation[margins]=isSmall?"0":that.options.expanderMargin;var callback=function(){$expander.toggleClass(that.options.smallClass,!isSmall)};$expander.animate(animation,that.options.duration,
easing,callback)})},destroy:function(){this.element.find("."+this.options.toggleButtonClass).remove();this.element.unbind("click.expando");return $.Widget.prototype.destroy.call(this)}})})(jQuery);$(".ui-expando-module").expando({largeWidth:"95%"});
(function(){var $printable=$(".ui-printable-list");var printMaterials={notes:'\x3ch3 class\x3d"print" id\x3d"checklist-notes"\x3eNotes* ( Location in store, quantity, quality, etc. )\x3c/h3\x3e',init:function(){var self=this;$.ajax({url:"/wcsstore/B2BDirectStorefrontAssetStore/css/npc/howto/printHijack.css",dataType:"text",success:function(data){$("head").append('\x3cstyle class\x3d"printHijack"\x3e'+data+"\x3c/style\x3e");$printable.after(self.notes)},complete:function(){window.print()}})},destroy:function(){setTimeout(function(){$(".printHijack").remove();
$printable.next().remove()},1E3)}};$(document).delegate(".print-button","click.printHijack",function(e){e.preventDefault();printMaterials.init();printMaterials.destroy()})})();