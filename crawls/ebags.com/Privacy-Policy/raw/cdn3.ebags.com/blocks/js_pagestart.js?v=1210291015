function openSesame(location,name,toolbar,width,height,resizable,scrollbars,status,left,top) {
var featureString = ("toolbar=" + toolbar + ",width=" + width + ",height=" + height + ",resizable=" + resizable + ",scrollbars=" + scrollbars + ",status=" + status + ",left=" + left + ",top=" + top);
window.open(location,name,featureString);	
}

function summonChart() {  // for comparison chart
    var now = new Date();
    var thetime = now.getTime();
    window.open("/info/Compare/index.cfm?fuseaction=chart&milliseconds=" + thetime, "chart", "toolbar=no,width=640,height=480,resizable=yes,scrollbars=yes,status=no");
}
function explainChart() {
    window.open("", "chart_explained", "toolbar=no,width=545,height=435,resizable=no,scrollbars=no,status=no");
}	 

function SwatchPop() {
window.open("","color","toolbar=no,width=200,height=168,resizable=no,scrollbars=no,status=no");
}

function QuickiePop() {
    window.open("", "q_signup", "toolbar=no,width=450,height=400,resizable=no,scrollbars=yes,status=no");
}	 
