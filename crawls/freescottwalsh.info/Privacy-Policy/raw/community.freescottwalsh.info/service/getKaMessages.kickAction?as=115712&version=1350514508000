	addProp: function(prop,val){this[prop]=val;},
	replaceTokens: function(string, variables) {
		try {
		    for(obj in variables){
		        string = string.replace(new RegExp("\\(\\$" + obj + "\\)","g"), variables[obj]); 
		    }
	    } catch(ex) {}
	    return string;
	}
};