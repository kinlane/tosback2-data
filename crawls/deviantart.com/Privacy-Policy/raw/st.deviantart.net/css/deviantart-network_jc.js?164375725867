this,function(){PubSub.publish("WriterAnywhere.collapse");return false}))},after_collapse:function(c){this.base.apply(this,[c]);var b;if(this.ccwriter_toplevel){b=this.$.closest(":gmi(TalkPostWrapper)").gmi1().talkpost}else{b=this.$.closest(".talk-post").data("TalkPost")}a(b.node).find(".drawPlz .showMedia").text("Add Media").off("click.writeranywhere").on("click.writeranywhere",bind(this,function(){PubSub.publish("WriterAnywhere.expand");return false}))}})})(jQuery);if(window.DWait){DWait.run("jms/pages/writeranywhere/comment.js")}});if(window.DWait){DWait.count()}