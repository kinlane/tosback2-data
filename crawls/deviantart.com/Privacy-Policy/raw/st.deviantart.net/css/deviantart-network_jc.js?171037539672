 Comment",n.done(bind(this,function(){this.writer.$node.css({"min-height":110})})),this.$.addClass("contains-dots").append(e('<div class="writer-dotdotdot">...</div>').attr("title",r).on("click.writeranywhere",bind(this,function(n){n.preventDefault(),t.each(bind(this,function(t,n){var r=e(n).clone();r.find(".thumb-attachment,.cc-signature").remove(),r.find(".thumb-attachment-content").replaceWith(function(){return e(this).data("html")}),WriterEmbed.normalizeServerRenderedDaml(r),r.find('img[src^="http://e.deviantart."][alt]').replaceWith(function(){return e(this).attr("alt")}),e("<blockquote>"+r.html()+"</blockquote>").appendTo(this.writer.$node)})),this.writer.sync(),WriterEmbed.damlToLoaders(this.writer.$node),WriterEmbed.runLoaders(this.writer.$node),this.$.find(".writer-dotdotdot").trigger("mouseleave").remove(),e(".writer-dotdotdot-tooltip").remove()})).on("mouseenter",function(t){var n=e(this);n.attr("title")&&(n.data("title",n.attr("title")),n.removeAttr("title")),n.after(e("<div>",{text:n.data("title"),"class":"writer-dotdotdot-tooltip"}))}).on("mouseleave",function(t){e(".writer-dotdotdot-tooltip").remove()}))}));if(this.ccwriter_toplevel){if(t){var i=t.gmi_args.itemid,s=t.gmi_args.typeid;if(s in{61:!0,1:!0}){var o="Quote Original Submission";window.deviantART&&window.deviantART.pageData.catpath.indexOf("journals/")===0&&(o="Quote Original Journal");var u=e("#gmi-ResourcePageDisplayPane .text");u.find(".da-quotable").length&&(u=u.find(".da-quotable")),r.resolve(u,o)}}r.reject()}else this.$.closest(".mcbox").length?r.resolve(this.$.closest(".mcbox").prev(".mcbox").find(".mcb-body")):r.resolve(this.$.closest("div.nest").prev(".ccomment-activereply").find("div.text"))}vms_feature("comment_writer_webscale")&&!e("body").hasClass("ccwriter-webscale")&&e("body").addClass("ccwriter-webscale"),PubSub.subscribe([{eventname:"WriterAnywhere.hooked",subscriber:this,callback:function(){PubSub.unsubscribe([{eventname:"WriterAnywhere.hooked",subscriber:this}]),e(document).on("click.ccwriter",bind(this,function(t){if(!e(t.target).closest(":gmi(CCommentThread), :gmi(TalkPostWrapper), .ccomment-post, .commentwriter-interface").length){var n=this.$.closest(":gmi(TalkPostWrapper)").gmi1();n&&!this.$.find(".writer").html()&&(n.textarea_focused=0,n.refreshTextareaState(),e(n.gmi_node).find(".inputs").hide(),n.talkpost.confirmExit())}}))}}])},after_expand:function(){this.base.apply(this),Glbl("Site.is_stash")&&e(this.$writer_ui[0]).find(".commentwriter-cancel em").text("Close");var t;this.ccwriter_toplevel?t=this.$.closest(":gmi(TalkPostWrapper)").gmi1().talkpost:t=this.$.closest(".talk-post").data("TalkPost"),t.showCancel(),e(this.$writer_ui[0]).find(".commentwriter-cancel").show().on("click.writeranywhere",bind(this,function(){return t.confirmCancel(),!1})),e(this.$writer_ui[0]).find(".commentwriter-submit").show().on("click.writeranywhere",bind(this,function(){return PubSub.publish("WriterAnywhere.collapse"),t.confirmPost(),!1})).find("em").text("Submit Comment"),e(this.$writer_ui[0]).find(".commentwriter-preview").show().on("click.writeranywhere",bind(this,function(){return t.confirmPreview(),!1})),e(t.node).find(".drawPlz .showMedia").addClass("hideMedia").text("Hide Media").off("click.writeranywhere").on("click.writeranywhere",bind(this,function(){return PubSub.publish("WriterAnywhere.collapse"),!1}))},after_collapse:function(t){this.base.apply(this,[t]);var n;this.ccwriter_toplevel?n=this.$.closest(":gmi(TalkPostWrapper)").gmi1().talkpost:n=this.$.closest(".talk-post").data("TalkPost"),e(n.node).find(".drawPlz .showMedia").removeClass("hideMedia").text("Add Media").off("click.writeranywhere").on("click.writeranywhere",bind(this,function(){return PubSub.publish("WriterAnywhere.expand"),!1})),e(document).off("click.ccwriter")},after_shutdown:function(){e(document).off("click.ccwriter")}})})(jQuery),window.DWait&&DWait.run("jms/pages/writeranywhere/comment.js")});
if(window.DWait){DWait.count()}