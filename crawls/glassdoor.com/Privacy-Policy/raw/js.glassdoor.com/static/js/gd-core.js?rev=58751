GD.jobScope._loadMapContent=function(a,b){var c=a.parents(".jobScopePanel:first").attr("id");GD.maps.loadMap({$tabContent:a,jobInfo:b,jobScopePanelId:c,searchTerms:['"'+b.employerName+'"','"'+b.employerShortName+'"']})};GD.maps={};GD.maps.loadMap=function(a){GD.maps.loadMap=function(){};GD.maps.loadParams=a;a=document.createElement("script");a.type="text/javascript";a.src="//maps.googleapis.com/maps/api/js?v=3.6&libraries=places&sensor=false&callback=GD.maps.initialize";(document.body||document.documentElement).appendChild(a)};
