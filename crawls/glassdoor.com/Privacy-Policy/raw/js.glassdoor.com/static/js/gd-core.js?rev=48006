function(q,r){return GD.ajax._formatACItem(n,q,r)}};GD.ajax.selectAutoComplete._onKeydown=function(b,c,d){c=true;d=d.val().trim().length>0;if(b&&d){if(b.which===13)c=false;switch(String.fromCharCode(b.which)){case "\t":c=false;break;case "\n":case "\r":c=false}}if(!c){b.preventDefault();b.stopPropagation()}};GD.ajax.selectAutoComplete._selectItem=function(b,c,d,e,f){GD.ajax.selectAutoComplete._addSelectedItem(b,c,d,e,f);d.selected=true};
GD.ajax.selectAutoComplete._addSelectedItem=function(b,c,d,e,f){var g="div ";if(f)g="span ";f="<"+g+'class="selectedItem" title="%TITLE%"><span class="textPart"></span><img src=\'/static/img/spacer.gif\' class=\'closeBox\' /></'+g+">";var h="<"+g+"class=\"selectedItem\" title=\"%TITLE%\"><a href='%URL%' target='_blank'><span class='textPart'></span></a><img src='/static/img/spacer.gif' class='closeBox' /></"+g+">";g=d.label;var j=b.data("selectName");if(jQuery.isFunction(e))if(e=e(b,d))f=h.replace(/%URL%/,
e);e="ID "+d.id;e=e.replace('"',"");f=f.replace(/%TITLE%/,e);f=$(f);c.before(f);f.attr("value",d.id);$(".textPart",f).html(g);$("img",f).click(function(k){GD.ajax.selectAutoComplete._onCloseBoxClick(b,d,k);k.stopPropagation()});$('input[type="hidden"][value="'+d.id+'"]',b).length==0&&b.append('<input type="hidden" name="'+j+'" value="'+d.id+'" />')};GD.ajax.selectAutoComplete._onCloseBoxClick=function(b,c){GD.ajax.selectAutoComplete._deselectItem(b,c.id)};
GD.ajax.selectAutoComplete._deselectItem=function(b,c){var d=$('.selectedItem[value="'+c+'"]',b),e=b.data("acItems",e),f=false;b.data("selectName");d.remove();$("input.acEditField",b).focus();for(d=0;d<e.length&&!f;d++){var g=e[d];if(g.id==c){g.selected=false;f=true}}e=$('input[type="hidden"][value="'+g.id+'"]',b);e.length&&e.remove()};
GD.ajax.getMatchingItemsFromStaticList=function(b,c,d,e){var f=[];if(b&&b.term){b=b.term.replace(/[-[\]{}()*+?.,\\^$|#\s]/g,"\\$&");b=RegExp(b,"i");for(var g=0;g<d.length&&f.length<e;g++)if(d[g].label&&b.test(d[g].label))f.push(d[g]);else b.test(d[g])&&f.push(d[g])}c(f)};GD.ajax.DISAMBIG_RESULTS_WINDOW_ID="AjaxLocationDisambigResults";GD.ajax.TRUNC_NUM_RESULTS_TO_SHOW=5;
GD.ajax.initLocationDisambig=function(b,c,d,e,f,g,h,j,k,l,m,n,u,q){var r=$(b),p=$(q),t=r.closest("form");if(typeof u!=="boolean")u=false;j||(j=330);if(t.length){var v=$(c),C=$(d);r=$(b);t.bind("submit",function(){var K=true;GD.augments.uninstallPlaceholders(r);var o=r.val();if(o)o=o.trim();if(o){if(!t.attr("disambigComplete")){GD.ajax._doLocationDisambig(r,v,C,G,f,g,h,j,k,l,m,n,u,p);K=false}}else t.attr("disambigComplete","true");return K});var G=$(e);b=GD.ajax._getDisambigKeyDownFunc(v,C,G);r.keydown(b)}};
GD.ajax._getDisambigKeyDownFunc=function(b,c,d){return function(e){if(e.keyCode<33||e.keyCode>40){b.val("");c.val("");d.val("")}GD.ajax._closeLocDisambigPanel();return true}};
GD.ajax._doLocationDisambig=function(b,c,d,e,f,g,h,j,k,l,m,n,u,q){var r=$(b),p=$(c),t=$(d),v=$(e);GD.augments.uninstallPlaceholderField(r);var C="";if(C=r.val()){C=C.trim();r.val(C)}allowPostalCodes=q.val()=="true";C||GD.ajax._handleAllLocationSelect(r,p,t,v,l);b="/findLocationsByFullTextAjax.htm?locationSearchString="+encodeURIComponent(C);b+="&allowPostalCodes="+allowPostalCodes;b=GD.ajax.getAjaxUrlRespectingSecurity(b);b={url:b,dataType:"json",success:function(G){GD.ajax._disambigLocationAjaxHandler(G.locations,
C,f,g,h,j,k,r,p,t,v,l,m,u)},error:function(){GD.ajax._disambigLocationAjaxHandler(null,C,f,g,h,j,k,null,null,null,null,null,m,u);disambigHandler=GD.ajax._getDisambigHandler();disambigHandler()}};if(m&&n){m=$(m);m.attr("defaultLabel",m.html()).attr("submittingLabel",n).html(n)}jQuery.ajax(b);GD.augments.augmentPlaceholderField(r);return false};
GD.ajax._disambigLocationAjaxHandler=function(b,c,d,e,f,g,h,j,k,l,m,n,u,q){if(b){k=$(k);l=$(l);m=$(m);if(b.length==1||b.length>0&&q){GD.ajax._closeLocDisambigPanel();d=b[0];GD.ajax._setLocation(d.name,d.type,d.id,j,k,l,m);j.parents("form:first").attr("disambigComplete","true");n()}else{var r=null;if(b.length==0)if(q)GD.ajax._handleAllLocationSelect(j,k,l,m,n);else{r="";r+="<div class='locDisambigNoResults'>";r+="<img src='/static/img/spacer.gif' class='warning' />";r+="<p class='header'>No matches for <strong>"+
c+"</strong></p>";r+="<p>Please try a different location or see results for ";r+='<a href="javascript:noop();">all available locations</a>.</p>';r+="</div>";r=$(r);$("a",r).mousedown(function(){GD.ajax._handleAllLocationSelect(j,k,l,m,n)})}else r=GD.ajax._populateSelectPanel(c,b,j,k,l,m,n);GD.ajax._restoreSubmitLabel(u);if(r){GD.ajax._showLocDisambigPanel(d,e,f,g,h,j);GD.dialogManager.panelSetContents("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID,r)}}}else if(q)GD.ajax._handleAllLocationSelect(j,k,l,m,n);
else{b="<p><strong>An error occurred in processing the location '"+c+"'.</strong></p><p>Please try again.  If the problem persists, please be assured that our engineers are scrambling to fix the problem.</p>";GD.ajax._restoreSubmitLabel(u);GD.augments.augmentPlaceholderField(j);GD.ajax._showLocDisambigPanel(d,e,f,g,null,j);GD.dialogManager.panelSetContents("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID,b)}};
GD.ajax._populateSelectPanel=function(b,c,d,e,f,g,h){var j=$("<div class='locDisambigPanel'>"),k=$("<div class='locDisambigResults truncDisambig'>"),l,m=GD.ajax.TRUNC_NUM_RESULTS_TO_SHOW;j.append("<img src='/static/img/spacer.gif' class='circledGreenCheck' />");j.append("<p class='header'>We found <strong>"+c.length+"</strong> results for <strong>"+b+"</strong></p>");if(c.length>GD.ajax.TRUNC_NUM_RESULTS_TO_SHOW+1)l=$("<div class='locDisambigResults fullDisambig hidden'>");else c.length==GD.ajax.TRUNC_NUM_RESULTS_TO_SHOW+
1&&m++;for(b=0;b<c.length;b++){var n=c[b],u;u=GD.ajax._getSelectPanelItem(n,d,e,f,g,h);c.length>m&&l.append(u);if(b<m){u=GD.ajax._getSelectPanelItem(n,d,e,f,g,h);k.append(u)}}if(c.length>m){k.append("<p class='footer'><a href='javascript:noop();' onmousedown='return GD.ajax._showFullDisambigList(true);'>More results</a></p>");l.append("<p class='footer'><a href='javascript:noop();' onmousedown='return GD.ajax._showFullDisambigList(false);'>Fewer results</a></p>")}j.append(k);l&&j.append(l);return j};
GD.ajax._getSelectPanelItem=function(b,c,d,e,f,g){var h=$("<p>"),j=$("<a href='javascript:noop();'></a>");c=GD.ajax._getLocSelectHandler(c,b.name,d,b.type,e,b.id,f,g);j.html(b.name);j.click(c);h.append(j);return h};GD.ajax._getLocSelectHandler=function(b,c,d,e,f,g,h,j){return function(){GD.ajax._handleLocationSelect(b,c,d,e,f,g,h,j);return false}};
GD.ajax._showFullDisambigList=function(b){if(typeof b!=="boolean")b=true;if(b){$("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID+" .truncDisambig").hide();$("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID+" .fullDisambig").show()}else{$("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID+" .fullDisambig").hide();$("#"+GD.ajax.DISAMBIG_RESULTS_WINDOW_ID+" .truncDisambig").show()}return false};
GD.ajax._handleLocationSelect=function(b,c,d,e,f,g,h,j){$("#AjaxLocationDisambigResults").css("display","none");GD.ajax._setLocation(c,e,g,b,d,f,h);b.parents("form:first").attr("disambigComplete","true");j()};GD.ajax._handleAllLocationSelect=function(b,c,d,e,f){$("#AjaxLocationDisambigResults").remove();GD.ajax._setLocation("","","",b,c,d,e);b.parents("form:first").attr("disambigComplete","true");f()};
GD.ajax._setLocation=function(b,c,d,e,f,g,h){e.length&&e.val(b);if(f.length&&g.length){f.val(c);g.val(d)}if(h.length)c&&d?h.val(c+","+d):h.val("")};
GD.ajax._showLocDisambigPanel=function(b,c,d,e,f,g){GD.ajax._closeLocDisambigPanel();b=$(b);b=b.length?b[0]:null;GD.dialogManager.panelCreate("AjaxLocationDisambigResults",null,b,e,c,d,null,null,null,null,true,false,function(){GD.ajax._onLocDisambigPanelClose(g)});GD.dialogManager.panelShow("#AjaxLocationDisambigResults");f&&jQuery.scrollTo($("#AjaxLocationDisambigResults"),{axis:"y",duration:200,offset:{left:0,top:10}})};GD.ajax._closeLocDisambigPanel=function(){try{GD.dialogManager.panelClose("#AjaxLocationDisambigResults")}catch(b){}return false};
GD.ajax._onLocDisambigPanelClose=function(b){b.focus().select()};GD.ajax._restoreSubmitLabel=function(b){b=$(b);var c=b.attr("defaultLabel");c&&b.html(c)};GD.ajax._onACKeyPress=function(b,c,d){if(GD.event.isTextChangeKeystroke(b)){c&&c.length&&c.val("");d&&d.length&&d.val("")}return true};GD.ajax._boldString=function(b){return b.indexOf("<")===-1?"<b>"+b+"</b>":b};
GD.ajax._expandGeoAbbreviations=function(b){b=GD.ajax._abbreviationReplaceHelper(b,"ft","Fort");b=GD.ajax._abbreviationReplaceHelper(b,"mt","Mount");b=GD.ajax._abbreviationReplaceHelper(b,"pt","Point");b=GD.ajax._abbreviationReplaceHelper(b,"st","Saint");b=GD.ajax._abbreviationReplaceHelper(b,"intl","International");b=GD.ajax._abbreviationReplaceHelper(b,"n","North");b=GD.ajax._abbreviationReplaceHelper(b,"s","South");b=GD.ajax._abbreviationReplaceHelper(b,"e","East");b=GD.ajax._abbreviationReplaceHelper(b,
"w","West");b=GD.ajax._abbreviationReplaceHelper(b,"ne","Northeast");b=GD.ajax._abbreviationReplaceHelper(b,"nw","Northwest");b=GD.ajax._abbreviationReplaceHelper(b,"se","Southeast");return b=GD.ajax._abbreviationReplaceHelper(b,"sw","Southwest")};
GD.ajax._expandOccupationAbbreviations=function(b){b=GD.ajax._abbreviationReplaceHelper(b,"sr","Senior");b=GD.ajax._abbreviationReplaceHelper(b,"jr","Junior");b=GD.ajax._abbreviationReplaceHelper(b,"mgr","Manager");return b=GD.ajax._abbreviationReplaceHelper(b,"dir","Director")};GD.ajax._abbreviationReplaceHelper=function(b,c,d){b=GD.ajax._replaceStringSmartCase(b,c+"\\.",d);return b=GD.ajax._replaceStringSmartCase(b,c,d)};
GD.ajax._replaceStringSmartCase=function(b,c,d){if(b.length==c.length)return b;b=b.replace(RegExp("\\b"+c.toLowerCase()+"( |$|\\b)"),d+" ").trim();b=b.replace(RegExp("\\b"+c.toUpperCase()+"( |$|\\b)"),d+" ").trim();b=b.replace(RegExp("\\b"+c.capitalize()+"( |$|\\b)"),d+" ").trim();return b=b.replace(/ ,/,",")};
GD.ajax._cleanSearchString=function(b){b=GD.ajax._replaceStringSmartCase(b,"http","");b=GD.ajax._replaceStringSmartCase(b,"www\\.","");b=GD.ajax._replaceStringSmartCase(b,"intl","");b=GD.ajax._replaceStringSmartCase(b,"int'l","");b=GD.ajax._replaceStringSmartCase(b,"corp","");b=GD.ajax._replaceStringSmartCase(b,"corp.","");b=GD.ajax._replaceStringSmartCase(b,"corporation","");b=GD.ajax._replaceStringSmartCase(b,"inc","");b=GD.ajax._replaceStringSmartCase(b,"inc.","");b=GD.ajax._replaceStringSmartCase(b,
"incorporated","");b=GD.ajax._replaceStringSmartCase(b,"cos","");b=GD.ajax._replaceStringSmartCase(b,"cos.","");b=GD.ajax._replaceStringSmartCase(b,"svcs","");b=GD.ajax._replaceStringSmartCase(b,"mgmt","");b=GD.ajax._expandGeoAbbreviations(b);b=GD.ajax._expandOccupationAbbreviations(b);b=b.replace(/(\\b[A-Za-z][A-Za-z]{0,2})[ ]*&[ ]*([A-Za-z][A-Za-z]{0,2}\\b)/g,"$1$2");b=b.replace(/\&/g," ");b=b.replace(/[\&\'\!\|~\*]/g,"");b=b.replace(/\.[^c]/g,"");return b=b.replace(/[\?\^\-\:\;\(\)\+\/\\\[\]]/g,
" ")};GD.ajax._findAndBoldQueryString=function(b,c){var d=GD.ajax._cleanSearchString(b).trim().split(/\s+/),e,f="(<.+>)",g=false;for(e=0;e<d.length;e+=1){if(d[e].length>0){if(g)g=false;else f+="|";f=f+"(\\b"+d[e]+")"}var h=RegExp(f,"ig");c=c.replace(h,GD.ajax._boldString)}return c};GD.ajax.setDisambiguatedCity=function(b,c,d){b=$(b);b=$("option:selected",b);c=$(c);d=$(d);var e;e=b.length&&b.val()<=0?"":b.text();c.val(e);d.val(b.val())};
GD.ajax.updateWordCounter=function(b,c,d,e,f){if(typeof id(c)!=="undefined"){b=id(b).value.countWords();if(f&&b<=0)id(c).innerHTML="&nbsp;";else if(typeof id(c)!=="undefined")id(c).innerHTML=GD.formatNumber(b)+" words completed"}};GD.ajax.updateCharCounter=function(b,c,d,e,f){if(id(c)){b=id(b).value.length;if(f&&b<=0)id(c).innerHTML="&nbsp;";else id(c).innerHTML=b+" characters entered"}};GD.ajax.keepAliveIntervalId=null;GD.ajax.keepAliveHoursSoFar=0;GD.ajax.keepAliveHoursMax=0;
GD.ajax.keepSessionAlive=function(b){b=parseInt(b);if(isNaN(b))b=24;else if(b<0)b=8765;if(GD.ajax.keepAliveIntervalId)try{window.clearInterval(GD.ajax.keepAliveIntervalId)}catch(c){}GD.ajax.keepAliveHoursSoFar=0;GD.ajax.keepAliveHoursMax=0;if(b>0){GD.ajax.keepAliveHoursMax=b;GD.ajax.keepAliveIntervalId=window.setInterval(GD.ajax._keepSessionAliveWorker,18E5)}};
GD.ajax._keepSessionAliveWorker=function(){GD.ajax.keepAliveHoursSoFar+=1;if(GD.ajax.keepAliveHoursSoFar>=GD.ajax.keepAliveHoursMax)try{window.clearInterval(GD.ajax.keepAliveIntervalId)}catch(b){}else jQuery.ajax({url:"/extendSessionAjax.htm",dataType:"json"})};
GD.ajax.initEditInPlace=function(b,c,d,e,f){if(typeof b!=="string"||b.length===0)throw"idPrefix must be specified as a non-empty string to GD.ajax.initEditInPlace";if(typeof c!=="boolean")c=false;if(typeof d!=="boolean")d=false;if(typeof e!=="function")e=null;if(typeof f!=="function")f=null;var g=$("#"+b+"View"),h=$("#"+b+"Wrapper"),j=$("#"+b+"ViewControls"),k=$("#"+b+"EditControls"),l=$("#"+b+"Edit");g[0].editor=l;if(h.length)g[0].wrapper=h;if(j.length)g[0].viewControls=j;if(e)g[0].onEditEnter=e;
if(f)g[0].onEditCancel=f;if(k.length){g[0].editControls=k;k.attr("prevDisplay",k.css("display"));k.css("display","none")}e=function(){GD.ajax.showEditInPlace(b);return false};if(c)g.dblclick(e);else d&&g.click(e);l.keydown(function(m){if(m.keyCode===GD.event.TAB){m=true;if(g[0].onEditEnter)m=g[0].onEditEnter();m&&GD.ajax.finishEditInPlace(this)}});l.keyup(function(m){if(m.keyCode===GD.event.ESCAPE){m=true;if(g[0].onEditCancel)m=g[0].onEditCancel();m&&GD.ajax.finishEditInPlace(b,true);return false}else return m.keyCode===
GD.event.TAB?false:true})};
GD.ajax.showEditInPlace=function(b){var c=$("#"+b+"View");b=c[0].editor;var d=c[0].wrapper,e=c[0].viewControls,f=c[0].editControls;if(!b)return true;b[0].currentView&&GD.ajax.finishEditInPlace(b);b[0].currentView=c;b.keypress(function(g){if(g.keyCode===13){g=true;if(c[0].onEditEnter)g=c[0].onEditEnter();g&&GD.ajax.finishEditInPlace(this,false);return false}else return true});if(b[0].nodeName==="TEXTAREA"){b.css("width",c.offsetWidth);b.css("height",c.offsetHeight)}b.val(c.html());b.css("visibility",
"visible");b.css("display","inline");d.length&&d.css("visibility","hidden");c.css("visibility","hidden");if(e.length){e.attr("prevDisplay",e.css("display"));e.css("display","none")}if(f.length){f.attr("prevDisplay")!="none"?f.css("display",f.attr("prevDisplay")):f.css("display","block");f.removeAttr("prevDisplay")}b.focus();return false};
GD.ajax.finishEditInPlace=function(b,c){if(typeof c!=="boolean")c=false;if(typeof b==="string")b="#"+b+"Edit";b=$(b);var d=b[0].currentView;if(d&&d.length){var e=d[0].wrapper,f=d[0].viewControls,g=d[0].editControls;c||d.html(b.val());b.css("visibility","hidden");b.css("display","none");e.length&&e.css("visibility","visible");if(g.length){g.attr("prevDisplay",g.css("display"));g.css("display","none")}if(f.length){f.attr("prevDisplay")!="none"?f.css("display",f.prevDisplay):f.css("display","block");
f.removeAttr("prevDisplay")}d.css("visibility","visible")}b.removeAttr("currentView")};GD.ajax.isEditInPlace=function(b){var c=false;b=$("#"+b+"View");var d=b[0].editor;if(d&&d.length)if((d=d[0].currentView)&&d.length)c=b[0]===d[0];return c};if(typeof GD==="undefined")GD={};if(typeof GD.augments==="undefined")GD.augments={};
GD.augments.augmentTargetedLinks=function(b,c,d){c=$(c);if(b){if(c.length==0)c=null;$("a[target="+b+"]",c).each(function(){var e=$(this),f=e.attr("href");e.data("href",f);e.click(function(g){if(!d){var h=window.open(f,"_blank");h.focus&&h.focus()}g.preventDefault()}).removeAttr("href").removeAttr("target")});return false}else Logger.error("GD.augments.augmentTargetedLinks called without 'target' attribute.")};
