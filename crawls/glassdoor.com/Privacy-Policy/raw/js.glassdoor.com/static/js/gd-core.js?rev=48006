GD.graph._createSchoolEditor=function(b,c){var d={},e=$("div.eduHistory");if(b.schoolName){d.name=b.schoolName;d.id=b.schoolId}if(c){d.avatar=c.avatarSqThumbUrl;d.relationshipId=c.relationshipId;d.profileCompletePercentage=c.profileCompletePercentage}d.avatarClass="";if(!d.avatar){d.avatar="/static/img/spacer.gif";d.avatarClass="genericAvatar"}d=GD.graph._createNewItemEditor(e,d);e.removeClass("emptySection");return d};
GD.graph._getEduDataFields=function(b,c){var d=b.is(".recordEditorTemplate"),e;if(e=d?$("input:radio[name=schoolType]:checked",b).val():$("input:hidden[name=schoolType]",b).val())if(e=e.trim())c.type=e;if(c.type!=GD.graph.schoolTypeHS){if(e=d?$("input[name=degree]",b).val():$(".schoolDetails .degree",b).html())if(e=e.trim())c.degree=e;(d?$("input[name=concentration]",b):$(".schoolDetails .concentrationList .concentrationName",b)).each(function(){var f=$(this);if(e=f.is(":input")?f.val():f.html())if(e=
e.trim()){if(!c.concentration)c.concentration=[];c.concentration.push(e)}});c.hasConcentrations=c.concentration&&c.concentration.length>0}GD.graph._getDateRangeFields(b,c)};
GD.graph._setEduDataFields=function(b,c){var d=b.is(".recordEditorTemplate"),e,f;if(d){$("input:radio[name=schoolType]",b).prop("checked",false);f=c.type?c.type:GD.graph.schoolTypeCollege;$('input[value="'+f+'"]',b).prop("checked",true).change()}else{e=$("input:hidden[name=schoolType]",b);f=c.type?c.type:-1;e.val(f)}if(c.type!=GD.graph.schoolTypeHS)if(d){e=$("input[name=degree]",b);c.degree?e.val(c.degree):e.val("");if(c.hasConcentrations){e=$("input:text[name=concentration]",b);f="";for(d=0;d<c.concentration.length;d++)if(d<
e.length)$(e[d]).val(c.concentration[d]);else f+='<input type="hidden" name="concentration" value="'+c.concentration[d]+'" />\n';$(".surplusConcentrations",b).html(f)}}else{e=$(".schoolDetails .degree",b);c.degree?e.html(c.degree):e.html("");if(c.hasConcentrations){f=[];e=Math.min(c.concentration.length,GD.graph.numShownConcentrations);for(d=0;d<e;d++)f.push("<span class='concentrationName'>"+c.concentration[d]+"</span>");d=f.join(", ");$(".concentrationList .shownConcentrations",b).html(d);d="";
if(e<c.concentration.length){f=[];for(d=GD.graph.numShownConcentrations;d<c.concentration.length;d++)f.push("<span class='concentrationName'>"+c.concentration[d]+"</span>");d=f.join("\n")}$(".concentrationList .hiddenConcentrations",b).html(d)}c.degree&&(c.hasDate||c.hasConcentrations)?$("span.dateDegreeSep",b).show():$("span.dateDegreeSep",b).hide();c.degree&&c.hasConcentrations?$("span.degreeConcentrationSep",b).show():$("span.degreeConcentrationSep",b).hide()}GD.graph._setDateRangeFields(b,c)};
GD.graph._validateEduFields=function(b,c){var d=b.is(".recordEditorTemplate");c.valid=true;if(d&&c.hasDate)if(c.startYear>0&&c.endYear>0&&c.endYear<c.startYear){c.valid=false;$("div.fieldErrorReversedDates",b).show("blind",200)}else $("div.fieldErrorReversedDates",b).hide();c.valid&&$("div.fieldError",b).hide()};
GD.graph._handleSchoolTypeRadio=function(b){var c=$("input:radio[name=schoolType]:checked",b),d=$("div.higherEdFields",b);if(c.val()==GD.graph.schoolTypeHS)d.css("display")!="none"&&d.hide("blind",200);else if(d.css("display")=="none")b.is(":visible")?d.show("blind",200):d.show()};
GD.graph._getDateRangeFields=function(b,c){b.is(".recordEditorTemplate");c.hasDate=false;c.startMonth=$("[name=startMonth]",b).val();c.startYear=$("[name=startYear]",b).val();c.endMonth=$("[name=endMonth]",b).val();c.endYear=$("[name=endYear]",b).val();if(c.startYear>0||c.startMonth>0||c.endYear>0||c.endMonth>0)c.hasDate=true;c.isCurrent=false;if(c.endYear<=0&&c.endMonth<=0)c.isCurrent=true};
GD.graph._makeDateFieldsNumeric=function(b){if(jQuery.isNumeric(b.startMonth))b.startMonth-=0;else b.startMonth=0;if(jQuery.isNumeric(b.startYear))b.startYear-=0;else b.startYear=0;if(jQuery.isNumeric(b.endMonth))b.endMonth-=0;else b.endMonth=0;if(jQuery.isNumeric(b.endYear))b.endYear-=0;else b.endYear=0};
GD.graph._setDateRangeFields=function(b,c){var d=b.is(".recordEditorTemplate"),e=b;e.is(".historyRecord")||(e=e.closest(".historyRecord"));c.isCurrent=true;$("[name=startMonth]",b).val(c.startMonth);$("[name=startYear]",b).val(c.startYear);$("[name=endMonth]",b).val(c.endMonth);$("[name=endYear]",b).val(c.endYear);c.isCurrent=!(c.endYear>0);if(d){if(e.is(".workItem")){d=$("input[name=currentJob]",b);d.prop("checked",c.isCurrent);GD.graph._handleCurrentJobCheck(b,d)}}else if(e.is(".eduItem")){d="";
if(c.startYear>0&&c.endYear>0)d=c.startYear==c.endYear?c.endYear:c.startYear+" &ndash; "+c.endYear;else if(c.startYear>0)d=c.startYear+" &ndash;";else if(c.endYear>0)d=c.endYear;e=$(".schoolDetails .schoolYears",b);var f=$(".dateConcentrationSep",b);e.html(d);d?f.show():f.hide()}else if(e.is(".workItem"))if(c.startYear>0){if(c.startMonth>0){d=new Date(c.startYear,c.startMonth-1);d=c.startYear==c.endYear&&c.startMonth>=0&&c.endMonth>=0&&c.startMonth!=c.endMonth?d.f("NNN"):d.f("NNN yyyy")}else d=new String(c.startYear);
d=d.toString();e=c.isCurrent?"Present":c.endMonth>0?(new Date(c.endYear,c.endMonth-1)).f("NNN yyyy"):new String(c.endYear);e=e.toString();d=d==e?d:d+" &ndash; "+e;$(".position .dateDesc",b).html(d);(d=$(".position .jobTitle",b).html())&&d.trim()?$("span.jobTitleSeparator",b).show():$("span.jobTitleSeparator",b).hide()}else{$("span.dateDesc",b).html("");$("span.dateLocSeparator",b).hide()}};
GD.graph._handleCurrentJobCheck=function(b,c){var d=c.prop("checked"),e=$("span.jobNotCurrentFields",b),f=$("span.jobIsCurrentFields",b);if(d){$("select[name=endMonth]",e).val(-1);$("select[name=endYear]",e).val(-1);e.hide();f.show()}else{f.hide();e.show()}};GD.graph._isNameEntered=function(b){var c=b.val();b=b.attr(GD.augments.ATTR_PLACEHOLDER);if(c)c=c.trim();if(c==b)c="";return!!c};
GD.graph._onRecordAdded=function(b,c){var d=b;if(d)d.is(".historyRecord")||(d=d.closest(".historyRecord"));if(c){c.relationshipId&&d.data("relationship-id",c.relationshipId);if(c.name){var e=$(".desc .name",d);e.html()!=c.name&&e.html(c.name)}if(c.avatarSqThumbUrl){d=$("img.avatar",d);if(d.attr("src")!=c.avatarSqThumbUrl){d.attr("src",c.avatarSqThumbUrl);d.removeClass("genericAvatar")}}}};
