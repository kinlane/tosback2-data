GD.flip.scrollLeft=function(b,c,d){try{var e=GD.flip.getSliderPane(b),f=e.offsetWidth,g=id(b),h=g.offsetWidth-2;if(!c){if(g.vivifyInactiveTimeoutId){clearTimeout(g.vivifyInactiveTimeoutId);g.vivifyInactiveTimeoutId=null;g.isAutoScrolling=false}if(g.vivifyActiveTimerId){clearTimeout(g.vivifyActiveTimerId);g.vivifyActiveTimerId=null;g.isAutoScrolling=false}g.vivified&&GD.flip.vivify(b,true)}if(g.slideAnim&&g.slideAnim.isAnimated()&&typeof g.slideDest==="number"){g.slideAnim.stop();e.style.left=g.slideDest+
"px"}if(e.style.left){var j=Number(String(e.style.left).replace(/px/,"")),k=h*2-f;j-=h+2;if(j<k)j=k}else j=-(h+2);g.slideDest=j;g=null;if(c&&d)g=function(){GD.flip._setNextVivifiedScrollTimeout(b)};$(e).animate({left:j},GD.flip.speed,"easeInOutExpo",g);GD.flip.adjustButtons(b,e,f,h,-j)}catch(l){Logger.fatal("GD.flip.scrollLeft",l)}};
GD.flip.scrollRight=function(b,c,d){try{var e=GD.flip.getSliderPane(b),f=e.offsetWidth,g=id(b),h=g.offsetWidth-2;if(!c){if(g.vivifyInactiveTimeoutId){clearTimeout(g.vivifyInactiveTimeoutId);g.vivifyInactiveTimeoutId=null;g.isAutoScrolling=false}if(g.vivifyActiveTimerId){clearTimeout(g.vivifyActiveTimerId);g.vivifyActiveTimerId=null;g.isAutoScrolling=false}g.vivified&&GD.flip.vivify(b,true)}if(g.slideAnim&&g.slideAnim.isAnimated()&&typeof g.slideDest==="number"){g.slideAnim.stop();e.style.left=g.slideDest+
"px"}if(e.style.left){var j=Number(String(e.style.left).replace(/px/,""));j+=h+2;if(j>0)j=0}else j=0;g.slideDest=j;g=null;if(c&&d)g=function(){GD.flip._setNextVivifiedScrollTimeout(b)};$(e).animate({left:j},GD.flip.speed,"easeInOutExpo",g);GD.flip.adjustButtons(b,e,f,h,-j)}catch(k){Logger.fatal("GD.flip.scrollRight",k)}};GD.flip.getSliderPane=function(b){try{return id(b+"Slider")}catch(c){Logger.fatal("GD.flip.getSliderPane",c)}};
GD.flip.adjustButtons=function(b,c,d,e,f){try{var g=GD.flip.isAtStart(b,c,f),h=GD.flip.isAtEnd(b,c,d,e,f),j=$("#"+b+"PrevBtn").parent(),k=$("#"+b+"NextBtn").parent();g?j.addClass("disabled"):j.removeClass("disabled");h?k.addClass("disabled"):k.removeClass("disabled")}catch(l){Logger.fatal("GD.flip.adjustButtons",l)}};
GD.flip.isAtStart=function(b,c,d){try{if(!c){c=GD.flip.getSliderPane(b);d=c.style.left?-Number(String(c.style.left).replace(/px/,"")):0}if(d<0)d=0;return d<=0}catch(e){Logger.fatal("GD.flip.isAtStart",e)}};GD.flip.isAtEnd=function(b,c,d,e,f){try{if(!c){var g=id(b);c=GD.flip.getSliderPane(b);d=c.offsetWidth;e=g.offsetWidth-2;f=c.style.left?-Number(String(c.style.left).replace(/px/,"")):0}b=d-e*2;if(f<0)f=0;if(f+10>b)f=b;return f>=b}catch(h){Logger.fatal("GD.flip.isAtEnd",h)}};
GD.flip.adjustHeight=function(b){try{var c=id(b),d=id(b+"Slider").offsetHeight;c.style.height=d+"px";c.style.minHeight=d+"px"}catch(e){Logger.fatal("GD.flip.adjustHeight",e)}};
GD.flip.vivify=function(b,c){try{var d=id(b);if(typeof c!=="boolean")c=true;d.vivified=c;d.isAutoScrolling=false;if(c){var e=GD.flip.isAtStart(b),f=GD.flip.isAtEnd(b);if(!(e&&f))d.vivifyInactiveTimeoutId=setTimeout(function(){GD.flip._activateVivification(b)},GD.flip.vivifyInactiveSeconds*1E3)}else{if(d.vivifyInactiveTimeoutId){clearTimeout(d.vivifyInactiveTimeoutId);d.vivifyInactiveTimeoutId=null}if(d.vivifyActiveTimerId){clearTimeout(d.vivifyActiveTimerId);d.vivifyActiveTimerId=null}}}catch(g){Logger.fatal("GD.flip.vivify",
g)}};GD.flip._activateVivification=function(b){try{var c=id(b);c.isAutoScrolling=true;GD.flip._vivifiedScroll(b,false);c.vivifyActiveTimerId=setTimeout(function(){GD.flip._vivifiedScroll(b,true)},GD.flip.vivifyFlipPauseSeconds*1E3)}catch(d){Logger.fatal("GD.flip._activateVivification",d)}};
GD.flip._vivifiedScroll=function(b,c){try{var d=id(b);if(d.isAutoScrolling)if(GD.flip.isAtEnd(b)){var e=GD.flip.getSliderPane(b),f=e.offsetWidth,g=d.offsetWidth-2;d.slideDest=0;d=null;if(c)d=function(){GD.flip._setNextVivifiedScrollTimeout(b)};$(e).animate({left:0},GD.flip.resetSpeed,"easeInOutExpo",d);GD.flip.adjustButtons(b,e,f,g,0)}else GD.flip.scrollLeft(b,true,c)}catch(h){Logger.fatal("GD.flip._vivifiedScroll",h)}};
GD.flip._setNextVivifiedScrollTimeout=function(b){id(b).vivifyActiveTimerId=setTimeout(function(){GD.flip._vivifiedScroll(b,true)},GD.flip.vivifyFlipPauseSeconds*1E3)};if(typeof GD==="undefined")GD={};if(typeof GD.empInfo==="undefined")GD.empInfo={};GD.empInfo.awardFadeSpeed=100;GD.empInfo.awardRevealSpeed=500;GD.empInfo.awardHideSpeed=500;GD.empInfo._deepLinkTimeout=null;
