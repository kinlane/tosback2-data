d;b.eventType=f;b.gid=c.data("gid");var g;if(b.error_code&&b.error_msg)g="fb wallpost failed: c="+b.error_code+",m="+b.error_msg+" [fr="+b.gid+"]";else if(b.error)g="fb wallpost failed: c="+b.error.code+",m="+b.error.message+" [fr="+b.gid+"]";g?e.handleFbError(g):e.handleFbSuccess(b)}else e.handleEmptyResponse()},handleFbError:function(b){var c=this;Logger.error("GD.fb.FeedDialog: "+b);jQuery.ajax({url:GD.ajax.getAjaxUrlRespectingSecurity("/util/ajax/lgr.htm"),type:"POST",dataType:"json",data:{m:b,
l:"e"}})},handleFbSuccess:function(b){var c=this;this.feedTracker.trackWallPostFeed(b.gid,b.eventType)}});GD.fb.initFbFeedDialogLink=function(){var b=new GD.fb.TrackingHandler("/track/ajax/invitationTracking.htm"),c=new GD.fb.FeedDialog(b);c.bind($("a.fbFeedDialogLink"),function(){c.promptToFeed($(this));return false});return c};
