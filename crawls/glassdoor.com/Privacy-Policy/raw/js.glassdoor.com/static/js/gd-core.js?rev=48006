GD.photos.preloadPhoto=function(){for(var b=0;b<GD.photos.pagePhotoIds.length;b++){var c=$("#Photo_"+GD.photos.pagePhotoIds[b]+" img.companyPhoto"),d=c.attr("src"),e=c.attr("photoUrl");if(!c.attr("photoComplete")&&e&&d!=e){c.one("load",function(){setTimeout(GD.photos.preloadPhoto,10);c.attr("photoComplete","true")});c.one("error",function(){GD.photos.brokenImage(c);c.attr("photoComplete","true");setTimeout(GD.photos.preloadPhoto,10)});Logger.info("Preloading photo id="+GD.photos.pagePhotoIds[b]);
c.attr("src",e);break}}};GD.photos.onThumbError=function(b){b=$(b);b.attr("src","/static/img/icons/broken/broken-50.png");b.attr("width","50");b.attr("height","50")};GD.photos.brokenImage=function(b){b.attr("src","/static/img/icons/broken/broken-200.png");b.attr("width","200");b.attr("height","200")};
