String.prototype.toTitleCase=function(){return this.replace(/([\w&`'\u2018\u2019"\u201c.@:\/\{\(\[<>_]+-? *)/g,function(b,c,d,e){if(d>0&&e.charAt(d-2)!==":"&&b.search(/^(a(nd?|s|t)?|b(ut|y)|en|for|i[fn]|o[fnr]|t(he|o)|vs?\.?|via)[ \-]/i)>-1)return b.toLowerCase();if(e.substring(d-1,d+1).search(/['"_{(\[]/)>-1)return b.charAt(0)+b.charAt(1).toUpperCase()+b.substr(2);if(b.substr(1).search(/[A-Z]+|&|[\w]+[._][\w]+/)>-1||e.substring(d-1,d+1).search(/[\])}]/)>-1)return b;return b.charAt(0).toUpperCase()+
b.substr(1)})};String.prototype.removeToken=function(b){if(!b)return this;var c=this.indexOf(b);if(c==-1)return this;return this.substring(0,c)+this.substring(c+b.length).removeToken(b)};String.prototype.encodeHTML=function(){var b=this.toString();b=b.replace(/&/g,"&amp;");b=b.replace(/</g,"&lt;");b=b.replace(/>/g,"&gt;");b=b.replace(/"/g,"&quot;");return b=b.replace(/'/g,"&apos;")};
String.prototype.decodeHTML=function(){var b=this.toString();b=b.replace(/&gt;/g,">");b=b.replace(/&lt;/g,"<");b=b.replace(/&amp;/g,"&");b=b.replace(/&quot;/g,'"');return b=b.replace(/&apos;/g,"'")};String.prototype.truncateToLength=function(b,c,d){var e=this.toString();if(e.length>b){if(c){for(b=b;b>0;){if(/\s/.test(e.charAt(b-1)))break;b--}for(;b>0&&!/\s/.test(e.charAt(b-1));)b--;e=e.substr(0,b).trim()}else e=e.substr(0,b).trim();d||(e+="&hellip;")}return e};
String.prototype.entityToPlainText={lt:"<",gt:">",amp:"&","#32":" ",nbsp:" ","#39":"'",apos:"'",quot:'"',cent:"\u00a2",pound:"\u00a3",yen:"\u00a5",copy:"\u00a9",reg:"\u00ae",ndash:"\u2013",mdash:"\u2014",lsquo:"\u2018",rsquo:"\u2019",ldquo:"\u201c",rdquo:"\u201d",bull:"\u2022",hellip:"\u2026",euro:"\u20ac",trade:"\u2122",infin:"\u221e"};
