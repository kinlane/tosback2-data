GD.augments.augmentButtons=function(){try{$("button.gd-button").each(function(){var c=$(this),d=c.attr("name");d&&c.attr("gdName",d).removeAttr("name")});$("button.gd-button:not(.ui-state-disabled)").hover(function(){$(this).addClass("ui-state-hover")},function(){$(this).removeClass("ui-state-hover")}).mousedown(function(){var c=$(this);c.parents(".gd-buttonset-single:first").find(".gd-button.ui-state-active").removeClass("ui-state-active");c.is(".ui-state-active.gd-button-toggleable, .gd-buttonset-multi .ui-state-active")?
c.removeClass("ui-state-active"):c.addClass("ui-state-active")}).mouseup(function(){$(this).is(".gd-button-toggleable, .gd-buttonset-single .gd-button,  .gd-buttonset-multi .gd-button")||$(this).removeClass("ui-state-active")}).keydown(function(c){if(c.keyCode==13||c.keyCode==32){c=$(this);c.parents(".gd-buttonset-single:first").find(".gd-button.ui-state-active").removeClass("ui-state-active");c.is(".ui-state-active.gd-button-toggleable, .gd-buttonset-multi .ui-state-active")?c.removeClass("ui-state-active"):
c.addClass("ui-state-active")}return true})}catch(b){Logger.fatal("GD.augments.augmentButtons",b)}};GD.augments.addHiddenButtonFieldsToForm=function(b){b=b?$(b):$("form");b.each(function(){var c=$(this);c.find("button.ui-state-active").each(function(){var d=$(this),e=d.attr("gdName");d=d.attr("value");var f=c.find("input[type='hidden'][name='"+e+"']");if(e&&d){if(d==="NA")d=0;if(f.length)f.val(d);else{f=$("<input type='hidden'>");f.attr("name",e).attr("value",d);b.append(f)}}else f&&f.remove()})})};
