GD.fb.FeedDialog=GD.Class({constructor:function(b){this.feedTracker=b},bind:function(b,c){b.bind("click keypress",c)},promptToFeed:function(b,c){GD.effects.occludePage();GD.fb.fbRootReset();var d=this;FB.ui({method:"feed",link:b.data("fbFeedLink"),picture:b.data("fbFeedPicture"),name:b.data("fbFeedName"),caption:b.data("fbFeedCaption"),description:b.data("fbFeedDescription")},function(e){GD.effects.revealPage();d.handleFbResponse(e,b,"_PREVIEW");typeof c=="function"&&c(e)})},postToFeed:function(b,
