GD.photos.loadPhoto=function(b){var c=$("#Photo_"+b+" img.companyPhoto"),d=c.attr("src"),e=c.attr("photoUrl");if(!c.attr("photoComplete")&&e&&d!=e){$("#PhotosListSpinner").show();c.load(function(){$("#PhotosListSpinner").hide();c.attr("photoComplete","true")}).error(function(){$("#PhotosListSpinner").hide();c.attr("photoComplete","true");GD.photos.brokenImage(c)});c.attr("src",e)}GD.photos.trackPhoto(b)};
