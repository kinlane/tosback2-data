return true}};if(h.length)h[0].updateCityField=function(n){var u=$("option:selected",h).val(),q=true;if(!u||u<=0)q=false;if(n)if(q){n.autocomplete("enable");l.show();n.show()}else{l.hide();n.hide();n.autocomplete("disable")}};h.change(function(){GD.ajax.createLocationAutoComplete.onCountryListChange(this.updateCityField,h,j,k,g);return true});h.keydown(function(n){GD.ajax.createLocationAutoComplete.onCountryListKeyDown(n,this.updateCityField,h,j,k,g);return true});j.keypress(function(n){return GD.ajax._onACKeyPress(n,
k)});j.blur(function(){j.hasClass("ui-autocomplete-opened")||j.autocomplete("disable").autocomplete("close").autocomplete("enable")});j.autocomplete(b).data("autocomplete")._renderItem=function(n,u){return GD.ajax._formatACItem(j,n,u)}}};GD.ajax.createLocationAutoComplete.onCountryListKeyDown=function(b,c,d,e,f,g){b.keyCode!=GD.event.TAB&&setTimeout(function(){GD.ajax.createLocationAutoComplete.onCountryListChange(c,d,e,f,g)},1);return true};
