GD.augments._addTagHelpNode=function(b,c,d){try{var e=$("<div>");e.attr("id",b+GD.augments.tagsHelpAreaPostfix);e.addClass(GD.augments.classNameTagEditorHelpArea);e.addClass("minorText");if(d){if(typeof d==="string"){d=d.trim();d=d.split(",")}}else d=[];var f="";f+="Separate each tag with a comma.";if(d.length>0){var g=d.length>GD.augments.maxInitialSuggestions+1,h=Math.min(d.length,GD.augments.maxSuggestions);f+="<br/>\n";f+="<span class='suggestedTags'>";f+="<strong>Suggested Tags:</strong> ";for(var j=
0;j<h;j++){var k=d[j];if(g&&j==GD.augments.maxInitialSuggestions){var l;l="onclick=\"GD.augments._seeMore('"+b+"')\"";f+="<span id='"+(b+GD.augments.tagsSeeMoreLinkPostfix)+"'>&nbsp;";f+="&nbsp; <a href='javascript:noop();' "+l+" class='link'><strong class='keepTogether'>See more</strong></a></span>";f+="<span class='hidden' id='"+(b+GD.augments.tagsSeeMoreTagsPostfix)+"'>"}if(j>0)f+=", ";var m=k.replace("'","&#39;"),n='GD.augments.addTags("'+b+'", "'+m+'");';f+="<span class='keepTogether'>";f+="<a href='javascript:noop();' onclick='"+
