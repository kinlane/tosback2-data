GD.rollover.preloader=function(b){for(i=0;i<b.length;i++)if(b[i].getAttribute("srchover")||b[i].getAttribute("srcclick")){GD.rollover._storeImgs(b[i]);var c="";c=b[i].getAttribute("srchover")?c+"A":c+"";c=b[i].getAttribute("srcclick")?c+"B":c+"";switch(c){case "A":GD.rollover.mouseover(b[i]);GD.rollover.mouseout(b[i]);break;case "B":GD.rollover.mousedown(b[i]);GD.rollover.mouseup2(b[i]);break;case "AB":GD.rollover.mouseover(b[i]);GD.rollover.mouseout(b[i]);GD.rollover.mousedown(b[i]);GD.rollover.mouseup(b[i]);
break;default:return}b[i].src&&b[i].setAttribute("oldsrc",b[i].src)}};GD.rollover.mouseup=function(b){var c;if(b.onmouseup){b.oldmouseup=b.onmouseup;c=function(){this.src=this.getAttribute("srchover");this.oldmouseup()}}else c=function(){this.src=this.getAttribute("srchover")};b.onmouseup=c};
GD.rollover.mouseup2=function(b){var c;if(b.onmouseup){b.oldmouseup=b.onmouseup;c=function(){this.src=this.getAttribute("oldsrc");this.oldmouseup()}}else c=function(){this.src=this.getAttribute("oldsrc")};b.onmouseup=c};GD.rollover.mousedown=function(b){if(b.onmousedown)b.oldmousedown=b.onmousedown;b.onmousedown=function(){this.src=this.getAttribute("srcclick")}};
GD.rollover.mouseover=function(b){var c;if(b.onmouseover){b.oldmouseover=b.onmouseover;c=function(){this.src=this.getAttribute("srchover");this.oldmouseover()}}else c=function(){this.src=this.getAttribute("srchover")};b.onmouseover=c};GD.rollover.mouseout=function(b){var c;if(b.onmouseout){b.oldmouseout=b.onmouseout;c=function(){this.src=this.getAttribute("oldsrc");this.oldmouseout()}}else c=function(){this.src=this.getAttribute("oldsrc")};b.onmouseout=c};
