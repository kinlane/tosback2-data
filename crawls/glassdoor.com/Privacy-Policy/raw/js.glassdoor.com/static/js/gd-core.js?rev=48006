invite:function(b){this.hasPermission!==true?this.promptToSendWallPost(b):this.sendWallPost(b)},promptToSendWallPost:function(b){Logger.debug("sending publish_stream permissions.request for wall post invite");var c=this,d={method:"permissions.request",perms:"publish_stream",display:"popup"};try{FB.ui(d,function(f){if(f.perms===d.perms){c.hasPermission=true;c.sendWallPost(b)}})}catch(e){Logger.debug(e.message)}},sendWallPost:function(b){var c=this,d=b.attr("data-to-sid"),e=c.fbAuthToken,f=c.fbInviteParams;
Logger.debug("sending invite to Facebook sid: "+d);var g={url:"/"+d+"/feed?access_token="+e,method:"post",params:f,$btn:b,callback:function(j){j||(j={error:"empty response"});j.data=g;c.handleResponse(j);delete c;delete g}};try{FB.api(g.url,g.method,g.params,g.callback)}catch(h){Logger.error(h.message)}}});
