GD.fb.onFacebookLink=function(b){var c=false;if(b&&b.status==="connected")c=true;if(c){GD.analytics.trackPartnerAuth("facebook",true);jQuery.ajax({url:"/profile/socialUserLinkAjax.htm",data:{token:b.authResponse.accessToken,socialUid:b.authResponse.userID,socialNetwork:"FACEBOOK"},dataType:"json",success:function(d){if(d.redirectURL)GD.dom.clearWindowAndRedirect(d.redirectURL);else if(d.successful){$("div.fbSettings .unlinked").hide();$("div.fbSettings .linked").show()}else{if(!d.errorMessage)d.errorMessage=
"Something went wrong while authorizing you with Facebook.  Could you try again?";alert(d.errorMessage)}},error:function(){alert("Something went wrong while authorizing you with Facebook.  Could you try again?")}})}};GD.fb.facebookDelink=function(){GD.analytics.trackPartnerAuth("facebook",false);FB.api({method:"Auth.revokeAuthorization"},function(){GD.fb.onFacebookDelink()})};
GD.fb.onFacebookDelink=function(){jQuery.ajax({url:"/profile/delinkSocialUserAjax.htm",data:{socialNetwork:"FACEBOOK"},dataType:"json",success:function(b){if(b.loggedIn&&!b.redirectURL){$("div.fbSettings .linked").hide();$("div.fbSettings .unlinked").show()}else{if(!b.redirectURL)b.redirectUrl="/index.htm";GD.dom.clearWindowAndRedirect(b.redirectURL)}},error:function(){GD.dom.clearWindowAndRedirect("/index.htm")}})};
GD.fb.checkPublishStreamPermission=function(b){var c=false,d=10,e=setInterval(function(){if(d>0){d-=1;try{FB.api("/me/permissions",function(g){Logger.inspect(g,"inviteHandler permissions check");if(!g||!g.data)Logger.inspect({error:"Error retrieving permissions from Facebook",data:g.data||g});else{c=g.data[0].publish_stream===1;clearInterval(e);b(c)}})}catch(f){Logger.inspect(f,"Bad permissions fetch")}}else{clearInterval(e);b(c)}},500)};
GD.fb.fbRootReset=function(){var b=$("#fb-root").get(0);b.style.position="absolute";b.scrollIntoView(false);b.style.position="fixed"};GD.fb.initFBWallPostInviteHandler=function(b,c){var d=new GD.fb.TrackingHandler("/track/ajax/invitationTracking.htm","FB_WALL_INVITE",function(e){GD.graph._updateProfileOnInvite(e)});return new GD.fb.FBWallPostInviteHandler({fbAuthToken:b,fbInviteParams:["link","picture","name","description","caption"],selector:"span.fbInviteBtn",$context:c,inviteTracker:d})};
GD.fb.TrackingHandler=GD.Class({SOCIAL_INVITATION_CATEGORY:"social-invitation",constructor:function(b,c,d,e){this.trackingUrl=b;this.eventType=c;this.success=typeof d=="function"?d:this.success;this.error=typeof e=="function"?e:this.error},success:function(b){Logger.debug("GD.fb.TrackingHandler: tracking success: "+b)},error:function(b){Logger.debug("GD.fb.TrackingHandler: tracking error: "+b)},trackAppRequest:function(b){var c=this,d=b.request;b=b.toSids;if(typeof d=="string"||typeof d=="number"){if(typeof b==
"string")b=b.split(",");if({}.toString.call(b)!="[object Array]")Logger.error("GD.fb.TrackingHandler: Expected 'toGids' field in FB response to be an array.");else{d="invitationType="+c.eventType;for(var e=0;e<b.length;++e)d+="&sourceId="+b[e];GD.analytics.trackEvent(c.SOCIAL_INVITATION_CATEGORY,c.eventType,null,b.length);jQuery.ajax({url:c.trackingUrl,type:"POST",data:d,success:function(f){Logger.debug("GD.fb.TrackingHandler: trackAppRequest success: "+f)},error:function(f,g,h){Logger.debug("GD.fb.TrackingHandler: trackAppRequest error: "+
h)}})}}else Logger.error("GD.fb.TrackingHandler: Expected 'data.request' field in FB response to be a string or number.")},trackWallPostInvite:function(b){var c=this;GD.analytics.trackEvent(c.SOCIAL_INVITATION_CATEGORY,c.eventType);jQuery.ajax({url:c.trackingUrl,type:"POST",dataType:"json",data:{invitationType:c.eventType,sourceId:b},success:function(d){d.sourceId=b;d.success?c.success(d):c.error(d);delete c},error:function(d,e,f){c.error({sourceId:b,textStatus:e,errorThrown:f});delete c}})},trackWallPostFeed:function(b,
c){var d=this;GD.analytics.trackEvent(d.SOCIAL_INVITATION_CATEGORY,c);jQuery.ajax({url:d.trackingUrl,type:"POST",dataType:"json",data:{invitationType:c,sourceId:b},success:function(e){Logger.debug("GD.fb.TrackingHandler: trackWallPostFeed success: "+e)},error:function(e,f,g){Logger.debug("GD.fb.TrackingHandler: trackWallPostFeed error: "+g)}})}});
