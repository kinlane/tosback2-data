GD.augments.augmentPlaceholders=function(b){try{b=typeof b=="undefined"?$("input[placeholder]").not("."+GD.augments.PLACEHOLDER_OK):$(b).not("."+GD.augments.PLACEHOLDER_OK);b.each(function(){var d=$(this),e=d.attr(GD.augments.ATTR_PLACEHOLDER);typeof e==="string"&&e.length>0&&GD.augments.augmentPlaceholderField(d,e)})}catch(c){Logger.fatal("GD.augments.augmentPlaceholders",c)}};
GD.augments.getPlaceholderVal=function(b){b=$(b);var c;if(b.length)if(!Modernizr.input.placeholder&&b.hasClass(GD.augments.PLACEHOLDER_OK)){c=b.attr(GD.augments.ATTR_PLACEHOLDER);b=b.val();c=c&&b==c?"":b}else c=b.val();return c};
GD.augments.setPlaceholderVal=function(b,c){var d=$(b);if(d.length)if(!Modernizr.input.placeholder&&d.hasClass(GD.augments.PLACEHOLDER_OK)){var e=d.attr(GD.augments.ATTR_PLACEHOLDER);if(c){d.val(c);d.removeClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE);d.addClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)}else{d.val(e);d.removeClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE);d.addClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)}}else d.val(c);return c};
GD.augments.changePlaceholderText=function(b,c){if(c){var d=$(b);if(d.length&&d.hasClass(GD.augments.PLACEHOLDER_OK)){d.attr(GD.augments.ATTR_PLACEHOLDER,c);Modernizr.input.placeholder||d.hasClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)&&d.val(c)}}};GD.augments.uninstallPlaceholders=function(b){try{if(typeof b=="undefined")b=$("input[placeholder]");b.each(function(){var d=$(this);GD.augments.uninstallPlaceholderField(d)})}catch(c){Logger.fatal("GD.augments.uninstallPlaceholders",c)}};
GD.augments.augmentPlaceholderField=function(b,c){try{b=$(b);if(b.length)if(!b.hasClass(GD.augments.PLACEHOLDER_OK)){if(jQuery.type(c)==="string")b.attr(GD.augments.ATTR_PLACEHOLDER,c);else c=b.attr(GD.augments.ATTR_PLACEHOLDER);if(b.val()==c){b.removeClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE);b.addClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)}else{b.removeClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE);b.addClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)}b.addClass(GD.augments.PLACEHOLDER_OK);b.focus(GD.augments.onPlaceholderFocus);
b.blur(GD.augments.onPlaceholderBlur);b.blur()}if(c){var d=b.parents("form:first");GD.augments.augmentPlaceholderForm(d)}}catch(e){Logger.fatal("GD.augments.augmentPlaceholderField",e)}};GD.augments.augmentPlaceholderForm=function(b){b&&b.each(function(){var c=$(this);if(!c.attr("isPlaceholderEnabled")){c.bind("submit",function(d){d=$("input[placeholder]",d.target);GD.augments.uninstallPlaceholders(d)});c.attr("isPlaceholderEnabled","true")}})};
GD.augments.uninstallPlaceholderField=function(b){try{b=$(b);if(b.hasClass(GD.augments.PLACEHOLDER_OK)){var c=b.attr(GD.augments.ATTR_PLACEHOLDER);if(c){b.unbind("focus",GD.augments.onPlaceholderFocus);b.unbind("blur",GD.augments.onPlaceholderBlur);if(!Modernizr.input.placeholder)if(b.hasClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)){b.val()===c&&b.val("");b.removeClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)}else b.hasClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)&&b.removeClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)}b.removeClass(GD.augments.PLACEHOLDER_OK)}}catch(d){Logger.fatal("GD.augments.uninstallPlaceholderField",
d)}};GD.augments.onPlaceholderFocus=function(b){b=b instanceof jQuery?b:typeof b==="string"?$(b):$(b.target);if(b.hasClass(GD.augments.PLACEHOLDER_OK)){if(!Modernizr.input.placeholder){var c=b.attr(GD.augments.ATTR_PLACEHOLDER);b.val()===c&&b.val("")}b.removeClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE);b.addClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)}return true};
GD.augments.onPlaceholderBlur=function(b){$field=b instanceof jQuery?b:typeof b==="string"?$(b):$(b.target);if($field.hasClass(GD.augments.PLACEHOLDER_OK)){b=$field.attr(GD.augments.ATTR_PLACEHOLDER);var c=$field.val();if(c===""||c===b){Modernizr.input.placeholder||$field.val(b);$field.removeClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE);$field.addClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE)}else if(c!==""&&c!==b){$field.removeClass(GD.augments.CLASS_PLACEHOLDER_ACTIVE);$field.addClass(GD.augments.CLASS_PLACEHOLDER_INACTIVE)}}return true};
