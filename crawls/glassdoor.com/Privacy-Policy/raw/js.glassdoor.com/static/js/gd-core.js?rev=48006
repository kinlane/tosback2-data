GD.dom.getDocUrl());return false}else{g.attr("contentPanelId").trim();var h=$(".tabbed",e);if(h.is(":visible"))GD.jobScope._selectTab(e,g,false);else{GD.analytics.trackEvent("jobPanel","open",f.employerShortName);GD.jobScope.open(e,h,g,false)}}return true})});$(".jobScopeLink").click(function(e){var f=$(e.currentTarget);e=f.attr("jobScopeId");f=f.attr("jobScopeTab");if(e){e=$("#"+e);GD.jobScope.showTab(e,f)}})};GD.jobScope._logTabClick=function(b){(b=b.attr("loggingUrl"))&&jQuery.ajax({url:b,dataType:"json"})};
GD.jobScope._disableAutoOpen=function(){if($("body").data("jobScopeAutoOpenEnabled")){jQuery.ajax({url:"/jobScope/panels/jobScopeDisableAutoOpen.htm",dataType:"json"});$("body").data("jobScopeAutoOpenEnabled",false)}};
GD.jobScope.toggle=function(b,c,d){c||(c=GD.jobScope._getJobInfo(b));var e=$(".tabbed",b);if(!e.is(":animated"))if(e.is(":visible")){GD.analytics.trackEvent("jobPanel","close",c.employerShortName);GD.jobScope._close(b,e,true)}else{d?GD.analytics.trackEvent("jobPanel","auto-open",c.employerShortName):GD.analytics.trackEvent("jobPanel","open",c.employerShortName);GD.jobScope.open(b,e,null,d)}d||GD.jobScope._disableAutoOpen();return false};
GD.jobScope.showTab=function(b,c){var d=$(".tabbed",b),e=d.is(":visible"),f=$(".jobScopeTabs li[tabType='"+c+"']",b);if(e)GD.jobScope._selectTab(b,f);else{GD.jobScope.open(b,d,f,false);GD.jobScope._disableAutoOpen()}};
GD.jobScope.open=function(b,c,d,e){var f=b.attr("panelDefaultBgColor");if(!f||f==="transparent")f="white";b.is(".initialized")||GD.jobScope._initPanel(b,c);if(d==null){var g=b.attr("selectedTab");if(g)d=$(".jobScopeTabs li[tabType='"+g+"']",b);d||(d=$(".jobScopeTabs li:first",b))}d&&GD.jobScope._selectTab(b,d,e);b.toggleClass("jobScopePanelOpened");b.css({backgroundColor:f,borderColor:"white"});b.animate({backgroundColor:"#e8f3e8",borderTopColor:"#cae8ca",borderRightColor:"#cae8ca",borderBottomColor:"#cae8ca",
borderLeftColor:"#cae8ca"},GD.jobScope.ANIMATE_SPEED);c.slideDown(GD.jobScope.ANIMATE_SPEED);c=$(".panelHeader .jobScopeTabs .hiddenWhenOpened",b);c.length>0?c.fadeOut(GD.jobScope.ANIMATE_SPEED/2,function(){GD.jobScope._showOpenedTabs(b)}):GD.jobScope._showOpenedTabs(b);$(".panelHeader div.closeBox",b).fadeIn(GD.jobScope.ANIMATE_SPEED);if(b.is(".jobScopePanelLast")){c=b.parents(".featuredJobListings");c.length&&c.animate({paddingBottom:"5px"},GD.jobScope.ANIMATE_SPEED)}};
GD.jobScope._showOpenedTabs=function(b){$(".panelHeader .jobScopeTabs li.hiddenWhenClosed",b).fadeIn(GD.jobScope.ANIMATE_SPEED)};
GD.jobScope._close=function(b,c){var d=b.attr("panelDefaultBgColor");if(!d||d==="transparent")d="white";c.slideUp(GD.jobScope.ANIMATE_SPEED);b.animate({backgroundColor:d,borderTopColor:d,borderRightColor:d,borderBottomColor:d,borderLeftColor:d,paddingBottom:0},GD.jobScope.ANIMATE_SPEED);b.animate({marginBottom:0},GD.jobScope.ANIMATE_SPEED/2);if(b.is(".jobScopePanelLast")){d=b.parents(".featuredJobListings");d.length&&d.animate({paddingBottom:0},GD.jobScope.ANIMATE_SPEED)}b.toggleClass("jobScopePanelOpened");
$(".jobScopeTabs li:visible",b).not(":first").children(".divider").css("border-left-width",1);d=$(".panelHeader .jobScopeTabs li.hiddenWhenClosed,.panelHeader div.closeBox",b);var e=$(".panelHeader .jobScopeTabs .fbSignInPrompt",b);e.length>0&&d.add('.panelHeader .jobScopeTabs li[tabType="CONNECTIONS"] .tabLinkConnected',b);d.fadeOut(GD.jobScope.ANIMATE_SPEED/2,function(){GD.jobScope._showClosedTabs(b,e)})};GD.jobScope._showClosedTabs=function(b,c){c&&c.length&&c.fadeIn(GD.jobScope.ANIMATE_SPEED)};
GD.jobScope._initPanel=function(b,c){var d=$(".panelHeader .closeBox",b),e;e=b.css("border-top-color");b.attr("panelDefaultBgColor",e);d.click(function(){var f=GD.jobScope._getJobInfo(b);GD.analytics.trackEvent("jobPanel","close",f.employerShortName);GD.jobScope._close(b,c);GD.jobScope._disableAutoOpen()});b.addClass("initialized")};
GD.jobScope._selectTab=function(b,c,d){var e=c.attr("contentPanelId").trim(),f=c.attr("tabType").trim(),g=false,h=null;if(e&&f){g=$(".tabContent",b);var j=$(".jobScopeTabs li",b);h=$("#"+e);e=j.not(c);j=c.next("li");var k=c.children(".divider");if(j.length)k=k.add(".divider",j);e.removeClass("selected");e.not(":first").children(".divider").css("border-left-width",1);k.css("border-left-width",0);c.addClass("selected");h.show();g.not(h).hide();g=GD.jobScope._populateTabContents(b,h,f)}d||GD.analytics.trackEvent("jobPanel",
"tabView",f);!g&&h&&GD.jobScope._logTabClick(h)};GD.jobScope._populateTabContents=function(b,c,d){b=GD.jobScope._getJobInfo(b);var e=false;if(c.is(".alreadyLoaded"))$(".focus:first",c).focus();else{c.is(".cloneable")&&GD.jobScope._cloneTabContents(c,d,b);c.is(".alreadyLoaded")||(e=GD.jobScope._loadTabContents(c,d,b,GD.jobScope.googleSearchKey));c.addClass("alreadyLoaded")}return e};
GD.jobScope._cloneTabContents=function(b,c,d){c=c.toLowerCase()+"Panel";var e=null;if(b.is(".cloneableByEmployer")){if(d.employerId)e=$(".jobScopePanel[employerId="+d.employerId+"] .tabContent."+c+".cloneable.alreadyLoaded:first")}else e=$(".jobScopePanel .tabContent."+c+".cloneable.alreadyLoaded:first");if(e.length){d=e.children().clone(true);b.children().remove();b.append(d);b.addClass("alreadyLoaded");$(".focus:first",b).focus()}};
GD.jobScope._loadTabContents=function(b,c,d,e){var f=b.attr("contentsUrl"),g=false;if(f){GD.jobScope._loadAjaxTabContent(b,c,d,f);g=true}else switch(c){case "WEBSEARCH":GD.jobScope._loadWebSearchContent(b,d);break;case "MAP":GD.jobScope._loadMapContent(b,d);break;case "NEWS":d.employerId>0?GD.jobScope._loadNewsSourceData(b,d,e):GD.jobScope._loadNewsContent(b,d,e);break;default:GD.jobScope._removeSpinner(b)}return g};
GD.jobScope._loadAjaxTabContent=function(b,c,d,e){jQuery.ajax({url:e,dataType:"html",error:function(f,g){GD.jobScope._loadAjaxTabContentComplete(b,c,d,null,g)},success:function(f,g){GD.jobScope._loadAjaxTabContentComplete(b,c,d,f,g)}})};
GD.jobScope._loadAjaxTabContentComplete=function(b,c,d,e){if(e)e=e.trim();if(e){e+="<div class='clear'></div>";b.html(e);$(".contentIconsCol .icons .icon[openTab]",b).each(function(){$(this).click(GD.jobScope._onOverviewIconClick)});switch(c){case "CONNECTIONS":GD.augments.augmentContextHelpNodes($(".contextHelpLink",b));GD.runAfterBody(function(){$("a.fbInviteDialogLink",b).click(function(f){GD.graph.showInviteDialog();f.preventDefault()})});GD.runOnFacebookLoad(function(){GD.fb.initLoginButtons(GD.jobScope.facebookRequestedPerms,
"/profile/createSocialNetworkAccount.htm",GD.dom.getDocUrl())})}}else b.html("<div class='tabContentInside'><p class='ajaxError'>Sorry, this information is currently unavailable.  Please check back in a few minutes.</p></div>");GD.jobScope._removeSpinner(b)};GD.jobScope._onOverviewIconClick=function(b){var c=$(b.currentTarget),d=c.attr("openTab");c=c.parents(".jobScopePanel:first");d&&GD.jobScope.showTab(c,d);b.preventDefault();b.stopPropagation();return false};
GD.jobScope._onTabContentsLoaded=function(b,c){$("a[target]",c);$("form[target]",c);$(".focus:first",c).focus()};GD.jobScope._loadWebSearchContent=function(b,c){var d=b.parents(".jobScopePanel:first").attr("id"),e=$(".webResultsContainer",b),f=$(".webSearchField input:first",b),g=['"'+c.employerName+'"','"'+c.employerShortName+'"'];(new GoogleSearchControl(e,f,c,function(){setTimeout(function(){GD.jobScope._onTabContentsLoaded(d,b)},1)})).execute(g)};
GD.jobScope._loadMapContent=function(b,c){var d=b.parents(".jobScopePanel:first").attr("id");GD.maps.loadMap({$tabContent:b,jobInfo:c,jobScopePanelId:d,searchTerms:['"'+c.employerName+'"','"'+c.employerShortName+'"']})};GD.maps={};GD.maps.loadMap=function(b){GD.maps.loadMap=function(){};GD.maps.loadParams=b;b=document.createElement("script");b.type="text/javascript";b.src="//maps.googleapis.com/maps/api/js?v=3.6&sensor=false&callback=GD.maps.initialize";(document.body||document.documentElement).appendChild(b)};
GD.maps.initialize=function(){GD.maps.loadMap=function(b){var c=b.$tabContent,d=b.jobScopePanelId,e=b.searchTerms;(new MapSearchControl(c,b.jobInfo,function(){setTimeout(function(){GD.jobScope._onTabContentsLoaded(d,c)},1)})).execute(e)};GD.maps.loadMap(GD.maps.loadParams);delete GD.maps.loadParams;delete GD.maps.initialize};
GD.jobScope._loadNewsSourceData=function(b,c,d){jQuery.ajax({url:"/jobScope/panels/jobScopeEmployerNewsSource.htm?employerId="+c.employerId,dataType:"json",error:function(e,f){GD.jobScope._onLoadNewsSourceComplete(b,c,d,null,f)},success:function(e,f){GD.jobScope._onLoadNewsSourceComplete(b,c,d,e,f)}})};
GD.jobScope._onLoadNewsSourceComplete=function(b,c,d,e){if(e&&e.newsSource){c.newsSource=e.newsSource;c.newsSourceData=e.newsSourceData;c.googleKeywords=e.googleKeywords;c.rssAttribName=e.rssAttribName}else{c.newsSource="GOOGLE";c.newsSourceData=c.employerShortName}GD.jobScope._loadNewsContent(b,c,d)};
GD.jobScope._loadNewsContent=function(b,c){var d=$(".contentHolder",b);GD.news.initNewsFeed(d,c.employerShortName,{newsSource:c.newsSource,newsSourceData:c.newsSourceData,googleKeywords:c.googleKeywords,rssAttribName:c.rssAttribName,maxItems:GD.jobScope.MAX_NEWS_ITEMS,onCompleteCallback:function(e,f){GD.jobScope._onNewsComplete(e,f,c)}})};
GD.jobScope._getJobInfo=function(b){var c={};if(b&&b.length){var d=b.attr("employerId");if(d){d=Number(d);if(isNaN(d))d=0}else d=0;c.jobScopeId=b.attr("id");c.employerId=d;if(b.attr("employerName"))c.employerName=b.attr("employerName");if(b.attr("employerShortName"))c.employerShortName=b.attr("employerShortName");if(b.attr("employerUrl"))c.employerUrl=b.attr("employerUrl");if(b.attr("location"))c.location=b.attr("location")}return c};
GD.jobScope._onNewsComplete=function(b,c,d){if(c>0){b.is(".tabContentInside")||b.parents(".tabContentInside:first");if(d.newsSource==="GOOGLE"){d=$("<div class='credit googleCredit minorText'>");d.html("Provided by Google News");b.append(d)}else if(d.newsSource==="PRESS_CENTER"){d=$("<div class='credit pressCenterCredit minorText'>");d.html("Provided by employer");b.append(d)}}else{c=$("#NewsSearchPanelTemplate").html();var e=RegExp("%JOB_SCOPE_ID%".escapeForRegex(),"g"),f=RegExp("%SEARCH_TERM%".escapeForRegex(),
"g"),g=RegExp("%URL_ENCODED_SEARCH_TERM%".escapeForRegex(),"g"),h='"'+d.employerShortName.replace('"',"")+'"';c=c.replace(e,d.jobScopeId);c=c.replace(f,d.employerShortName);c=c.replace(g,encodeURIComponent(h));b.empty().html(c);$(".focus:first",b).focus()}};GD.jobScope._removeSpinner=function(b){$(".spinner:first",b).remove()};
GD.jobScope._getNoResultsMessage=function(b){var c="<img src='/static/img/spacer.gif' class='warning' />Sorry, we could not find anything";if(b.employerShortName)c+=" matching <span class='keepTogether'>\""+b.employerShortName+'"</span>';if(b.location)c+=" in <span class='keepTogether'>"+b.location+"</span>";c+=".";return"<p class='noResults'>"+c+"</p>"};
function GoogleSearchControl(b,c,d,e){this.jobInfo=d;this.container=$(b);this.searchField=$(c);this.onLoadCallback=e;this.webSearch=new google.search.WebSearch;this.webSearch.setResultSetSize(2);this.webSearch.setSearchCompleteCallback(this,this._searchComplete)}
GoogleSearchControl.prototype._searchComplete=function(){var b=this.container,c=$("<div class='results webSearchResults'>");results=this.webSearch.results;if(results.length){for(loop=0;loop<results.length&&loop<2;loop++){var d=results[loop],e=$("<div class='data wideData'>");e.append(d.html.cloneNode(true));d=$("<div class='resultItem'>");d.append(e);d.append("<div class='clear'>");c.append(d)}d=this.queryArray[this.currentQueryIndex];e=$("<div class='moreResults'>");d="http://www.google.com/search?q="+
encodeURIComponent(d);var f=$("<a>More Google results &raquo;</a>");f.attr("href",d);f.attr("rel","nofollow");f.attr("target","_blank");e.append(f);c.append(e);b.empty();b.append(c);this.searchField.val(this.queryArray[this.currentQueryIndex]);jQuery.isFunction(this.onLoadCallback)&&this.onLoadCallback()}else if(this.currentQueryIndex+1>=this.queryArray.length){this.searchField.val(webSearchControl.queryArray[webSearchControl.currentQueryIndex]);jQuery.isFunction(this.onLoadCallback)&&this.onLoadCallback()}else{this.currentQueryIndex+=
1;this.webSearch.execute(this.queryArray[this.currentQueryIndex])}};GoogleSearchControl.prototype.execute=function(b){this.queryArray=b;this.currentQueryIndex=0;this.webSearch.execute(this.queryArray[this.currentQueryIndex])};
function MapSearchControl(b,c,d){this.jobInfo=c;this.container=$(b);this.onLoadCallback=d;this.localSearch=new google.search.LocalSearch;this.localSearch.setCenterPoint(c.location);this.localSearch.setResultSetSize(2);this.localSearch.setAddressLookupMode(google.search.LocalSearch.ADDRESS_LOOKUP_DISABLED);this.localSearch.setSearchCompleteCallback(this,this._searchComplete)}MapSearchControl.prototype.execute=function(b){this.queryArray=b;this.currentQueryIndex=0;this.localSearch.execute(this.queryArray[this.currentQueryIndex])};
MapSearchControl.prototype._searchComplete=function(){var b=this.container,c=b.parents(".jobScopePanel:first").attr("id"),d=this.localSearch.results,e=false,f=$("<div class='tabContentInside mapSearchResults'>");if(d.length)d=this._filterResults(d);if(d.length){var g=$("<div class='data'>"),h="A";g.append("<h3 class='contentDesc'>Possible locations</h3>");for(var j=0;j<2&&j<d.length;j++){var k=d[j],l=$("<div class='text'>"),m=$("<div class='resultItem"+(j==0?" resultItemSelected":"")+"'>");if(k.lat&&
k.lng){e=$("<img class='pin'>");d[j].mapPin="http://chart.apis.google.com/chart?chst=d_map_pin_letter&chld="+h+"|FF0000|000000";e.attr("src",d[j].mapPin);m.append(e);h=String.fromCharCode(h.charCodeAt(0)+1);e=true}l.append(k.html.cloneNode(true));m.append(l);m.append("<img src='/static/img/spacer.gif' class='beak' />");m.append("<div class='clear'>");g.append(m);k=this._createPinClickHandler(f,j);m.click(k)}g.append("<div class='clear'>");j=this.queryArray[this.currentQueryIndex]+encodeURIComponent(" "+
this.jobInfo.location);h=$("<div class='moreResults'>");j="http://maps.google.com/maps?q="+j;m=$('<span><a>See all results for "'+this.jobInfo.employerShortName+'" &raquo;</a></span>');k=$("a",m);k.attr("href",j);k.attr("rel","nofollow");k.attr("target","_blank");g.append(h);h.append(m);f.append(g);f.append("<div id='GoogleMapCanvas' class='googleMap'>")}else if(this.currentQueryIndex+1>=this.queryArray.length){g=GD.jobScope._getNoResultsMessage(this.jobInfo);f.append(g)}else{this.currentQueryIndex+=
1;this.localSearch.execute(this.queryArray[this.currentQueryIndex])}f.append("<div class='clear'></div>");b.empty();b.append(f);if(e){this._getPins(d);this._drawMap(f,c)}jQuery.isFunction(this.onLoadCallback)&&this.onLoadCallback()};MapSearchControl.prototype._filterResults=function(b){var c=this.queryArray[this.currentQueryIndex];if(c.charAt(0)=='"')c=c.substring(1);if(c.charAt(c.length-1)=='"')c=c.substring(0,c.length-1);b=this._filterExactMatches(b,c);return b=this._filterNonMatches(b,c)};
MapSearchControl.prototype.PUNCT_REGEX=/[!\?.,|\-=\+\$\*\(\)]+/g;MapSearchControl.prototype.MULTI_SPACE_REGEX=/ {2,}/g;
MapSearchControl.prototype._filterExactMatches=function(b,c){for(var d=false,e=c.replace(this.PUNCT_REGEX," ").replace(this.MULTI_SPACE_REGEX," ").trim(),f=0;f<b.length&&!d;f+=1){d=b[f];var g=d.titleNoFormatting.replace(this.PUNCT_REGEX," ").replace(this.MULTI_SPACE_REGEX," ").trim();d=e.equalsIgnoreCase(g)}if(d){var h=[];for(f=0;f<b.length;f+=1){d=b[f];g=d.titleNoFormatting.replace(this.PUNCT_REGEX," ").replace(this.MULTI_SPACE_REGEX," ").trim();e.equalsIgnoreCase(g)&&h.push(d)}b=h}return b};
MapSearchControl.prototype._filterNonMatches=function(b,c){for(var d=[],e=c.replace(this.PUNCT_REGEX," ").replace(this.MULTI_SPACE_REGEX," ").trim(),f=0;f<b.length;f+=1){var g=b[f],h=g.titleNoFormatting.replace(this.PUNCT_REGEX," ").replace(this.MULTI_SPACE_REGEX," ").trim();h=this._getWordMatch(e,h);if(h>0.5){g.wordMatch=h;d.push(g)}}return d};
MapSearchControl.prototype._commonEmployerNameWords={and:true,associates:true,association:true,co:true,com:true,company:true,corp:true,corporation:true,cos:true,group:true,holding:true,holdings:true,"in":true,inc:true,incorporated:true,international:true,intl:true,limited:true,llc:true,llp:true,lp:true,ltd:true,of:true,plc:true,the:true};
MapSearchControl.prototype._getWordMatch=function(b,c){var d=/[\b\s&!\.,\?\-]+/,e=/([a-zA-Z])[\.\&]([a-zA-Z])/g,f=0,g=0,h=0,j=0;b||(b="");c||(c="");b=b.replace(e,"$1$2");c=c.replace(e,"$1$2");e=b.split(d).sort();var k=c.split(d);d={};for(var l=null,m=0;m<k.length;m++)d[k[m].toLowerCase()]=true;for(m=0;m<e.length;m++){k=e[m].toLowerCase();var n=this._commonEmployerNameWords[k];if(k.trim().length>0&&l!=k){if(n)g+=1;else f+=1;if(d[k])if(n)j+=1;else h+=1;l=k}}return h/f};
MapSearchControl.prototype._createPinClickHandler=function(b,c){var d=this;return function(e){d._onPinSelect(e,b,c)}};MapSearchControl.prototype._onPinSelect=function(b,c,d){if(d!=this.selectedPin){$(".resultItemSelected",this.container).removeClass("resultItemSelected");b=$(b.target);b.is(".resultItem")||(b=b.parents(".resultItem:first"));b.addClass("resultItemSelected");this._setActiveMarker(d,true)}};
MapSearchControl.prototype._setActiveMarker=function(b,c){this.activeMarker&&this.activeMarker.setMap(null);var d=this.pins[b];this.activeMarker=new google.maps.Marker({position:d.center,map:this.map,icon:d.image,title:d.title});this.selectedPin=b;c&&this.map.panTo(d.center)};MapSearchControl.prototype._getPins=function(b){this.pins=[];for(var c=0;c<b.length;c++){var d=b[c];d.mapPin&&this.pins.push({center:new google.maps.LatLng(d.lat,d.lng),image:d.mapPin,title:d.titleNoFormatting})}};
MapSearchControl.prototype._drawMap=function(b,c){var d={zoom:13,center:this.pins[0].center,mapTypeId:google.maps.MapTypeId.ROADMAP,backgroundColor:"white",mapTypeControl:false},e=$("#"+c+" .mapSearchResults .googleMap");if(e.length){this.map=new google.maps.Map(e[0],d);this._setActiveMarker(0,false)}};if(typeof GD==="undefined")GD={};if(typeof GD.news==="undefined")GD.news={};
GD.news.initNewsFeed=function(b,c,d){switch(d.newsSource){case "FINS_GOOGLE":GD.news.getGenericRssFeed(b,c,d);break;case "PRESS_CENTER":GD.news.getPressCenterFeed(b,c,d);break;default:GD.news.getGoogleNewsFeed(b,c,d)}};
GD.news.getGenericRssFeed=function(b,c,d){b=$(b);b.empty().append("<div class='rssNewsItems'>");b=$(".rssNewsItems",b);(new GD.news.RssReader(b)).show(d.newsSourceData,{employerName:c,maxPosts:d.maxItems,maxAgeDays:30,showFeedTitle:false,dateClass:"minorText",moreNewsLinkText:d.moreNewsLinkText},d)};
GD.news.getPressCenterFeed=function(b,c,d){b=$(b);b.empty().append("<div class='rssNewsItems'>");b=$(".rssNewsItems",b);b=new GD.news.RssReader(b);c={employerName:c,maxPosts:d.maxItems,showFeedTitle:false,dateClass:"minorText",rssAttribName:d.rssAttribName,moreNewsLinkText:d.moreNewsLinkText};if(d.googleKeywords)c.googleKeywords=d.googleKeywords;b.show(d.newsSourceData,c,d)};
GD.news.getGoogleNewsFeed=function(b,c,d,e){var f=d.googleKeywords;f=f?'"'+f.replace('"',"")+'"':'"'+c.replace('"',"")+'"';b=$(b);jQuery.ajax({url:"http://ajax.googleapis.com/ajax/services/search/news",data:{v:"1.0",key:d.googleSearchKey,q:f,rsz:"small"},dataType:"jsonp",success:function(g){GD.news.onGoogleNewsResponse(c,b,g,f,d,e)},error:function(){GD.news.onGoogleNewsResponse(c,b,null,f,d,e)}})};
GD.news.onGoogleNewsResponse=function(b,c,d,e,f,g){var h;b=true;if(d&&d.responseData)h=d.responseData.results;if(c&&h&&h.length){var j=false,k=999;if(g){b=false;k=g.fillCount}else if(f.maxItems)k=f.maxItems;if(k<0)k=1;if(g){d=$(".rssItems",c);if(!d.length){d=$("<div class='rssItems'></div>");c.append(d)}}else{d=$("<div class='results newsSearchResults'>");c.empty();c.append(d)}if(b){for(var l=0;l<h.length&&l<k&&!j;l++)if(h[l].image)j=true;j&&d.addClass("newsSearchResultsWithPhotos")}var m;for(l=0;l<
h.length&&l<k;l++){var n=h[l],u=$("<div class='storyCol'>"),q=null;m=$("<div class='entry'>");d.append(m);if(j&&b){q=$("<div class='photoCol'>");m.append(q)}m.append(u);if(n.image&&b){var r=$("<a>"),p=$("<img>");p.attr("src",n.image.url).attr("width",n.image.tbWidth).attr("height",n.image.tbHeight);r.attr("href",n.image.originalContextUrl).attr("rel","nofollow").attr("target","_blank");r.append(p);q.append(r)}q=$("<div class='title'>");r=$("<a>"+n.title+"</a>");r.attr("href",n.unescapedUrl).attr("rel",
