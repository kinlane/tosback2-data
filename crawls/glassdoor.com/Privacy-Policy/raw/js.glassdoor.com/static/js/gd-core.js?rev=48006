h*100;d.totalIncludedFriendsPercent=d.totalIncludedFriendsPercent>99?100:d.totalIncludedFriendsPercent>0&&d.totalIncludedFriendsPercent<1?1:Math.floor(d.totalIncludedFriendsPercent);d.friendsUsingGlassdoorPercent=d.friendsUsingGlassdoorCount/h*100;if(d.friendsUsingGlassdoorPercent>0&&d.friendsUsingGlassdoorPercent<1)d.friendsUsingGlassdoorPercent=1;d.totalInvitedFriendsPercent=d.totalIncludedFriendsPercent-d.friendsUsingGlassdoorPercent;$("div.existingFriends",f).css("width",d.friendsUsingGlassdoorPercent+
