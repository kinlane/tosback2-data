GD.augments._createTagEditorNode=function(b,c,d,e,f,g){if(typeof b!=="string")alert("GD.augments.addTagField called without valid tagEditorId.");else{b=b.trim();if(b.length<=0)alert("GD.augments.addTagField called with an empty tagEditorId.");else if(typeof c!=="string")alert("tag field name is required.");else{c=c.trim();if(typeof d!=="string")d="";d=d.trim();if(typeof e!=="string")e="";e=e.trim();f=$("<div>");f.attr("id",b);f.addClass(GD.augments.classNameTagEditor);f.click(function(h){GD.augments._onTagEditorClick(h,
b)});GD.augments._addTagEditAreaNode(b,f,c,d,g);GD.augments._addTagHelpNode(b,f,e);return f}}};GD.augments._addTags=function(b,c,d,e){if(typeof e==="string"){var f=d.val(),g;if(f){f=f.trim();if(f.length>0)g=f.split(",")}g||(g=[]);e=e.split(",");for(f=0;f<e.length;f+=1){var h=e[f].trim();if(h.length>0)if(!g.contains(h)){g.push(h);h=GD.augments._createEnteredTagNode(b,h);c.children(":last").before(h)}}g.sort();d.val(g.join());c&&GD.augments._callOnChangeHandler(c.onchange)}};
GD.augments._removeTagValues=function(b,c,d){if(id(b)&&d){if(typeof d==="string"){d=d.trim();d=d.length>0?d.split(","):[]}if(!(!d.length||d.length<=0)){if(b=id(b+GD.augments.tagsFieldValuePostfix)){var e=b.value.trim(),f=[];e=e.length>0?e.split(","):[];for(var g=0;g<e.length;g++){for(var h=e[g],j=true,k=0;k<d.length&&j;k+=1)if(d[k].toLowerCase()===h)j=false;j&&f.push(h)}b.value=f.length>0?f.join():""}c&&GD.augments._callOnChangeHandler(c.onchange)}}};
GD.augments._addTagEditAreaNode=function(b,c,d,e,f){try{var g=$("<div>");g.attr("id",b+GD.augments.tagsEditAreaPostfix);g.addClass(GD.augments.classNameTagEditorEditArea);g.addClass("clearfix");c.append(g);var h=$("<input>");h.attr("id",b+GD.augments.tagsEditFieldPostfix);h.addClass(GD.augments.classNameTagEditorEditField);h.attr("name","temp_edit_"+d);h.attr("type","text");h.attr("maxLength",GD.augments.tagsMaxTagLength);h.keypress(function(l){return GD.augments._onTagEditorKeyPress(l,b)}).keydown(function(l){return GD.augments._onTagEditorKeyPress(l,
