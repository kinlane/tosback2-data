GD.empInfo._showDeepLinkBar=function(){if(GD.empInfo._deepLinkTimeout){clearTimeout(GD.empInfo._deepLinkTimeout);GD.empInfo._deepLinkTimeout=null}$(window).unbind("scroll",GD.empInfo._showDeepLinkBar);var b=$("#DeepLinkSocialSharingBar");b.css({display:"none",height:"0"});b.fadeIn(100,function(){b.animate({height:45},{queue:false,duration:600,easing:"easeOutQuint"})})};GD.empInfo._closeDeepLinkBar=function(b){b.animate({height:0},{queue:false,duration:600,easing:"easeOutQuint",complete:function(){b.fadeOut(100)}})};
