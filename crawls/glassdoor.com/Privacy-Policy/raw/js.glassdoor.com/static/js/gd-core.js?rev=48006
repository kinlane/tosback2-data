if(typeof String.prototype.trim=="undefined")String.prototype.trim=function(){a=this.replace(/^\s+/,"");return a.replace(/\s+$/,"")};String.prototype.equalsIgnoreCase=function(b){b=b.escapeForRegex();return this.match(RegExp(b,"i"))};String.prototype.startsWith=function(b){return this.indexOf(b)==0};String.prototype.contains=function(b){return this.indexOf(b)>=0};String.prototype.endsWith=function(b){var c=this.length-b.length;return c>=0&&this.lastIndexOf(b)==c};
