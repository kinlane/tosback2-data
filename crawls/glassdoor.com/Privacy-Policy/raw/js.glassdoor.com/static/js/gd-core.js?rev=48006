GD.fb.FBAppRequestHandler=GD.Class({constructor:function(b){var c=this;c.tracker=b;c.$includeFriends=$("div#IncludeFriends")},onRequestComplete:function(){Logger.inspect("GD.fb.FBAppRequestHandler.onRequestComplete(): expected to be re-defined by the controller via 'bind'")},onRequestCancel:function(){Logger.inspect("GD.fb.FBAppRequestHandler.onRequestCancel(): expected to be re-defined by the controller via 'bind'")},bind:function(b,c){var d=this;if(typeof c!="function")throw Error("GD.fb.FBAppRequestHandler.bind() expects function argument");switch(b){case "requestComplete":d.onRequestComplete=
c;break;case "requestCancel":d.onRequestCancel=c;break;default:throw Error("FBAppRequestHandler#bind() unexpected method: '"+b+"'");}},sendRequest:function(b,c){var d=this;Logger.inspect(b,"GD.fb.FBAppRequestHandler.sendRequest()");GD.fb.fbRootReset();FB.ui({display:"dialog",method:"apprequests",new_style_message:true,message:b.message,title:b.title,to:""+b.toSids},function(e){if(e){e.networkId=b.networkId;e.toGids=b.toGids;e.toSids=b.toSids}d.handleResponse(e,c)})},handleResponse:function(b,c){var d=
this;if(b&&!(b.error_code||b.error_msg)){Logger.inspect(b,"GD.fb.FBAppRequestHandler.handleResponse(): Facebook app request response");d.tracker.trackAppRequest(b);GD.analytics.trackMvGoal();d.onRequestComplete(b)}else if(d.$includeFriends.data("endGame")){Logger.debug("GD.fb.FBAppRequestHandler.handleResponse(): fbResponse is null -- probably cancelled (in end game)");d.onRequestCancel(b);d.onRequestComplete(b)}else{b?d.error(b):Logger.debug("GD.fb.FBAppRequestHandler.handleResponse(): fbResponse is null -- probably cancelled");
c.onInvitesComplete()}},error:function(b){Logger.inspect(b,"GD.fb.FBAppRequestHandler.error(): app request error");b="fb apprequest failed: c="+b.error_code+",m="+b.error_msg+" [fr="+response.authResponse.userID+", to="+b.toSids+"]";Logger.error("GD.fb.FBAppRequestHandler.error(): "+b);jQuery.ajax({url:GD.ajax.getAjaxUrlRespectingSecurity("/util/ajax/lgr.htm"),type:"POST",dataType:"json",data:{m:b,l:"e"}})}});
GD.fb.InviteFriendsLoader=GD.Class({constructor:function(b,c,d){this.ajaxUrl=b;this.attemptIntervalMs=c;this.maxAttempts=d;this.finishedLoading=this.friendsLoaded=false},onNetworksLoaded:function(b){Logger.inspect(b,"GD.fb.InviteFriendsLoader.onNetworksLoaded() expected to be re-defined by the controller via 'bind'")},onFriendsLoaded:function(b){Logger.inspect(b,"GD.fb.InviteFriendsLoader.onFriendsLoaded() expected to be re-defined by the controller via 'bind'")},onFinishedLoading:function(b){Logger.inspect(b,
"GD.fb.InviteFriendsLoader.onFinishedLoading() expected to be re-defined by the controller via 'bind'")},bind:function(b,c){if(typeof c!="function")throw Error("FBAppRequestHandler#bind() expects function argument");switch(b){case "networksLoaded":this.onNetworksLoaded=c;break;case "friendsLoaded":this.onFriendsLoaded=c;break;case "finishedLoading":this.onFinishedLoading=c;break;default:throw Error("InviteFriendsLoader#bind() unexpected method: '"+b+"'");}},poll:function(){var b=this;jQuery.ajax({url:b.ajaxUrl,
dataType:"json",success:function(c){c||Logger.error("No data returned for GD.fb.InviteFriendsLoader.poll()");b.process(c)},error:function(c,d,e){Logger.inspect(e,"GD.fb.InviteFriendsLoader.poll() error: "+d);b.process(null)}})},process:function(b){var c=this;if(b&&b.networks&&!this.networksLoaded){this.networksLoaded=true;this.onNetworksLoaded(b)}if(b&&b.friendsLoaded&&!this.friendsLoaded){this.friendsLoaded=b.friendsLoaded;this.onFriendsLoaded(b)}if(b&&b.finishedLoading&&!this.finishedLoading){this.finishedLoading=
b.finishedLoading;this.onFinishedLoading(b)}if(this.maxAttempts<=0){if(!this.friendsLoaded){this.friendsLoaded=true;this.onFriendsLoaded()}if(!this.finishedLoading){this.finishedLoading=true;this.onFinishedLoading(b)}}else if(!this.finishedLoading){this.maxAttempts-=1;setTimeout(function(){c.poll()},c.attemptIntervalMs)}}});
