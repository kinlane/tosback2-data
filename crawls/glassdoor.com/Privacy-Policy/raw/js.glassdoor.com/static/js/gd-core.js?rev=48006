GD.photos.nextPhoto=function(b){var c=true;if(GD.photos.currentPhotoIndex==-1)GD.photos.currentPhotoIndex=0;else if(GD.photos.currentPhotoIndex<GD.photos.pagePhotoIds.length-1)GD.photos.currentPhotoIndex++;else if(GD.photos.nextPageUrl)GD.photos.loadNextPage(GD.photos.nextPageUrl,false);else if(GD.photos.pagePhotoIds.length==1)c=false;else GD.photos.currentPhotoIndex=0;c&&GD.photos.selectPhoto(GD.photos.pagePhotoIds[GD.photos.currentPhotoIndex],b);GD.photos.updateLocalNavButtons()};
