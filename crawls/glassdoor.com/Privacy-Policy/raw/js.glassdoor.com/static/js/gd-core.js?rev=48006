GD.photos.onHashPhotoResponse=function(b){if(b&&GD.photos.isExpectingAjaxResponse){var c=$("#Photo_0 .empShortName").html();c+=" &ndash; ";if(b.location)c+=b.location+" &ndash; ";c+="Posted ";if(b.employerPhoto)c+="by <strong>employer</strong>";else if(b.credit)c+="by <strong>"+b.credit+"</strong>";c+=" on "+b.createDate;$("#Photo_0 .photoData").html(c);$("#Photo_0 .caption").html(b.caption);$("#Photo_0 img").attr({height:b.mediumHeight,width:b.mediumWidth});$("#Photo_0 img.companyPhoto").attr("photoUrl",
