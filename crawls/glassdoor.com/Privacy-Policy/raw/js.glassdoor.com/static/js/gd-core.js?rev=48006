GD.photos.checkHashTag=function(){var b=window.location.hash,c=0;if(b&&b.startsWith("#")&&b.indexOf("-")>=0){b=b.split("-");if(b.length>1){b=b[1];if(b=="last")c=GD.photos.pagePhotoIds[GD.photos.pagePhotoIds.length-1];else{b=parseInt(b);if(b!=NaN){c=b;GD.photos.currentPhotoFromHash=true}}}}if(GD.photos.currentPhotoFromHash)for(b=0;b<GD.photos.pagePhotoIds.length;b++)if(GD.photos.pagePhotoIds[b]==c){GD.photos.currentPhotoFromHash=false;break}return c};
GD.photos.selectPhoto=function(b,c){GD.photos.isExpectingAjaxResponse=false;if(b!=GD.photos.currentPhotoId){for(var d=0;d<GD.photos.pagePhotoIds.length;d++)if(GD.photos.pagePhotoIds[d]==b){GD.photos.currentPhotoIndex=d;break}d=(GD.photos.firstRecordIndexOnPage+GD.photos.currentPhotoIndex+1).addCommas();$("#PhotoContainer .nav .count .currItemIndex").html(d);$("#PhotoContainer .nav .count").show();$("#PhotosDataHeader .thumbs .frame").removeClass("currPhoto");$("#PhotoThumbnailFrame_"+b).addClass("currPhoto");
