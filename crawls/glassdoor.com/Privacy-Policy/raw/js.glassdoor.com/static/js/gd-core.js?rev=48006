GD.news.RssReader.prototype._onRssNewsComplete=function(b,c){this.options&&jQuery.isFunction(this.options.onCompleteCallback)&&this.options.onCompleteCallback(this.rssFeedContainer,b,this.options);c&&jQuery.isFunction(c.onCompleteCallback)&&c.onCompleteCallback(this.rssFeedContainer,b,c)};if(typeof GD==="undefined")GD={};if(typeof GD.photos==="undefined")GD.photos={};GD.photos.isFirstPage=true;GD.photos.isLastPage=true;GD.photos.firstRecordIndexOnPage=0;GD.photos.pagePhotoIds=[];
