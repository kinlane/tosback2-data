GD.videos.getRandomStartIndex=function(b){var c=b.videoData,d=c.items.length,e=c.startIndex+Math.floor(Math.random()*d);if(e<1)e=1;else if(e>=c.startIndex+d)e=c.startIndex+d-1;b.currentVideoIndex=e};GD.videos.showPreviousVideo=function(b){var c=b[0];if(c.currentVideoIndex>1)c.currentVideoIndex<=c.videoData.startIndex?GD.videos.loadAndShowVideo(b,c.videoData.startIndex-c.videoData.itemsPerPage,c.currentVideoIndex-1):GD.videos.showVideo(b,c.currentVideoIndex-1)};
GD.videos.showNextVideo=function(b){var c=b[0];if(c.currentVideoIndex<c.videoData.totalItems){var d=c.videoData.startIndex+c.videoData.items.length-1;c.currentVideoIndex>=d?GD.videos.loadAndShowVideo(b,c.currentVideoIndex+1,d+1):GD.videos.showVideo(b,c.currentVideoIndex+1)}};
GD.videos.loadAndShowVideo=function(b,c,d){var e=b[0],f="http://gdata.youtube.com/feeds/api/users/"+e.ytUsername+"/uploads";if(c<1)c=1;else if(c>e.videoData.totalItems)c=e.videoData.totalItems-e.videoData.itemsPerPage+1;jQuery.ajax({url:f,data:{v:"2",alt:"jsonc","start-index":c},dataType:"jsonp",success:function(g){GD.videos.onLoadAndShowVideoComplete(b,g,d)},error:function(){GD.videos.onLoadAndShowVideoComplete(b,null)}})};
GD.videos.onLoadAndShowVideoComplete=function(b,c,d){if($(".vidContainer",b).length&&c&&c.data){var e=b[0];e.videoData=c.data;GD.videos.showVideo(b,d)}else c&&c.error&&c.error.message?GD.videos.displayError(b,"YouTube user '"+e.ytUsername+"': "+c.error.message):GD.videos.displayError(b,"Got an error for '"+options.url+"' &ndash; no items.")};
