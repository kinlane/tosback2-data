GD.videos.createVideoPlayer=function(b){var c=b.attr("id"),d=c+GD.videos.VIDEO_PLAYER_ID_POSTFIX,e=b[0];$(".vidContainer",b);var f=b.attr("videoWidth"),g=b.attr("videoHeight"),h=b.attr("showHD")=="true",j="http://www.youtube.com/v/"+GD.videos.getVideoId(e,e.currentVideoIndex);j+="?rel=0&fs=1&enablejsapi=1&version=3&iv_load_policy=3&showinfo=0&playerapiid="+d;if(h)j+="&hd=1";swfobject.embedSWF(j,c+GD.videos.VIDEO_PLAYER_PLACEHOLDER_ID_POSTFIX,f,g,"8",null,null,{allowScriptAccess:"always",rel:"0",allowFullScreen:"true",
