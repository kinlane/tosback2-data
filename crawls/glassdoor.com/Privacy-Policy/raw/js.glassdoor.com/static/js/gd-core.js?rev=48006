d.appendChild(f);return d}catch(g){Logger.fatal("GD.augments._createEnteredTagNode",g)}};GD.augments._onCloseBoxMouseOver=function(b){try{b=GD.event.fixEvent(b);if(b.target){var c=b.target.parentNode;if(c.className==GD.augments.classNameTagEditorEnteredTag)c.className=GD.augments.classNameTagEditorEnteredTagHilited}}catch(d){Logger.fatal("GD.augments._onCloseBoxMouseOver",d)}};
GD.augments._onCloseBoxMouseOut=function(b){try{b=GD.event.fixEvent(b);if(b.target){var c=b.target.parentNode;if(c.className==GD.augments.classNameTagEditorEnteredTagHilited)c.className=GD.augments.classNameTagEditorEnteredTag}}catch(d){Logger.fatal("GD.augments._onCloseBoxMouseOut",d)}};
