function initTree(a){var b,c,e,f,h,m,n;for(b=0;b<a.childNodes.length;b++)if(a.childNodes[b].tagName&&a.childNodes[b].tagName.toLowerCase()=="li"){var o=a.childNodes[b];e=document.createElement("span");c=document.createElement("span");f=document.createElement("span");e.appendChild(c);c.appendChild(f);e.className="a "+o.className.replace("closed","spanClosed");e.onMouseOver=function(){};c.className="b";c.onclick=treeToggle;f.className="c";m=o.childNodes.length;h=0;n=null;for(c=0;c<o.childNodes.length;c++)if(o.childNodes[c].tagName&&
