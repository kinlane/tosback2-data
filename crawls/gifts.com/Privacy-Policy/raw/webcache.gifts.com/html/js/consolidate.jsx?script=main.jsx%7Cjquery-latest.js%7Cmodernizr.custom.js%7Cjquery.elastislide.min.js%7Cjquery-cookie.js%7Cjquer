        function(){
            $(this).closest('li').find('.persDetails').show(0,function(){
                $(this).hover(
                    function(){
                        $(this).show();
                    },
                     function(){
                         $(this).hide();
                     }
                );
            }) 
        },
        function(){
            $(this).closest('li').find('.persDetails').hide(); 
        }
     );

     //------------------------------------------
    
     if($('#ad').length) {
        var $this = $('#ad');       
        $this.not('.hpSection #ad').height($this.prev('section').height() - 4);
        
        $this.find('iframe').wrap('<div class="adWrapper"/>');
        var $adH = $this.height() - $this.find('.cpmtitle').outerHeight(true);
        var $adWrapperH = $this.find('.adWrapper').height();
        $('.adWrapper').css('margin-top', parseInt(($adH - $adWrapperH) /2) + 'px');
         

    };
  
});
//==========================================
//xmas countdown
function getDaysUntilXmas(){
    var now = new Date();
    var curr_year = now.getFullYear();
    var dateEnd = new Date(curr_year, 11, 25); // months are zero-based
    var days = (dateEnd - now) / 1000/60/60/24;   // convert milliseconds to days

    return Math.floor(days);
    document.write(getDaysUntilXmas());
}

//============================================

function toggleThisIdeaRow(row, ideaPerRow) {
    var endIndex = (row * ideaPerRow);
    var startIndex = endIndex - ideaPerRow;
    do {
        startIndex++;

        var scoreDivName = 'score'+startIndex;
        var scoreDiv =  document.getElementById(scoreDivName)
        if (scoreDiv != null) {
            if (scoreDiv.style.display == "block") {
             //hide
             scoreDiv.style.visibility = "hidden";
             scoreDiv.style.display = "none";
            }
            else {
                 //show
                 scoreDiv.style.visibility = "visible";
                 scoreDiv.style.display = "block";
            }
        }
     } while (startIndex < endIndex);
 };

//-------------------------------------
 function changeLocation(menuObj)
 {
    var i = menuObj.selectedIndex;

    if (i >= 0)
    {
        window.location = menuObj.options[i].value;
    }
 };
     
//-------------------------------------------

function uniPop(href, winname, width, height, x, y, toolbar, scrollbars, resizable, status) {
    if (href == "") href = '/support';
    if (winname == 'spellChecker') href += escape(document.eventForm.notes.value);
    if ((width < 100) || (width == null)) var width = '515';
    if ((height < 100) || (height == null)) var height = '475';
    if (toolbar == null) var toolbar = 'yes';
    if (scrollbars == null) var scrollbars = 'yes';
    if (resizable == null) var resizable = 'yes';
    if (status == null) var status = 'yes';
    var theParam = "width=" + width +
                   ",height=" + height +
                   ",toolbar=" + toolbar +
                   ",menubar=" + toolbar +
                   ",location=" + toolbar +
                   ",scrollbars=" + scrollbars +
                   ",resizable=" + resizable +
                   ",status=" + status +
                   ",screenX=" + x + ",screenY=" + y + ",top=" + y + ",left=" + x;
    
    var thePopup = window.open(href, winname, theParam);
    if (thePopup.opener == null) thePopup.opener = self;
};  

function externalPop(href) {

    uniPop(href, "_blank", 765, 600, 75, 50, 'yes', 'yes', 'yes', 'yes');
};

//---------------------------------------------------


function linkCodeFinder(obj,linkName) {
    var s=s_gi('${omnitureAcctName}');
    s.linkTrackVars='prop22';
    s.linkTrackEvents='None';
    s.prop22=linkName;
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
};
function linkCodeChannelFinder(obj, linkPrefix) {
    var s=s_gi('${omnitureAcctName}');
    channel = (s != null && s.channel != "") ? s.channel : '';
    linkCodeFinder(obj, linkPrefix+': '+channel);
};
function linkCodeSLTracker(obj, linkPrefix) {
    var s=s_gi('${omnitureAcctName}');
    channel = (s != null && s.channel != "") ? s.channel : '';
    s.linkTrackVars='events';
    s.linkTrackEvents='event20';
    s.events='event20';
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',channel); }
};
function linkCodeFBConnect(obj,linkName) {
    var s=s_gi('${omnitureAcctName}');
    s.linkTrackVars='eVar11';
    s.linkTrackEvents='event14';
    s.eVar11=linkName;
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
};

function linkCodePinterest(obj,linkName) {
    var s=s_gi('${omnitureAcctName}');
    s.linkTrackVars='prop8,products,events';
    s.linkTrackEvents='event9';
    s.prop8=linkName;
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
};

function linkCodeNewProd(obj,linkName) {
    var s=s_gi('${omnitureAcctName}');
    s.linkTrackVars='prop32';
    s.linkTrackEvents='None';
    s.prop32=linkName;
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
};


function linkCodeQuizAnswers(obj,linkName) {
    var s=s_gi('${omnitureAcctName}');
    s.linkTrackVars ='eVar14';
    s.linkTrackEvents ='None';
    s.prop17 = '${question.id} | ${answer.id}';
    s.eVar12 = '${quiz.recipientId} | ${question.id} | Q${status.index}: A${answerStatus.count}:linkName}'
    s.eVar14 = '${question.id}';
    var lt=obj.href!=null?s.lt(obj.href):"";if (lt=="") { s.tl(obj,'o',linkName); }
};
          

