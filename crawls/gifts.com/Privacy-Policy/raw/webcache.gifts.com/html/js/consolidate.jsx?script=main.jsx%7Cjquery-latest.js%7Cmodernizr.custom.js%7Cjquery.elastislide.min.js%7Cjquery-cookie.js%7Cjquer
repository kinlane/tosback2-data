    $navitem.each(function(){
        var $navDrop = $(this).find('.navDrop').css('left','0');
        var $label =  $(this).find('a.label');      
        $(this).hover(
            function(){
                $navDrop.show();
                $label.css({'background-color':'#fff','color':'#202020','display': 'block'}).closest($navitem).css('z-index','115').prev().find('a.label').css({'border-right':'0','padding-right':'26px', 'padding-left':'25px'});
                $label.addClass('nav_xmasOn');
                            
                $('.col2 li').width($navDrop.width() / 2);
                $('.col3 li').width($navDrop.width() / 3);
                $('.col4 li').width($navDrop.width() / 4);
                                                                
            },
            function(){
                $navDrop.hide();
                $label.css({'background-color':'','color':'','display': ''}).closest($navitem).css('z-index','').prev().find('a.label').css({'border-right':'','padding':''});
                $navitem.find('a.labelSelected').closest($navitem).prev().find('a.label').css({'border-right':'0','padding-right':'26px','padding-left':'25px'});
                $label.removeClass('nav_xmasOn');                      
            }
        )
        
       $navitem.find('a.labelSelected').closest($navitem).prev().find('a.label').css({'border-right':'0','padding-right':'26px', 'padding-left':'25px'});
        
    });
    
     $navitem.slice(-4).each(function(){
        $(this).find('.navDrop').css({'right':'0','left':''});    
        
  
    })
   

