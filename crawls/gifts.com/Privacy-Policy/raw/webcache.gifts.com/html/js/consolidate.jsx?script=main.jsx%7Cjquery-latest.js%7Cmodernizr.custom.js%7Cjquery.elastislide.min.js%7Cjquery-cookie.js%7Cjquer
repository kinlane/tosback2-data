//--------- SponsoredListing  ----------
//--------------------------------------
//--------------------------------------

function loadSponsoredListings(url, queryString, callback) {
   
    $.ajax({
        url: url,
        dataType: "html",
        data: queryString,
        context:this,
        success: callback        
    });
};
function loadMgmSponsoredListings(keyword, config, channel) {
    var params = {'keyword': keyword, 'channel': channel, 'listingsConfig': config};

    loadSponsoredListings('/sponsored/listing/mgm', params,function (data) {renderSLListings(data, config);});
};
function loadContentSponsoredListings(keyword, serverUrl, channel, config, isSearchKey) {
    var params = {'keyword': keyword, 'channel': channel, 'listingsConfig': config, 'serverUrl' : serverUrl, 'searchKey' : isSearchKey};

    loadSponsoredListings('/sponsored/listing/content', params, function (data) { renderSLListings(data, config);});
};
function loadKeywordSponsoredListings(keyword, channel, config) {
    var params = {'keyword': keyword, 'channel': channel, 'listingsConfig': config};

    loadSponsoredListings('/sponsored/listing/keyword', params, function (data) {renderSLListings(data, config);});
};

renderSLListings = function (html, config) {
     var v2 = $(document.createElement('div'));
     var v1 = $(v2).append(html);
     var v3 = $(v1).children();
    $.each(config.split(';'), function(index, el) {

      try{
        el = this.split(':')[0];

        $('#'+el).append(v3[index]);

      }
        catch(e) {}
    });
};

