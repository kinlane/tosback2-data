     /* jquery select box */
    if($('.skinned-select').length == 0){
        $('select.jSelect').wrap('<div class="my-skinnable-select"></div>');
        $('.my-skinnable-select').each(function(i){
            var selectContainer = $(this);
            // Remove the class for non JS browsers
            selectContainer.removeClass('my-skinnable-select');
            // Add the class for JS Browers
            selectContainer.addClass('skinned-select');
            // Find the select box
            selectContainer.children().before('<div class="select-text"></div>').each(function() {
                $(this).prev().html(this.options[this.selectedIndex].innerHTML)
            });
            // Store the parent object
            var parentTextObj = selectContainer.children().prev();
            // As we click on the options
            selectContainer.children().change(function() {
                 // Set the value of the html
                parentTextObj.html(this.options[this.selectedIndex].innerHTML);
            })
        });
        
        $('.skinned-select').prepend('<div class="selectArrow"></div>');  
    }
    
    //-----------------------------
    //add divider after icon in srp
    if($('.icons div').length > 1) {
                    
      $('.icons div:not(:last-child)').after('<div class="divider"></div>');            
       
    }
    
    //=====================================
     
     //shorten product drescription in product page
     
    var showChar = 270;
    var ellipsestext = "...";
    var moretext = "<span class='arrowRt'></span> Expand";
    var lesstext = "<span class='arrowUp'></span> Collapse";
    
    $('.expandText').each(function() {
        var content = $(this).html();
    
        if(content.length > showChar) {
    
            var c = content.substr(0, showChar);
            var h = content.substr(showChar-1, content.length - showChar);
            
            var html = c + '<span class="moreellipses">' + ellipsestext+ '&nbsp;</span><span class="morecontent"><span class="restofcontent">' + h + '</span>&nbsp;&nbsp;<a href="" class="expandlink">' + moretext + '</a></span>';
            
            $(this).html(html);
            $('.ctmLink').insertBefore('.expandlink');
        }
    
    });
    
    $(".expandlink").click(function(){
   
        if($(this).hasClass("less")) {
            $(this).removeClass("less");
            $(this).html(moretext);
            
        } else {
            $(this).addClass("less");
            $(this).html(lesstext);
              
        }
        $(this).parent().prevAll('.moreellipses').toggle();        
        $(this).prevAll('.restofcontent').toggle();
        
        return false;
        
    });
    
    //===========================================
     //for popup videos
     
    $("a.fancyvideo").click(function(e) {
        e.preventDefault();
        $.fancybox({
            padding             : 10, //optional     
            overlayShow         : true,
            overlayOpacity      : 0.7,
            openEffect          : 'elastic',
            closeEffect         : 'elastic',
            title               : this.title,
            href                : this.href.replace(new RegExp("watch\\?v=", "i"), 'v/'),
            type                : 'swf',
            swf                 : {
                wmode: 'transparent',
                allowfullscreen : 'true',
                allowscriptaccess : 'always'
            }

        });
        
    })
   
   //================================ 
   
   
    
});


