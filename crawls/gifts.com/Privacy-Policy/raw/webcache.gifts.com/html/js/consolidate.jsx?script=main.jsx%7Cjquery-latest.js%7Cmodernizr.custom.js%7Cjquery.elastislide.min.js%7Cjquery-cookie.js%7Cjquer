    personalityView = function(){
        var thisRecip;
        if ( ($('.personality').attr('id')=="persMen") || ($('.personality').attr('id')=="persSrWomen") || ($('.landingPage').attr('id')=="mday")){thisRecip = "shopPersWomen";}
        else if($('.personality').attr('id')=="persTeenGuys"){thisRecip = "shopPersTeenGirls";}
        else if( ($('.personality').attr('id')=="persTeenGirls") || ($('.landingPage').attr('id')=="grad")){thisRecip = "shopPersTeenGuys";}
        else if($('.personality').attr('id')=="persBoys"){thisRecip = "shopPersGirlsSm";}
        else if($('.personality').attr('id')=="persGirls"){thisRecip = "shopPersBoysSm";}
        else {thisRecip = "shopPersMen";}
        
        dettachedPers = $('#shopPers #'+thisRecip).siblings().detach();                           
        $('#persRecip').siblings('.select-text').text($('select#persRecip option[value="'+thisRecip+'"]').text());
        $('select#persRecip option[selected]').removeAttr('selected');
        $('select#persRecip option[value="'+thisRecip+'"]').attr('selected', 'selected');
        
    }

    personalityView();
    
    
