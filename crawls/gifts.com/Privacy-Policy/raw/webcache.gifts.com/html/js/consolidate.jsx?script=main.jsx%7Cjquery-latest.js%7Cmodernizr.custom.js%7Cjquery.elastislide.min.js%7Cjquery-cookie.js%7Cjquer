                if( $('body').hasClass('ie8') ){
                    $('#'+bbID).find('.prodName').css('background','rgb('+$bgNum+')');
                }
                else {
                
                    $('#'+bbID).find('.prodName').css('background','rgba('+$bgNum+', 0.80)'); 
                }
            };
    
        });
    
        //---------------------------
        
        //show default bb
           var bbDefault = $bbTab.eq(0).attr('id').split('tab_')[1];
 
          $('#'+bbDefault).show();
          //  $('#tab_'+bbDefault).addClass('selected');
          
          //  $('#bb .tabs').css('visibility','visible'); //show tabs after bb is loaded
       //-------------------------------------------
       //cycle thru billboard
         
        if($('#bb').length){
            var currTab;
            var currBB;
            var currInt = 6000;
            
            var gotoNext = function () {           
                
                currTab = $('#bb .tabs .selected').attr('id');
        
                $('#'+currTab).removeClass('selected').next().addClass('selected');
        
                if(currTab == $bbTab.last().attr('id')) {
                    currTab = $bbTab.first().addClass('selected');
                };
        
                $('#bb .bbDivs').hide();                           
                currBB = $('#bb .tabs .selected').attr('id').split('tab_')[1];
                $('#'+currBB).show();
        
            };
        
             var interval = setInterval(gotoNext, currInt);
            
            $bbTab.hover(
                function(){
                   clearInterval(interval);
                },
                function(){
                   interval = setInterval(gotoNext, currInt);
                }
            );
          
            $bbTab.click(function(){
               clearInterval(interval);
                               
               $('.bbDivs').hide();
               $('.tab').removeClass('selected');
               $(this).addClass('selected');
                var BB = $(this).attr('id').split('tab_')[1];
               $('#'+BB).show();
           
                
                $bbTab.mouseout(function(){
                   clearInterval(interval);       
                })
            });
            
            
        };        
        
        //---------------------------------------- 
          //if no description under the products in bb 3-5 template
        $('#bb .bbDivs').each(function(){
            var $this = $(this);
            var $prodDesc = $this.find('ul.fiveProds .prodDesc');
            if($prodDesc.length) {
                $this.find('ul.fiveProds').css({'top':'80px','height':'207px'}); 
                $this.find('ul.fiveProds li').css('margin-bottom','25px');           
            };                                    
            
        });
                     
        
    };       
    
    //============================================   
     // dropdowns for nav 
