/*! sites/all/modules/custom/ex_skybox/js/skybox.js */
(function(Drupal,window,$){Drupal.behaviors.topStoriesNav={attach:function(context){$(context).find(this.container).once('top-stories-nav',$.proxy(this,'init'));},container:'.skybox-container',display_area:'.ts-display',story_item:'li',nav:'.ts-tabs',nav_item:'li',headline:'.ts-headline',auto_timeout:null,auto_advancing:null,nav_item_status:function(elem,status){if({'normal':1,'selected':1}[status]){$(elem).toggleClass('selected',status==='selected');}},nav_item_selected:function(elem){this.nav_item_status($(this.nav_item+'.selected',this.nav),'normal');this.nav_item_status(elem,'selected');},nav_item_over:function(elem){this.nav_item_status(elem,'hover');},nav_item_out:function(elem){this.nav_item_status(elem,$(elem).hasClass('selected')?'selected':'normal');},nav_item_click:function(elem){if(!$(elem).hasClass('selected')){this.nav_item_selected(elem);var tmp=$(elem).attr('id').split('-');this.story_selected($('#top-'+tmp[1]+'-'+tmp[2],this.display_area));}
return false;},story_item_status:function(elem,status){if({'normal':1,'selected':1}[status]){$(elem).toggleClass('selected',status==='selected');}},story_selected:function(elem){this.story_item_status($(this.story_item+'.selected',this.display_area),'normal');this.story_item_status(elem,'selected');},autoadvance:function(){var that=this,items=$(that.nav).children(that.nav_item),auto_index=items.index(items.filter('.selected:first')[0]),size=items.length;if(size>1){auto_index+=1;if(auto_index>=size){auto_index=0;}
that.nav_item_click(items.get(auto_index));that.auto_timeout=window.setTimeout(function(){that.autoadvance();},Drupal.settings.topstories_autoadvance*1000);}},clear_autoadvance:function(temporary){if(this.auto_timeout){window.clearTimeout(this.auto_timeout);}
if(!temporary){this.auto_advancing=false;}},init:function(){this.clear_autoadvance();this.nav_item_selected($(this.nav_item+':first',this.nav));var that=this,autoadvance=Drupal.settings&&Drupal.settings.topstories_autoadvance;$(this.nav).children(this.nav_item).each(function(){$(this).hover(that.nav_item_over,that.nav_item_out);$(this).bind('click',function(e){that.clear_autoadvance();return that.nav_item_click(this);});$(this).find('.tbs-headline').bind('click',function(e){that.clear_autoadvance();return that.nav_item_click($(this).parents(that.nav_item+':first'));});});if(autoadvance){this.auto_advancing=true;this.auto_timeout=window.setTimeout(function(){that.autoadvance();},autoadvance*1000);$(this.container).bind('mouseenter',function(){if(that.auto_advancing){that.clear_autoadvance(true);}}).bind('mouseleave',function(){if(that.auto_advancing){that.clear_autoadvance(true);that.auto_timeout=window.setTimeout(function(){that.autoadvance();},autoadvance*1000);}});}}};Drupal.behaviors.gearToggle={attach:function(context,settings){$('.skybox-container').once('gear-toggle',function(){var container=$(this),gear=container.find('.gear').hide();container.hover(function(){gear.show();},function(){gear.hide();});});}};}(Drupal,this,jQuery));;
