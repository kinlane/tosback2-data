(function($){$.fn.touchwipe=function(settings){var config={min_move_x:20,min_move_y:20,wipeLeft:null,wipeRight:null,wipeUp:null,wipeDown:null,preventDefaultEvents:true},moveEvent='touchmove',startEvent='touchstart';if(settings){$.extend(config,settings);}
function handleWipe(target,fn,amount,event){if(fn){if(config.preventDefaultEvents){event.preventDefault();}
fn.call(this,amount,event);}}
