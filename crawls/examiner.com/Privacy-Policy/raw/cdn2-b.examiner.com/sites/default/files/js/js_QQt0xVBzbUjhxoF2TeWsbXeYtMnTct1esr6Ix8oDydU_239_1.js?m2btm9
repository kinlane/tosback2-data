function onTouchMove(e){var x,y,dx,dy,ax,ay;if(isMoving){x=e.touches[0].pageX;y=e.touches[0].pageY;dx=startX-x;dy=startY-y;ax=Math.abs(dx);ay=Math.abs(dy);if(ax>=config.min_move_x){cancelTouch();handleWipe(this,dx>0?config.wipeLeft:config.wipeRight,ax,e);}else if(ay>=config.min_move_y){cancelTouch();handleWipe(this,dy>0?config.wipeDown:config.wipeUp,ay,e);}}}
function onTouchStart(e){if(e.touches.length===1){startX=e.touches[0].pageX;startY=e.touches[0].pageY;isMoving=true;this.addEventListener(moveEvent,onTouchMove,false);}}
if(("on"+startEvent)in document.documentElement){this.addEventListener(startEvent,onTouchStart,false);}});return this;};var t;if($.tools){t=$.tools.scrollable;t.plugins=t.plugins||{};t.plugins.wipescroll={version:'0.0.1',conf:{api:false,speed:50}};$.fn.wipescroll=function(conf){var globals=$.extend({},t.plugins.wipescroll.conf),ret;if(typeof conf==='number'){conf={speed:conf};}
conf=$.extend(globals,conf);this.each(function(){var api=$(this).scrollable(),scrollableConf=api.getConf();function next(result){api.move(1,conf.speed||50);}
function prev(result){api.move(-1,conf.speed||50);}
