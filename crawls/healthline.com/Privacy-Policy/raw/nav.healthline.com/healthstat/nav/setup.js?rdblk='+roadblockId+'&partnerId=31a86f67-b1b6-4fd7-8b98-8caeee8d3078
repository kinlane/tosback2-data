function hmn_okToLookAtNode(el) {
	if (el.nodeType != 1) { return true; }
	var nn = el.nodeName.toUpperCase();
	// 12: (.)
	// Rule 1: Positive@<h1> -> Skip
	if (nn == "H1") { return false;}
	// Rule 1: Positive@<h2> -> Skip
	if (nn == "H2") { return false;}
	// Rule 1: Positive@<h3> -> Skip
	if (nn == "H3") { return false;}
	// Rule 1: Positive@<h4> -> Skip
	if (nn == "H4") { return false;}
	// Rule 1: Positive@<img> -> Skip
	if (nn == "IMG") { return false;}
	// Rule 1: Positive@<noscript> -> Skip
	if (nn == "NOSCRIPT") { return false;}
	// Rule 1: Positive@<script> -> Skip
	if (nn == "SCRIPT") { return false;}
	// Rule 1: Positive@<form> -> Skip
	if (nn == "FORM") { return false;}
	// Rule 1: Positive@<input> -> Skip
	if (nn == "INPUT") { return false;}
	// Rule 1: Positive@<select> -> Skip
	if (nn == "SELECT") { return false;}
	// Rule 1: Positive@<hr> -> Skip
	if (nn == "HR") { return false;}
	// Rule 2: Positive@<a class="hllink"> -> RestrictToPositive
	if (nn == "A") { if (el.className.match(/\bhllink\b/)) {return true;}}
	// Rule 3: Positive@<a> -> Skip
	if (nn == "A") { return false;}
	// Rule 5: Ambiguous@<div class="hl-navLink"> -> Skip
	if (nn == "DIV") { if (el.className.match(/\bhl-navLink\b/)) {return false;}}
	// Rule 16: Ambiguous@<div id="centerWell"> -> Traverse
	if (nn == "DIV") { if (el.id == "centerWell") {return true;}}
	return true;
};



