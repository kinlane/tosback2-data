function isGN(){var gnCookieVal=gn_Cookie.get();if(gnCookieVal==null)return false;if(gnCookieVal!=null&&(document.location.href.indexOf("searchcompare")>0||document.location.href.indexOf("-GN0")>0||document.location.href.indexOf("-GN1")>0||document.location.href.indexOf("-GN2")>0||document.location.href.indexOf("-GN3")>0||document.location.href.indexOf("-SL2")>0||document.location.href.indexOf("-SL1")>0||document.location.href.indexOf("-SL0")>0||document.location.href.indexOf("-CL0")>0||document.location.href.indexOf("-CL1")>
0||document.location.href.indexOf("-SL3")>0||document.location.href.indexOf("-CL2")>0||document.location.href.indexOf("-CL3")>0||document.location.href.indexOf("-CLqv")>0||document.location.href.indexOf("-GNqv")>0||document.location.href.indexOf("-SR")>0))return true;else return false}
