var patt = /@>>[^<]+<<@/g ;
var megaTest = false;
function addBanners(genders) {
	$(genders).each(function(i,v) {
		var htmlVar = v.toLowerCase()+'Html';
		var bannerVar = v.toLowerCase()+'Banner';
		switch(v){
		case 'Juniors':
		  v = 'Womens';
		  break;
		case 'Sale':
		  v = '_';	
		  break;
		case 'Bag':
  		  v = '_';	
		  break;
		case 'Brand':
		  v = '_';	
		  break;
		}
		if (window[bannerVar]) {
			megaTest = true;
			window[htmlVar] += window[bannerVar].replace(patt, v);
		} else {
			window[htmlVar] += megaBannerHtml.replace(patt, v);
		}
	});
}

$(document).ready(function() {
	addBanners(genders);
	$('#topnav-mega #women').html(womensHtml);
	$('#topnav-mega #men').html(mensHtml);
	$('#topnav-mega #boys').html(boysHtml);
	$('#topnav-mega #girls').html(girlsHtml);
	$('#topnav-mega #juniors').html(juniorsHtml);
	$('#topnav-mega #bags').html(bagHtml);
	$('#topnav-mega #brands').html(brandHtml);
	$('#topnav-mega #sale').html(saleHtml);
});


$(document).ready(function() {
	
	function megaHoverOver(){
		$(this).find(".sub").prev('a').addClass('overed');	
		$(this).find(".sub").stop().fadeTo('fast', 1).show();

	}
	
	function megaHoverOut(){ 
	  
	  $(this).find(".sub").prev('a').removeClass('overed');
	  $(this).find(".sub").stop().fadeTo('fast', 0, function() {
		  $(this).hide();
	  });
	}
	
	var config = {    
		 sensitivity: 2, // number = sensitivity threshold (must be 1 or higher)    
		 interval: 100, // number = milliseconds for onMouseOver polling interval    
		 over: megaHoverOver, // function = onMouseOver callback (REQUIRED)    
		 timeout: 200, // number = milliseconds delay before onMouseOut    
		 out: megaHoverOut // function = onMouseOut callback (REQUIRED)    
	};

	$("ul#topnav-mega li .sub").css({'opacity':'0'});
	$("ul#topnav-mega li").hoverIntent(config);

});