var sLoginHref;var QuickLogin={showLoginPopup:function(){$("#loginnavwrapper").attr("style","visibility: visible;");$("#HPLogIn .connector").attr("style","visibility: visible;");$("#loginnav").addClass("selected")},hideLoginPopup:function(){$("#loginnavwrapper").attr("style","visibility: hidden;");$("#HPLogIn .connector").attr("style","visibility: hidden;");$("#loginnav").removeClass("selected")},hideEmailWater:function(a){document.getElementById(a).className=document.getElementById(a).className.replace("EmailWater","HideWater")},hidePasswordWater:function(a){document.getElementById(a).className=document.getElementById(a).className.replace("PassWater","HideWater")},showEmailWater:function(a){if(document.getElementById(a).value.length==0){document.getElementById(a).className=document.getElementById(a).className.replace("HideWater","EmailWater");if(document.getElementById(a).className.indexOf("EmailWater")==-1){document.getElementById(a).className=document.getElementById(a).className+"EmailWater"}}},showPasswordWater:function(a){if(document.getElementById(a).value.length==0){document.getElementById(a).className=document.getElementById(a).className.replace("HideWater","PassWater");if(document.getElementById(a).className.indexOf("PassWater")==-1){document.getElementById(a).className=document.getElementById(a).className+"PassWater"}}},handleNavLoginResponse:function(e,d){var a,b,c,f;CB.e("navAuthorizing").style.display="none";if(e){if(d=="SITEDOWN"){CB.e("navSiteDownDiv").style.display="block"}else{if(d=="CQADD"||d=="CQASK"){window.location.replace(sLoginHref)}else{if(d=="FAILURE"){CB.e("navLoginFailureDiv").style.display="block"}else{if(location.pathname.toLowerCase().indexOf("logout.aspx")==-1){location.reload(true)}if(typeof(CB.AJAX.Login)!=="undefined"){CB.AJAX.Login.onLoginSuccess();if(typeof(CB.AJAX.Login.Prefix)!=="undefined"){CB.e(CB.AJAX.Login.Prefix+"hdn_auth").value="True"}}renderLoginSucess(d.split("SUCCESS;")[1])}}}}else{CB.e("navSiteDownDiv").style.display="block"}},attemptLogin:function(b){var a="UserEmail="+CB.e(b+"loginEmail").value;a+="&UserPassword="+CB.e("navLoginPW").value;if(CB.e("loginnav_rememberme").checked===true){a+="&RememberUser=True"}else{a+="&RememberUser=False"}if(CB.e(b+"loginEmail").value.length>0&&CB.e("navLoginPW").value.length>0){CB.AJAX.submitCallback(a,"Login.aspx",QuickLogin.handleNavLoginResponse,true);CB.e("navLoginPW").value="";CB.e("navLoginPW").className="PassWater";CB.e("navLoginFailureDiv").style.display="none";CB.e("navSiteDownDiv").style.display="none";CB.e("navAuthorizing").style.display="block"}else{CB.e("navSiteDownDiv").style.display="none";CB.e("navAuthorizing").style.display="none";CB.e("navLoginFailureDiv").style.display="block"}},initQuickLogin:function(){sLoginHref=jQuery("#loginnav a").attr("href");if(location.pathname.toLowerCase().indexOf("login.aspx")==-1&&location.pathname.toLowerCase().indexOf("logout.aspx")==-1){jQuery("#loginnav a").attr("href","#");jQuery("#loginnav a").click(function(){if(jQuery("#loginnav").hasClass("selected")){QuickLogin.hideLoginPopup()}else{QuickLogin.showLoginPopup()}});jQuery("#loginnav .close").click(function(){QuickLogin.hideLoginPopup()})}}};jQuery(document).ready(function(){QuickLogin.initQuickLogin()});if(_pageBeginTime!=undefined){jQuery(window).load(function(){var b=new Date();var a=b.getTime()-_pageBeginTime.getTime();var c="client side page load";if(ScriptVariables.Contains("ClientSideTimerTask")){c=ScriptVariables.Get("ClientSideTimerTask")}if(a>=0&&a<=120000&&jQuery.cb!=null){jQuery.cb.Timing(_pageName,"window.onload",c,a)}});jQuery(document).ready(function(){var b=new Date();var a=b.getTime()-_pageBeginTime.getTime();var c="client side page load";if(ScriptVariables.Contains("ClientSideTimerTask")){c=ScriptVariables.Get("ClientSideTimerTask")}if(a>=0&&a<=120000&&jQuery.cb!=null){jQuery.cb.Timing(_pageName,"document.ready",c,a)}})};