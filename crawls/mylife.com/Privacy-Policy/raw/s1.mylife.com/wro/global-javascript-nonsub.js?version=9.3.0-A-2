            }
            else {
            	var countLike = 0;
            	
            	if (typeof(postData.likes.count) != "undefined") {
            		countLike = postData.likes.count;
            	} else {
            		countLike = postData.likes.data.length;
            	}
            	
                $('#dataContainer .numLikes').html(countLike);
                //now determined if the user has liked the post themselves
                var selfLiked = 'no';
