function printLifeEvents(json,obj) {

	obj = eval('(' + json + ')');

	var header;
	var ul;

	var li;
	var anchor;
	var defaultTitleText;
	var divId;
	var spanArrow;

	$.each(obj,function(n,d) {
		divId = "";
		ul = document.createElement("ul");
		$(ul).attr("class","life-event-li");

		$.each(d,function(n2,d2) {

			li = document.createElement("li");
