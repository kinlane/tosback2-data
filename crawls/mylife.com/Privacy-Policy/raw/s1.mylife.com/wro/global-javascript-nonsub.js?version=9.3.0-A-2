function processJsonQms(obj, jsonObj) {
	obj.totalCount = parseInt(jsonObj.totalCount);
	obj.newCount = jsonObj.newMemberCount;
	obj.viewerZipCode = jsonObj.viewerZip;
	obj.viewerSubscribed = jsonObj.viewerSubscribed;
