function parseUrl(data) {
	var link = data.replace(/([A-Za-z]+:\/\/[A-Za-z0-9-_]+\.[A-Za-z0-9-_:%&~\?\/.=]+)/gi, '<a href="$1" target="_blank">$1</a></span>');
	return link;
}
function parseUrl2(data) {
	var link = data.replace(/<a href=/gi, '<a target="_blank" href=');
	return link;
}
