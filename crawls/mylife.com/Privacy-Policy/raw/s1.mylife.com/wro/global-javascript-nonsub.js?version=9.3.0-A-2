        that.attr('readOnly',true);
        that.attr('disabled',true);

        $.post('/json/social/comment/'+ $('#dataContainer .type').html() +'/' + $('#dataContainer .id').html(),{userMessage: message}, function(response) {
            if(response.success == "1")
            {
                $.getJSON('/json/social/feeds/facebook/' + $('#dataContainer .id').html(), function(postData) {
                    $(that).blur();
                    refreshModalComments(postData.comments);
                    that.removeAttr('readOnly');
                    that.removeAttr('disabled');
                    $('#newCommentData, #newComment .clear').remove();
                    $('#newComment').append('<textarea id="newCommentData">Comment posted successfully!</textarea><div class="clear"></div>');
                });
