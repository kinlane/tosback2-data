	filter = /^([a-zA-Z0-9_\.\-])+\@((yahoo|ymail|rocketmail)+\.)+([a-zA-Z0-9.]{2,4})+$/;
	if (filter.test(email)) {
		provider = "yahoo";
		providerText = "Yahoo";
	}

	filter = /^([a-zA-Z0-9_\.\-])+\@((aol|about)+\.)+([a-zA-Z0-9.]{2,4})+$/;
