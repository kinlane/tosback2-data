				if (messageText.search("Error validation access token")	> 0 || messageText.search("Unable to parse response") > 0) {
					providerTextVal = $("#providers").val();
					providerArr = providerTextVal.split(",");

					for(var iL = 0; iL < providerArr.length; iL++) {
