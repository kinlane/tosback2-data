            $("#" + carouselId + "ViewAllTextLink").addClass("viewAllResultsLinkFree");
            $("#" + carouselId + "ViewAllTextLink").html("Upgrade now to see names, photos, and more!");
            $("#" + carouselId + "ViewAllText").show();
