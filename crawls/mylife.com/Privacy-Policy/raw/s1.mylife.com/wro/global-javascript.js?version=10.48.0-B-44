function AjaxRequest(url,queryParam,reqType,successFxn)
{
    this.url=url;
    this.queryData=queryParam;
    this.reqType=reqType;
    this.onSuccessExecute=successFxn;
    this.Execute=function(){

        $.ajax({
            type: this.reqType,
            url: this.url,
            data: this.queryData,
            success:function(data){
                eval(successFxn + '('+data+')')
            }
        });

    }
}
