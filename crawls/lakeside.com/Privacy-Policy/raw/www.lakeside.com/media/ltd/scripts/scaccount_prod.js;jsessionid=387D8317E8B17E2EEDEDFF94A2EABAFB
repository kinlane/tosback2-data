/*
--------------------------------------------------
LTD Commodities script to determine
Site Catalyst Account
--------------------------------------------------
*/

function GetSiteCatalystAccount(siteId)
{

    if(	siteId == 'LTD')
    {
         //return "ltdcomltd2dev"; //test
         return "ltddev"; //production
    }
    else if (siteId == 'ABC')
    {
        //return "ltdcomabc2dev"; //test
        return "ltdcomabcdev"; //production


    }
    else if (siteId == 'LS')
    {
        //return "ltdcomlake2dev"; //test
        return "ltdcomlakedev"; //production


    }
    else
    {
         //return "ltdcomltd2dev"; //test
         return "ltddev"; //production


    }

}