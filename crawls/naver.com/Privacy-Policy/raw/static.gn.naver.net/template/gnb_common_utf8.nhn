		document.getElementById('gnb_nicknm').style.display = "inline-block";
		
		var isGroupId = ((typeof data.loginGroupId != "undefined") && (0 < data.loginGroupId.length))
		if(isGroupId){
			gnbNaverMeLayer.isGroupId = true;
		}
	} else {
		loginUrl = "https://nid.naver.com/nidlogin.login";
		document.getElementById('gnb_nicknm').style.display = "none";
	}

	try {
		if (gnb_searchbox == "on") {
			document.getElementById('gnb_naverme_layer').className = "me_layer search_on_me_layer";
			document.getElementById('gnb_searchbox').style.display = "inline-block";
		} else {
			document.getElementById('gnb_searchbox').style.display = "none";
		}
	} catch (e) {
		document.getElementById('gnb_searchbox').style.display = "none";
	}

	document.getElementById('gnb_btn_login').style.display = "inline-block";
	document.getElementById('gnb_utility').style.zIndex = 104;
	document.getElementById('gnb_login').onclick = loginNClicksEvent;
	setLoginUrl(loginUrl, (data.loginStatus == 'Y'));
	
	registerGNBLayerEventAll();
	gnbNClicks.installNClicks();
	gnbNClicks.registerNClicksEvent('gnb');
}

function registerGNBLayerEventAll() {
	registerGNBLayerEvent('more', gnb_more_layer);
	registerGNBLayerEvent('gnb_nicknm', gnb_user_layer);

	document.getElementById('gnb_login').onfocus = function() {
		gnb_user_layer.hide_top();
	};
	document.getElementById('gnb_view_all').onblur = function() {
		gnb_more_layer.hide_top();
	};
}

function registerGNBLayerEvent(layerId, layerEventObj) {
	var moreLayer = document.getElementById(layerId);

	moreLayer.onmouseover = function() {
		layerEventObj.show_top();
		gnbNaverMeLayer.hideLayer();
	};

	moreLayer.onmouseout = function() {
		layerEventObj.hide_top();
	};

	moreLayer.onclick = function() {
		layerEventObj.clickToggle();
		return false;
	};

	moreLayer.onfocus = function() {
		layerEventObj.showelement();
		gnbNaverMeLayer.hideLayer();
	};

	moreLayer.onkeydown = function(e) {
		var event = e ? e : window.event;
		var TAB_KEY_CODE = 9;
		if(event.keyCode == TAB_KEY_CODE && event.shiftKey) {
			layerEventObj.hide_top();
		}
	};
}

function showMeCount(totalCount) {
	var gnb_mecountplus = "";
	if (totalCount > 99) {
		gnb_mecountplus = "<span>+</span>";
		totalCount = 99;
	}
	
	if(totalCount == 0) {
		document.getElementById('gnb_me_menu').className="me_menu zero";
		document.getElementById('gnb_lnk_me').className="lnk_me zero";
	} else {
		document.getElementById('gnb_me_menu').className="me_menu";
		document.getElementById('gnb_lnk_me').className="lnk_me";
	}
		
	document.getElementById('gnb_lnk_me').innerHTML = "<em id='gnb_bubble'>" + totalCount + gnb_mecountplus + "</em>";
	document.getElementById('gnb_lnk_me').style.display = "inline-block";
}

function setLoginUrl(loginUrl, isLogin) {
	var loginAndReturnUrl = "";
	var isGnbLoginDefined = (typeof gnb_login == "string") && (gnb_login != "");
	var isGnbLogoutDefined = (typeof gnb_logout == "string") && (gnb_logout != "");

	try {
		if (isLogin && isGnbLogoutDefined) {
			loginAndReturnUrl = loginUrl + "?returl=" + gnb_logout;
		} else if (!isLogin && isGnbLoginDefined) {
			loginAndReturnUrl = loginUrl + "?url=" + gnb_login;
		} else {
			loginAndReturnUrl = loginUrl;
		}
	} catch(e) {
		loginAndReturnUrl = loginUrl;
	}

	gnb_more_layer._gnb_login = loginAndReturnUrl;
	document.getElementById('gnb_login').href = gnb_more_layer._gnb_login;
}

function getGNB() {
	var gnb_svcname;
	var gnb_tmplname;
	try{ gnb_svcname=gnb_service;} catch(e) {}
	try{ gnb_tmplname=gnb_template;} catch(e) {}
	var gnb_url = "http://gn.naver.com/getLoginStatus.nhn?callback=showGNB&charset=utf8&svc="+gnb_svcname+"&template="+gnb_tmplname;
	
	gnbJSONP.read('gnb_user_data', gnb_url);
}

function shortNick(fm){
	var nickNameNode = document.getElementById(fm).childNodes[0].childNodes[0];
	var nickName = nickNameNode.data;

	if (nickName.length > 10) {
		var shortened = nickName.substr(0, 10) + "...님";
		nickNameNode.data = shortened;
	}
}
