/*
 * Facebox (for jQuery)
 * version: 1.2 (05/05/2008)
 * @requires jQuery v1.2 or later
 *
 * Examples at http://famspam.com/facebox/
 *
 * Licensed under the MIT:
 *   http://www.opensource.org/licenses/mit-license.php
 *
 * Copyright 2007, 2008 Chris Wanstrath [ chris@ozmm.org ]
 *
 * Usage:
 *
 *  jQuery(document).ready(function() {
 *    jQuery('a[rel*=facebox]').facebox()
 *  })
 *
 *  <a href="#terms" rel="facebox">Terms</a>
 *    Loads the #terms div in the box
 *
 *  <a href="terms.html" rel="facebox">Terms</a>
 *    Loads the terms.html page in the box
 *
 *  <a href="terms.png" rel="facebox">Terms</a>
 *    Loads the terms.png image in the box
 *
 *
 *  You can also use it programmatically:
 *
 *    jQuery.facebox('some html')
 *    jQuery.facebox('some html', 'my-groovy-style')
 *
 *  The above will open a facebox with "some html" as the content.
 *
 *    jQuery.facebox(function($) {
 *      $.get('blah.html', function(data) { $.facebox(data) })
 *    })
 *
 *  The above will show a loading screen before the passed function is called,
 *  allowing for a better ajaxy experience.
 *
 *  The facebox function can also display an ajax page, an image, or the contents of a div:
 *
 *    jQuery.facebox({ ajax: 'remote.html' })
 *    jQuery.facebox({ ajax: 'remote.html' }, 'my-groovy-style')
 *    jQuery.facebox({ image: 'stairs.jpg' })
 *    jQuery.facebox({ image: 'stairs.jpg' }, 'my-groovy-style')
 *    jQuery.facebox({ div: '#box' })
 *    jQuery.facebox({ div: '#box' }, 'my-groovy-style')
 *
 *  Want to close the facebox?  Trigger the 'close.facebox' document event:
 *
 *    jQuery(document).trigger('close.facebox')
 *
 *  Facebox also has a bunch of other hooks:
 *
 *    loading.facebox
 *    beforeReveal.facebox
 *    reveal.facebox (aliased as 'afterReveal.facebox')
 *    init.facebox
 *    afterClose.facebox
 *
 *  Simply bind a function to any of these hooks:
 *
 *   $(document).bind('reveal.facebox', function() { ...stuff to do after the facebox and contents are revealed... })
 *
 */
(function($) {
  $.facebox = function(data, klass) {
    $.facebox.loading()

    if (data.ajax) fillFaceboxFromAjax(data.ajax, klass)
    else if (data.image) fillFaceboxFromImage(data.image, klass)
    else if (data.div) fillFaceboxFromHref(data.div, klass)
    else if ($.isFunction(data)) data.call($)
    else $.facebox.reveal(data, klass)
  }

  /*
   * Public, $.facebox methods
   */

  $.extend($.facebox, {
    settings: {
      opacity      : 0.5,
      overlay      : true,
      loadingImage : '/images/icons/loading.gif',
      closeImage   : '/images/buttons/close-pop-up.png',
      imageTypes   : [ 'png', 'jpg', 'jpeg', 'gif' ],
      faceboxHtml  : '\
    <div id="facebox" style="display:none;"> \
      <div class="popup"> \
        <div class="content"> \
        </div> \
        <a href="#" class="close"><img src="/images/buttons/close-pop-up.png" title="" alt="Close" class="close_image" /></a> \
      </div> \
    </div>'
    },

    loading: function() {
      init()
      if ($('#facebox .loading').length == 1) return true
      showOverlay()

      $('#facebox .content').empty();
      $('#facebox .body').children().hide().end().
        append('<div class="loading"><img src="'+$.facebox.settings.loadingImage+'"/></div>')

      $('#facebox').show()

      $(document).bind('keydown.facebox', function(e) {
        if (e.keyCode == 27) $.facebox.close()
        return true
      })
      $(document).trigger('loading.facebox')
    },

    reveal: function(data, klass) {
      $(document).trigger('beforeReveal.facebox')
      if (klass) $('#facebox .content').addClass(klass)
      $('#facebox .content').append(data)
      $('#facebox .loading').remove()
      $('#facebox .body').children().fadeIn('normal')
	  $('#facebox').css({
		'top' : Math.floor(($(window).height() / 2) - ($("#facebox").height() / 2)) + $(document).scrollTop() + "px",
		'left' : Math.floor(($(window).width() / 2) - ($("#facebox").width() / 2)) + "px"
      });
      $(document).trigger('reveal.facebox').trigger('afterReveal.facebox')
    },

    close: function() {
      $(document).trigger('close.facebox')
      return false
    }
  })

  /*
   * Public, $.fn methods
   */

  $.fn.facebox = function(settings) {
    if ($(this).length == 0) return

    init(settings);

    function clickHandler() {
      $.facebox.loading(true)

      // support for rel="facebox.inline_popup" syntax, to add a class
      // also supports deprecated "facebox[.inline_popup]" syntax
      var klass = this.rel.match(/facebox\[?\.(\w+)\]?/)
      if (klass) klass = klass[1]

      fillFaceboxFromHref(this.href, klass)
      return false
    }

    return this.on('click.facebox', clickHandler)
  }

  /*
   * Private methods
   */

  // called one time to setup facebox on this page
  function init(settings) {
    if ($.facebox.settings.inited) return true
    else $.facebox.settings.inited = true

    $(document).trigger('init.facebox')
    makeCompatible()

    var imageTypes = $.facebox.settings.imageTypes.join('|')
    $.facebox.settings.imageTypesRegexp = new RegExp('\.(' + imageTypes + ')$', 'i')

    if (settings) $.extend($.facebox.settings, settings)
    $('body').append($.facebox.settings.faceboxHtml)

    var preload = [ new Image(), new Image() ]
    preload[0].src = $.facebox.settings.closeImage
    preload[1].src = $.facebox.settings.loadingImage

    $('#facebox').find('.b:first, .bl').each(function() {
      preload.push(new Image())
      preload.slice(-1).src = $(this).css('background-image').replace(/url\((.+)\)/, '$1')
    })

    $('#facebox .close').click($.facebox.close)
    $('#facebox .close_image').attr('src', $.facebox.settings.closeImage)
  }

  // getPageScroll() by quirksmode.com
  function getPageScroll() {
    var xScroll, yScroll;
    if (self.pageYOffset) {
      yScroll = self.pageYOffset;
      xScroll = self.pageXOffset;
    } else if (document.documentElement && document.documentElement.scrollTop) {	 // Explorer 6 Strict
      yScroll = document.documentElement.scrollTop;
      xScroll = document.documentElement.scrollLeft;
    } else if (document.body) {// all other Explorers
      yScroll = document.body.scrollTop;
      xScroll = document.body.scrollLeft;
    }
    return new Array(xScroll,yScroll)
  }

  // Adapted from getPageSize() by quirksmode.com
  function getPageHeight() {
    var windowHeight
    if (self.innerHeight) {	// all except Explorer
      windowHeight = self.innerHeight;
    } else if (document.documentElement && document.documentElement.clientHeight) { // Explorer 6 Strict Mode
      windowHeight = document.documentElement.clientHeight;
    } else if (document.body) { // other Explorers
      windowHeight = document.body.clientHeight;
    }
    return windowHeight
  }

  // Backwards compatibility
  function makeCompatible() {
    var $s = $.facebox.settings

    $s.loadingImage = $s.loading_image || $s.loadingImage
    $s.closeImage = $s.close_image || $s.closeImage
    $s.imageTypes = $s.image_types || $s.imageTypes
    $s.faceboxHtml = $s.facebox_html || $s.faceboxHtml
  }

  // Figures out what you want to display and displays it
  // formats are:
  //     div: #id
  //   image: blah.extension
  //    ajax: anything else
  function fillFaceboxFromHref(href, klass) {
    // div
    if (href.match(/#/)) {
      var url    = window.location.href.split('#')[0]
      var target = href.replace(url,'')
      if (target == '#') return
      $.facebox.reveal($(target).html(), klass)

    // image
    } else if (href.match($.facebox.settings.imageTypesRegexp)) {
      fillFaceboxFromImage(href, klass)
    // ajax
    } else {
      fillFaceboxFromAjax(href, klass)
    }
  }

  function fillFaceboxFromImage(href, klass) {
    var image = new Image()
    image.onload = function() {
      $.facebox.reveal('<div class="image"><img src="' + image.src + '" /></div>', klass)
    }

    image.src = href
  }

  function fillFaceboxFromAjax(href, klass) {
    $.get(href, function(data) { $.facebox.reveal(data, klass) }, "html")
  }

  function skipOverlay() {
    return $.facebox.settings.overlay == false || $.facebox.settings.opacity === null
  }

  function showOverlay() {
    if (skipOverlay()) return

    if ($('#facebox_overlay').length == 0)
      $("body").append('<div id="facebox_overlay" class="facebox_hide"></div>')

    $('#facebox_overlay').hide().addClass("facebox_overlayBG")
      .css('opacity', $.facebox.settings.opacity)
      .click(function() { $(document).trigger('close.facebox') })
      .fadeIn(200)
    return false
  }

  function hideOverlay() {
    if (skipOverlay()) return

    $('#facebox_overlay').fadeOut(200, function(){
      $("#facebox_overlay").removeClass("facebox_overlayBG")
      $("#facebox_overlay").addClass("facebox_hide")
      $("#facebox_overlay").remove()
    })

    return false
  }

  /*
   * Bindings
   */

  $(document).bind('close.facebox', function() {
    $(document).unbind('keydown.facebox')
    $('#facebox').fadeOut(function() {
      $('#facebox .content').removeClass().addClass('content')
      $('#facebox .loading').remove()
      $(document).trigger('afterClose.facebox')
    })
    hideOverlay();
	closeAllDialogs();
  });
  
	$(window).resize(function(){
		$('#facebox').css({
			//'top' : Math.floor(($(window).height() / 2) - ($("#facebox").height() / 2)) + $(document).scrollTop() + "px",
			'left' : Math.floor(($(window).width() / 2) - ($("#facebox").width() / 2)) + "px"
 		 });
	});

})(jQuery);

var richMediaInit = false;

/* * Rich Media Function Library * */
(function($) {
    $.fn.RichMediaFour = function(options) {
        var opts = $.extend( {}, $.fn.RichMediaFour.defaults, options);
    };

    $.fn.RichMediaFour.initialize = function() {
		
		if(!richMediaInit) {
			var rSku = $("#rSku").html(), 
			rParam = $("#rm-audio-data").html(), 
			sliderPos = 0;
			
			// Attach rich media template functions
			function attachShadow() {
				if ($("#shadow").length < 1 && !$("#second_level_category_popup_underlay").is(":visible")) {
					$("body").prepend("<div id='shadow'/>");
					$("#shadow").css( {
						display : "block"
					});
				}
			}
	
			$("body").on("click", "#shadow", function() {
				closeRichMediaDialogs();
			});
	
			$("#rich-media, #rich-media-quick").on("click", ".close", function() {
				closeRichMediaDialogs();
			});
	
			// Show video banner animation if there is a video
	
			if ($("#main-image").hasClass("has-video")) {
				var vb = $('<div id="video-available"><a rel="nofollow" onclick="javascript:void(0); return false" href="/product-page/rich-media/video.htm?cm_sp=Rich%20Media-_-Product%20Page-_-Video">&nbsp;</a></div>');
				$("#main-image").prepend(vb);
				
				vb.fadeIn("slow").delay(5000).fadeOut().click( function() {
					$("#video-link a").trigger('click');
				});
				$("#main-image").hover( function() {
					vb.fadeIn("slow");
				}, function() {
					vb.fadeOut("slow");
				});
				
			}
			
			// Build thumbnail slider if more than 8 images
			var panels = Math.ceil($(".product-thumbnails:eq(0) .thumbnail").length / 8);
	
			if (panels > 1) {
				var newLeft = "";
	
				$(".arrow").css("visibility", "visible");
	
				$(".arrow-left").click( function() {
					if (sliderPos > 0) {
						sliderPos--;
						newLeft = ((sliderPos * 400) * -1) + "px";
						$(".thumbnail-container").css("left", newLeft);
					}
					showHideArrows();
				});
	
				$(".arrow-right").click( function() {
					if (sliderPos < (panels - 1)) {
						sliderPos++;
						newLeft = ((sliderPos * 400) * -1) + "px";
						$(".thumbnail-container").css("left", newLeft);
					}
					showHideArrows();
				});
	
				function showHideArrows() {
					if (sliderPos == 0) {
						$(".arrow-left").css("visibility", "hidden");
						$(".arrow-right").css("visibility", "visible");
					}
					if (sliderPos > 0) {
						$(".arrow-left").css("visibility", "visible");
						if (panels > (sliderPos + 1)) {
							$(".arrow-right").css("visibility", "visible");
						} else {
							$(".arrow-right").css("visibility", "hidden");
						}
					}
				}
				showHideArrows();
			}
	
			// Regular photo thumbnail click/hover functions
			$(".thumbnail").on("click", function() {
					var thisIndex = $(this).index(),
					thumbImgSrc = $(this).find("img").attr("src"),
					mainImg = $(".product-image").find("img"),
					mainImgHt = mainImg.attr("height") + "px",
					contributor = $(this).find("img").attr("name"),
					caption = $(this).find("img").attr("longdesc");
			
					$(".product-thumbnails").find(".thumbnail-active").removeClass("thumbnail-active");
					$("#left-column .thumbnail").eq(thisIndex).addClass("thumbnail-active");
					$("#rich-media .thumbnail").eq(thisIndex).addClass("thumbnail-active");
					mainImg.parent().removeClass("customer-product-image");
			
					$("#left-column").find(".caption-holder").remove();
					$(".caption").remove();
					$("#rich-media").find(".caption-holder").hide();
			
					// Customer submitted photo thumbnail click functions
					if ($(this).hasClass("csi-thumbnail")) {
						mainImg.attr("src", thumbImgSrc.replace(".icon", "")).parent().addClass("customer-product-image");
						$("#rich-media").find(".caption-holder").show();
						$("#left-column").find(".caption-holder").show();
			
						if (caption !== '') {
							$("#left-column").find(".product-image img").after("<div class='caption-holder'><p class='caption'>Submitted by " + contributor + "&nbsp;&nbsp;-&nbsp;&nbsp;<a href='#' class='product-tool' id='csi-" + thisIndex + "'>See customer notes</a></p></div>");
						} else {
							$("#left-column").find(".product-image img").after("<div class='caption-holder'><p class='caption'>Submitted by " + contributor + "</p></div>");
						}
						$("#additional-images p:first").after("<p class='caption'>" + caption + "</p>");
						$("#additional-images").find(".caption-holder").show().end().find(".contributor").text(contributor);
					} else {
						mainImg.attr("src", thumbImgSrc.replace("_med", "_lg"));
					}
				}).hover( function() {
			$(this).addClass("thumbnail-hover");
			}, function() {
				$(this).removeClass("thumbnail-hover");
			});
	
			// Slice down title in rich media window if too long
			var prodTitle = $("#rich-media").find("h3").text();
			if (prodTitle.length > 90) {
				prodTitle = prodTitle.substring(0, 90);
				$("#rich-media").find("h3").text(prodTitle + "...");
			}
	
			// Swfobject functions
			function create(targetId, rSku, rParam) {
				// check if SWF hasn't been removed, if this is the case, create a new alternative content container
				function loaderAttach(m) {
					$("#" + m + "-container").css( {
						backgroundImage : "url(/images/colors/color1/loading.gif)",
						backgroundPosition : "50% 50%",
						backgroundRepeat : "no-repeat"
					});
				}
				var c = "#" + targetId;
				if (!$(c).length) {
					var d = $("<div></div>").attr("id", targetId);
					var p = "#" + targetId + "-container";
					$(d).appendTo(p);
				}
				var parameters360 = {
					id : "1",
					src : "http://www.northerntool.com/images/product/360/" + rSku + ".mp4",
					autoPlay : "true",
					loop : "true",
					width : "800",
					height : "485",
					controlBarAutoHide : "false",
					controlBarPosition : "bottom"
				};
				var parameters = {
					id : "1",
					src : "http://www.northerntool.com/images/product/videos/" + rSku + ".flv",
					autoPlay : "true",
					width : "800",
					height : "485",
					controlBarAutoHide : "false",
					controlBarPosition : "bottom"
				};
				var BSP360Param = {
					autoStart : "true",
					bgcolor : "#FFFFFF",
					playerID : "1937987104001",
					playerKey : "AQ~~,AAABrbZSuok~,H_WTaxB_AQ8J2XBmyEcJSymnwxbeY6BE",
					width : "800",
					height : "485"
				};
				// 720 x 405
				var BSPVidParam = {
					autoStart : "true",
					bgcolor : "#000000",
					playerID : "1934507357001",
					playerKey : "AQ~~,AAABrbZSuok~,H_WTaxB_AQ-hJ0sRHBAC586Z0nbWE0eZ",
					width : "800",
					height : "485"
				};
				// create 360 SWF
				if (targetId == "r360-player") {
					loaderAttach(targetId);
					brightcoveObject.embedBSP(targetId, "northerntool_360_" + rSku, BSP360Param);
					brightcove.createExperiences();
				}
				// create video SWF
				if (targetId == "video-player" || targetId == "video-player-quick") {
					loaderAttach(targetId);
					brightcoveObject.embedBSP(targetId, "northerntool_videos_" + rSku, BSPVidParam);
					brightcove.createExperiences();
				}
				// create audio SWF
				if (targetId == "audio-player") {
					loaderAttach(targetId);
					var att = {
						data : "/images/product/MP3Player.swf",
						width : "400",
						height : "400"
					}, par = {
						flashvars : "item=" + rSku + "&sounds=" + rParam,
						allowfullscreen : 'false',
						bgcolor : '#FFFFFF',
						wmode : 'transparent'
					}, id = targetId;
					swfobject.createSWF(att, par, id);
				}
			}
			
			function removeMedia() {
				swfobject.removeSWF("video-player");
				swfobject.removeSWF("video-player-quick");
				swfobject.removeSWF("r360-player");
				swfobject.removeSWF("r360-player-quick");
				swfobject.removeSWF("audio-player");
			}
	
			$("#rich-media").on("click", ".tab", function() {
				var thisId = this.id;
				
				var elemId = "video-player";
				$('#' + elemId).replaceWith('<div id="' + elemId + '"></div>');
				var elemId = "video-player-quick";
				$('#' + elemId).replaceWith('<div id="' + elemId + '"></div>');
				var elemId = "r360-player";
				$('#' + elemId).replaceWith('<div id="' + elemId + '"></div>');
				var elemId = "r360-player-quick";
				$('#' + elemId).replaceWith('<div id="' + elemId + '"></div>');
				
				switch (thisId) {
				case 'video-tab':
					create("video-player", rSku);
					break;
				case 'r360-tab':
					create("r360-player", rSku);
					break;
				case 'audio-tab':
					rParam = $("#rm-audio-data").html();
					create("audio-player", rSku, rParam);
					break;
				case 'zoom-tab':
					if ($("#zoom .main-image img").length == 0) {
						$("<img />").hide().attr( {
							src : '/images/product/zoom_images/' + rSku + '.jpg',
							width : '660',
							height : '400'
						}).appendTo('#zoom .main-image').fadeIn(400, function() {
							initializeZoom(this);
						});
					}
					break;
				case 'features-tab':
					if ($("#features img").length == 0) {
						$("#features").html('<img src="/images/product/features/' + rSku + '.jpg" />');
					}
					break;
				default:
				}
			});
	
			$(".video-link-quick").on("click", "img", function(e) {
				attachShadow();
				create("video-player-quick", $(this).parents(".video-link-quick").children(".rSku").html());
				centerMedia($("#rich-media-quick"));
				$("#rich-media-quick").show().css({
					'z-index': '999'
				});
				e.preventDefault();
			});
			
			$(window).resize(function() {
				centerMedia($("#rich-media-quick"));
			}); 
			
			function centerMedia(elem) {
				elem.css({
					'left': Math.max(0, Math.floor((($(window).width() - elem.outerWidth()) / 2) + $(window).scrollLeft())) + 'px',
					'top':  Math.max(0, Math.floor((($(window).height() - elem.outerHeight()) / 2) + $(window).scrollTop()))  + 'px'
				});
			}
	
			// Open rich media template functions
			$(".product-tool").click(function(e) {
				attachShadow();
				$("#rich-media").show();
				$("#rich-media").find(".tab, .tab-content").removeClass("open");
	
				var thisId = $(this).attr('id'),
					sliderNext = $(".product-thumbnails").find(".arrow-right"),
					sliderPrev = $(".product-thumbnails").find(".arrow-left");
	
				switch (thisId) {
				case 'zoom-link':
					$("#zoom-tab, #zoom").addClass("open");
					if ($("#zoom .main-image img").length == 0) {
						$("<img />").hide().attr( {
							src : '/images/product/zoom_images/' + rSku + '.jpg',
							width : '660',
							height : '400'
						}).appendTo('#zoom .main-image').fadeIn(400, function() {
							initializeZoom(this);
						});
					}
					break;
				case 'video-link':
					$("#video-tab, #video").addClass("open");
					create("video-player", rSku);
					break;
				case '360-link':
					$("#r360-tab, #r360").addClass("open");
					rParam = $("#r360-tab").attr("title");
					create("r360-player", rSku);
					break;
				case 'demo-link':
					$("#demo-tab, #demo").addClass("open");
					break;
				case 'audio-link':
					$("#audio-tab, #audio").addClass("open");
					rParam = $("#audio-tab").attr("title");
					create("audio-player", rSku, rParam);
					break;
				case 'features-link':
					$("#features-tab, #features").addClass("open");
					if ($("#features img").length == 0) {
						$("#features").html('<img src="/images/product/features/' + rSku + '.jpg" />');
					}
					break;
				case 'features-link-trigger':
					$("#features-tab, #features").addClass("open");
					if ($("#features img").length == 0) {
						$("#features").html('<img src="/images/product/features/' + rSku + '.jpg" />');
					}
					var section = $(this).attr('href').split("?");
					var sectionHash = '#' + section[0];
					smoothscroll(sectionHash);
					break;
				case 'csi-link':
					$("#images-tab, #additional-images").addClass("open");
					var firstCsi = $("#additional-images").find(".csi-thumbnail").eq(0);
					if (firstCsi.index() > 7) {
						sliderNext.trigger("click");
						if (firstCsi.index() > 15) {
							sliderNext.trigger("click");
							if (firstCsi.index() > 23) {
								sliderNext.trigger("click");
							}
						}
					}
					firstCsi.trigger("click");
					break;
				default:
					$("#images-tab, #additional-images").addClass("open");
					var thisCsiIndex = thisId.replace("csi-", "");
					var thisCsi = $("#additional-images").find(".thumbnail").eq(thisCsiIndex);
					if (thisCsi.index() > 7) {
						sliderNext.trigger("click");
						if (thisCsi.index() > 15) {
							sliderNext.trigger("click");
							if (thisCsi.index() > 23) {
								sliderNext.trigger("click");
							}
						}
					}
					thisCsi.trigger("click");
					break;
				}
				e.preventDefault();
			});
			
			$("#additional-images .expand").click( function() {
				if ($(this).hasClass('full-open')) {
					$(this).attr('src', $(this).attr('src').replace('collapse', 'expand'));
					$(this).removeClass('full-open');
				} else {
					$(this).attr('src', $(this).attr('src').replace('expand', 'collapse'));
					$(this).addClass('full-open');
				}
				$(this).parent().find(".full-details").toggle();
			});
		}
		
		richMediaInit = true;
    };

    $.fn.RichMediaFour.defaults = {};

})(jQuery);

var brightcoveObject = function() {
    return {
        // Add a Brightcove Smart Player object
        embedBSP : function(elemId, bspImageTag, attObj) {
            var callbackObj = {
                success : false,
                id : elemId
            };

            if (elemId && bspImageTag) {
                var bspHtml = '<object';
                bspHtml += ' id="' + elemId + '"';
                if (attObj) {
                    if (attObj.objClass) {
                        bspHtml += ' class="BrightcoveExperience ' + attObj.objClass + '"';
                    } else {
                        bspHtml += ' class="BrightcoveExperience"';
                    }

                    if (attObj.style) {
                        bspHtml += ' style="' + attObj.style + '"';
                    }
                } else {
                    bspHtml += ' class="BrightcoveExperience"';
                }
                bspHtml += '>';

				// See: http://support.brightcove.com/en/docs/player-configuration-parameters#supported

                // isVid - Boolean: Required to be true for all video players.
                bspHtml += '<param name="isVid" value="true" />';

				// dynamicStreaming - Boolean: If true (the default setting), the player can take advantage of Flash Media Server 3.5 and Flash Player 10 smooth streaming capabilities.
                bspHtml += '<param name="dynamicStreaming" value="true" />';

				// @videoPlayer - String: The ID or refid of a video to program into the player. See Assigning content to players programmatically.
                bspHtml += '<param name="@videoPlayer" value="' + bspImageTag + '" />';

                // Boolean: Allows the HTML5 player to be loaded on devices that support Flash but do not have Flash installed. See Allow HTML5 Players in Flash-Supported Environments for more details.
                bspHtml += '<param name="htmlFallback" value="true" />';

				// includeAPI - Boolean: Required to be true in order to use the Smart Player API with this player.
                bspHtml += '<param name="includeAPI" value="true" />';

				// isUI - Boolean: Required to be true for video players whose templates include UI elements other than the VideoPlayer component, such as a ChromelessVideoPlayer or VideoDisplay and MediaControls components.
                bspHtml += '<param name="isUI" value="true" />';

                // Boolean: If true, prevents a player that is published on a page that uses HTTPS from triggering security warnings. Note that this only prevents security warnings; it does not cause the player to use HTTPS. Also note that you should use this setting only on HTTPS pages; using it on HTTP pages may cause errors. This is only relevant for JavaScript publishing code. See Publishing a Video Cloud Player on an HTTPS Page for more information.
                bspHtml += '<param name="secureConnections" value="' + (window.location.protocol === 'https:').toString() + '" />';

                if (attObj) {
                    if (attObj.playerID) { // playerId  - Number: The player ID. This parameter is being replaced by the playerKey parameter. If you want to change which player is loaded on a page, you should change the values of both the playerId and the playerKey parameters.
                        bspHtml += '<param name="playerID" value="' + attObj.playerID + '" />';
                    }
                    
                    if (attObj.playerKey) { // playerKey - String: A unique identifier for the player, including an encrypted representation of the player ID.
                        bspHtml += '<param name="playerKey" value="' + attObj.playerKey + '" />';
                    }
                    
                    if (attObj.autoStart) { // Boolean: If true, the video begins to play as soon as the player loads.
                        bspHtml += '<param name="autoStart" value="' + attObj.autoStart + '" />';
                    }

					if (attObj.bgcolor) { // String: A six-digit hexadecimal representation of the player's background color. For example, bgcolor = "#FFFFFF;" (the default) represents white. If you omit this parameter, the player has a white background until it fully loads, which might not be the appearance you want. One suggestion is to match the background color to the color of the page the player loads within.
                        bspHtml += '<param name="bgcolor" value="' + attObj.bgcolor + '" />';
                    }

					if (attObj.height) { // Number: The total height of the player, in pixels.
                        bspHtml += '<param name="height" value="' + attObj.height + '" />';
                    }

					if (attObj.labels) { // String: The URL of an XML file that describes the label set to use for this player. See Localizing Players for information about how to create a label set file.
                        bspHtml += '<param name="labels" value="' + attObj.labels + '" />';
                    }

					if (attObj.language) { // Enum: The language to use for the default player labels; one of en (English), fr (French), de (German), it (Italian), jp (Japanese), or es (Spanish). This value overrides the language setting set for the player in the Video Cloud Studio Publishing module.
                        bspHtml += '<param name="language" value="' + attObj.language + '" />';
                    }

					if (attObj.linkBaseURL) { // String: The base URL to use when the viewer clicks the "Share", "Get Link", or "Email" button in the player. Overrides the default linkBaseURL and enables linking to the parent page of the player. See Customizing the link URL.
                        bspHtml += '<param name="linkBaseURL" value="' + attObj.linkBaseURL + '" />';
                    }

					if (attObj.localizedErrorXML) { // String: The URL of a player error message file. This XML file specifies the messages to be displayed in case of player errors. See Localizing player error messages.
                        bspHtml += '<param name="localizedErrorXML" value="' + attObj.localizedErrorXML + '" />';
                    }

					if (attObj.majorVersion) { // String: The major version number of Flash that is required. By default, Video Cloud players require Flash version 9.0.28 or later. To require Flash 10, use this parameter: <param name="majorVersion" value="10" />.
                        bspHtml += '<param name="majorVersion" value="' + attObj.majorVersion + '" />';
                    }

					if (attObj.minorRevision) { // String: The minor revision number of Flash 9 that is required. By default, Video Cloud players require Flash version 9.0.28 or later. To require a later Flash 9 version, use a parameter like <param name="minorRevision" value="115" />.
                        bspHtml += '<param name="minorRevision" value="' + attObj.minorRevision + '" />';
                    }

					if (attObj.showNoContentMessage) { // Boolean: If true, enables the display of a warning message if no content has been assigned to the player.
                        bspHtml += '<param name="showNoContentMessage" value="' + attObj.showNoContentMessage + '" />';
                    }

					if (attObj.skipContentCall) { // Boolean: If true, prevents a content call from being made, if you are using the Flash-only Player API to retrieve content.
                        bspHtml += '<param name="skipContentCall" value="' + attObj.skipContentCall + '" />';
                    }

					if (attObj.templateErrorHandler) { // String: The name of a user-defined JavaScript function to be automatically invoked in response to player error events.
                        bspHtml += '<param name="templateErrorHandler" value="' + attObj.templateErrorHandler + '" />';
                    }

					if (attObj.templateLoadHandler) { // String: The name of a user-defined JavaScript function to be automatically invoked when the player loads. This provides an entry point for the Smart Player API.
                        bspHtml += '<param name="templateLoadHandler" value="' + attObj.templateLoadHandler + '" />';
                    }

					if (attObj.templateReadyHandler) { // String: The name of a user-defined JavaScript function to be automatically invoked player ready events.
                        bspHtml += '<param name="templateReadyHandler" value="' + attObj.templateReadyHandler + '" />';
                    }

					if (attObj.videoSmoothing) { // Boolean: Overrides the default video smoothing behavior for the player; if true (the default value), always applies video smoothing and if false, never applies video smoothing. If absent, the player uses video smoothing.
                        bspHtml += '<param name="videoSmoothing" value="' + attObj.videoSmoothing + '" />';
                    }

					if (attObj.width) { // Number: The total width of the player, in pixels.
                        bspHtml += '<param name="width" value="' + attObj.width + '" />';
                    }
                    /*
					if (attObj.playlistCombo) { // String: The ID or refid of one or more playlists to program into a player that uses a ComboBox (dropdown) for playlist selection. See Assigning content to players programmatically.
						bspHtml += '<param name="@playlistCombo" value="' + attObj.playlistCombo + '" />';
					}
					
					if (attObj.playlistTabs) { // String: The ID or refid of one or more playlists to program into a player that uses tabs for playlist selection. See Assigning content to players programmatically.
						bspHtml += '<param name="@playlistTabs" value="' + attObj.playlistTabs + '" />';
					}
					
					if (attObj.videoList) { // String: The ID or refid of a playlist to program into the player. See Designating featured content.
						bspHtml += '<param name="@videoList" value="' + attObj.videoList + '" />';
					}
                     */
                }
                bspHtml += '</object>';

                $('#' + elemId).replaceWith(bspHtml);

                callbackObj.success = true;
                callbackObj.ref = document.getElementById(elemId);
            }

            if (attObj && attObj.callbackFn) {
                attObj.callbackFn(callbackObj);
            }
        },

        // Pause a Brightcove Smart Player object
        loadByRefIdBSP : function(elemId, refId) {
			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.BrightcoveExperience.html
            var brightcoveExperience;

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.html#.getExperience
            brightcoveExperience = brightcove.getExperience(elemId);

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.modules.VideoPlayerModule.html
            var videoPlayerModule;

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.BrightcoveExperience.html#getModule
            videoPlayerModule = brightcoveExperience.getModule(APIModules.VIDEO_PLAYER);


			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.modules.VideoPlayerModule.html#loadVideoByReferenceID
            videoPlayerModule.loadVideoByReferenceID(refId);
        },

        // Pause a Brightcove Smart Player object
        pauseBSP : function(elemId) {
			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.BrightcoveExperience.html
            var brightcoveExperience;

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.html#.getExperience
            brightcoveExperience = brightcove.getExperience(elemId);

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.modules.VideoPlayerModule.html
            var videoPlayerModule;

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.BrightcoveExperience.html#getModule
            videoPlayerModule = brightcoveExperience.getModule(APIModules.VIDEO_PLAYER);

			// See: http://docs.brightcove.com/en/smart-player-api/symbols/brightcove.api.modules.VideoPlayerModule.html#pause
            videoPlayerModule.pause();
        }
    };
}();

// initial setup in the special templateLoaded() function
function onTemplateLoaded(elemId) {
    if (elemId == "r360-player") {
        var brightcoveExperience = brightcove.getExperience("r360-player");
        var experienceModule = brightcoveExperience.getModule(APIModules.EXPERIENCE);
        experienceModule.addEventListener(BCExperienceEvent.TEMPLATE_READY, function(event) {
            var brightcoveExperience = brightcove.getExperience("r360-player");
            var videoPlayerModule = brightcoveExperience.getModule(APIModules.VIDEO_PLAYER);
            videoPlayerModule.addEventListener(BCMediaEvent.PROGRESS, function(event) {
                if (event.position > event.duration - .1) {
                    var brightcoveExperience = brightcove.getExperience("r360-player");
                    var videoPlayerModule = brightcoveExperience.getModule(APIModules.VIDEO_PLAYER);
                    videoPlayerModule.seek(0);
                }
            });
            videoPlayerModule.addEventListener(BCMediaEvent.COMPLETE, function(event) {
                var brightcoveExperience = brightcove.getExperience("r360-player");
                var videoPlayerModule = brightcoveExperience.getModule(APIModules.VIDEO_PLAYER);
                videoPlayerModule.play();
            });
            videoPlayerModule.play();
        });
    }
}

/**
 * Zoom JS
 */

initializeZoom = function(img){
	var zoom;
	var increment;
	var left, top;
	var adjustzoom;
	var resetzoom;
	var fullscreen = false;

	zoom = 20;
	increment = 10;
	
	$(img)
		.draggable({
			drag:function(event, ui) {
				$(this).trigger("change");
			},
			stop:function(event, ui) {
				$(this).trigger("change");
			}
		})
		.change(function(){
			var thisObj = this;
			
			//Initial set values for positioning
			var ztop = Math.round($(this).css("top").replace("px", ""));
			var zleft = Math.round($(this).css("left").replace("px", ""));
			
			if(adjustzoom) {
				adjustzoom = false;
				zleft = zleft - 30;
			}
			
			if(zoom == 30){
				adjustzoom = true;
				zleft = zleft + 30;
			}
			
			//Half the height and width of img
			var zheight = Math.round($(this).css("height").replace("px", "")) / 2;
			var zwidth = Math.round($(this).css("width").replace("px", "")) / 2;
			
			//Store initial top and left positioning values
			var g = ztop + zheight;
			var h = zleft + zwidth;
			
			//Sets new height width values after zooming
			$(this).css({width:3300 * ( zoom / 100 ),height:2000 * ( zoom / 100 )});
			
			//New half height and width of zoom image
			zheight = Math.round($(this).css("height").replace("px", "")) / 2;
			zwidth = Math.round($(this).css("width").replace("px", "")) / 2;
			
			//Retrieve initial top and left values
			ztop = g - zheight;
			zleft = h - zwidth;
			
			if(resetzoom){
				resetzoom = false;
				zleft = 0;
				ztop = 0;
			}
			
			//Adjust top and left positioning on canvas
			$(this).css({ left : zleft, top : ztop });
		});
	$("#slider").slider({
			min:20,
			max:100,
			step:increment,
			slide:function(event, ui){
				zoom = ui.value;
				$("#zoom-image .main-image img").trigger("change");
			},
			change:function(event, ui){
				zoom = ui.value;
				$("#zoom-image .main-image img").trigger("change");
			}
		});
	$("#btn-zoom-in")
		.click(function() {
			$("#slider").slider('value', $("#slider").slider('value') + increment);
			$("#slider").trigger('change');
		});

	$("#btn-zoom-out")
		.click(function() {
			$("#slider").slider('value', $("#slider").slider('value') - increment);
			$("#slider").trigger('change');
		});
		
	$("#btn-zoom-reset")
		.click(function() {
			resetzoom = true;
			$("#slider").slider('value', 20);
			$("#slider").trigger('change');
		});
		
	var escMsg = $('<div id="escape"><p>Press Esc to exit full screen mode.</p></div>');
		
	$("#btn-full-screen")
		.click(function() {
			if(!fullscreen){
				fullscreen = true;
				$("#rich-media").hide();
				if($("#full-screen-zoom").length == 0){
					$("body").prepend('<div id="full-screen-zoom" />');
					$("#full-screen-zoom").height($(window).height());
					$(img).clone().appendTo('#full-screen-zoom');
					$(this).clone(true).appendTo('#full-screen-zoom').find("img").attr({src:'/images/buttons/btn-fullscreen-in.png',title: "Exit full screen", alt: "Exit full screen"});
					escMsg.appendTo('#full-screen-zoom');
					$("#escape").css("left",($("body").width() / 2) - 300);
				} else {
					$('#full-screen-zoom').show();
					$("#escape").css("left",($("body").width() / 2) - 300);
				}
				
				$("#escape").fadeIn(1500, function(){
					$(this).delay(700).fadeOut(1500);
				});
				$(document).scrollTop(0);
				$(document).bind("keydown", function(e) {
				  if (e.keyCode == 27) { $('#btn-full-screen').click(); }   // esc
				});
				adjustDimensions();
				$(window).bind('resize', function() {
					adjustDimensions();
				});
			} else {
				fullscreen = false;
				$('body').css({ height: 'auto', width: 'auto', overflow: 'auto' });
				$('#full-screen-zoom').hide();
				var fsImg = $(this).find(img);
				$('#escape').hide();
				$("#rich-media").show();
				$(window).unbind('resize');
				$(window).unbind('scroll');
				$(document).unbind("keypress");
			}
		});
	
	function adjustDimensions(){
		var target = $("#full-screen-zoom .ui-draggable"),
			topOffset = 0,
			winH = $(window).height(),
			winW = $(window).width(),
			imgH = winH,
			imgW = winW,
			maxH = 2000,
			maxW = 3300,
			zoomedIn = false;
		if(imgH > imgW){
			imgW = Math.ceil((imgH / maxH) * maxW);
			
			if(imgW > winW){
				imgW = winW;
				imgH = Math.ceil((imgW / maxW) * maxH);
			}
		}
		if(imgW > imgH) {
			imgH = Math.ceil((imgW / maxW) * maxH);
			
			if(imgH > winH){
				imgH = winH;
				imgW = Math.ceil((imgH / maxH) * maxW);
			}
		}
		if(imgH < winH){
			topOffset = Math.ceil((winH - imgH) / 2);
		}
		$("body, #full-screen-zoom").css({height: "100%", width: "100%", overflow: "hidden"});
		target.css({ height: imgH, width: imgW, marginTop: topOffset });
	}
};

