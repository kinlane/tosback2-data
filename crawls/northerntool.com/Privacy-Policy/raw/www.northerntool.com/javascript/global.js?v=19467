function updateBtn2ItemInCartGif(idPrefix){
    var itemInCartCookieVal = readCookie("WC_XITEMINCART");
    itemInCartCookieVal = unescape(itemInCartCookieVal);
    
    if(null != itemInCartCookieVal && 'null' != itemInCartCookieVal && itemInCartCookieVal != ""){
        var splitResult = itemInCartCookieVal.split("|");
        for(i = 0; i < splitResult.length; i++){
            $("#" + idPrefix + "_" + splitResult[i] + "_img").attr("src", imageDirectoryPath + "images/buttons/btn2-item-in-cart.gif"); 
        }
    }
}
