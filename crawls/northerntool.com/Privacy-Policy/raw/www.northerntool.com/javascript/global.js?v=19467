/* Jquery lightbox */
(function($){$.fn.lightBox=function(settings){settings=jQuery.extend({overlayBgColor:'#EBE7DE',overlayOpacity:0.7,fixedNavigation:false,imageLoading:'/wcsstore/NTEStorefrontAssetStore/images/buttons/loading.gif',imageBtnPrev:'/wcsstore/NTEStorefrontAssetStore/images/buttons/prev.gif',imageBtnNext:'/wcsstore/NTEStorefrontAssetStore/images/buttons/next.gif',imageBtnClose:'/wcsstore/NTEStorefrontAssetStore/images/buttons/close-black.gif',imageBlank:'/wcsstore/NTEStorefrontAssetStore/images/backgrounds/space.gif',containerBorderSize:10,containerResizeSpeed:0,txtImage:'Image',txtOf:'of',keyToClose:'c',keyToPrev:'p',keyToNext:'n',imageArray:[],activeImage:0},settings);var jQueryMatchedObj=this;function _initialize(){_start(this,jQueryMatchedObj);return false;}
