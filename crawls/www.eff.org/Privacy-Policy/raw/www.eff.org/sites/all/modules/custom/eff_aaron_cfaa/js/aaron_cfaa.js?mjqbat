(function ($) {
  $(function(){
    if($('body.toolbar').length == 0) {
      var $banner = $('<div id="aaron-banner"></div>')
        .html('<a href="https://action.eff.org/o/9042/p/dia/action/public/?action_KEY=9005"><img src="https://www.eff.org/sites/default/files/aarons-law-action-button.png" alt="Fix computer crime law. Take Action." /></a>');
      $('body').append($banner);
    }
  });
})(jQuery);
