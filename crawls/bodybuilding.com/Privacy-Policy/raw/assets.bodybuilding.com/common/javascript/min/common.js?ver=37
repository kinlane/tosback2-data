var BB = window.BB || {};
BB.Require = (function () {

    var methods = {
        registry: {}
    };

    /**
     * The register method will set a scriptId(key) and the path to the script file in the registry.
     * 
     * @name register
     * @param {String} scriptId The unique identifier for script. Try to use proper namespacing convention please.
     * @param {String} path The path to the script file on assets
     * @param {Boolean} forceOverwrite if true, this flag will overwrite an existing scriptId instead of throwing an exceot. *You better have a good reason for using this flag.*
     */
    methods.register = function (scriptId, path, forceOverwrite, chk) {
        var f = forceOverwrite ? true : false;
        if (scriptId && path && scriptId !== '' && path !== '') {
            if (!methods.registry[scriptId] || f) {
                methods.registry[scriptId] = {
                    'path': path,
                    'loaded': false
                };
                if (chk && typeof chk === 'function') {
                    methods.registry[scriptId].loaded = chk();
                }
            } else {
                try {
                    console.log('The scriptId "' + scriptId + '" is already in use');
                } catch (e) {}
            }
        }
        return true;
    };

    /**
     * The unregister method will un-set a scriptId(key) and the path to the script file in the registry. This is generally not a good idea to use unless absolutely required.
     * 
     * @name unregister
     * @param {String} scriptId The unique identifier for script. Try to use proper namespacing convention please.
     */
    methods.unregister = function (scriptId) {
        if (scriptId && scriptId !== '') {
            return delete methods.registry[scriptId];
        }
    };

    /**
     * Loads a given script file for use if a valid scriptId is used. The is the method that actually writes the script tag onthe page.
     * 
     * @name requires
     * @param {String} scriptId The unique identifier for script. Try to use proper namespacing convention please.
     * @param {Function} callback A callback function that will be run after the script is loaded.
     */
    methods.requires = function (scriptId, callback) {
        if (scriptId && scriptId !== '' && typeof scriptId === 'string' && methods.registry[scriptId]) {
            var loadedCheck = methods.registry[scriptId].check;
            var loaded = false;
            if (methods.registry[scriptId].loaded) {
                loaded = true;
            } else if (loadedCheck && typeof loadedCheck === 'function' && loadedCheck()) {
                loaded = true;
            }
            
            if (!loaded) {
                methods.includeScriptFile(scriptId, callback);
            } else {
                methods.scriptLoaded(scriptId, callback);
            }
        } else {
            throw new Error('Invalid script require: ' + scriptId + 'does not exist');
        }
        return true;
    };

    /**
     * Loads a given script file for use if a valid scriptId is used. The is the method that actually writes the script tag onthe page.
     * 
     * @name requires
     * @param {String} scriptId The unique identifier for script. Try to use proper namespacing convention please.
     * @param {Function} callback A callback function that will be run after the script is loaded.
     */
    methods.includeScriptFile = function (scriptId, callback) {
        var scr = document.createElement('script');
        scr.type = 'text/javascript';
        scr.src = methods.registry[scriptId].path;
        if (callback && typeof callback === 'function') {
            // IE 8 and older browsers use onreadystatechange to report that a script
            // is finished loaded. IE 9 will use onreadystatechange if rendering a page
            // in quirks mode. Otherwise it will use onload like modern browsers.
            if (typeof scr.onreadystatechange !== 'undefined') {
                scr.onreadystatechange = function () {
                    methods.checkStateChange.apply(this, [scr.readyState, scriptId, callback]);
                };
            } else {
                scr.onload = function() {
                    methods.scriptLoaded(scriptId, callback);
                };
            }
        }
        document.getElementsByTagName('head')[0].appendChild(scr);
    };

    /**
     * Callback wrapper for old browsers. Used because there are 5 different ready states so we need to check
     * agains them to figure out when a script is finally loaded.
     * 
     * @name requires
     * @param {String} scriptId A callback function that will be run after the script is loaded.
     * @param {Function} callback A callback function that will be run after the script is loaded.
     */
    methods.checkStateChange = function (readyState, scriptId, callback) {
        if (readyState === 'loaded') {
            methods.scriptLoaded(scriptId, callback);
        }
    };

    /**
     * Callback wrapper for modern browsers
     * 
     * @name requires
     * @param {String} scriptId A callback function that will be run after the script is loaded.
     * @param {Function} callback A callback function that will be run after the script is loaded.
     */
    methods.scriptLoaded = function (scriptId, callback) {
        methods.registry[scriptId].loaded = true;
        if (callback && typeof callback === 'function') {
            callback();
        }
    };

    return {
        registerScript: function (scriptId, path, forceOverwrite, checkFunc) {
            return methods.register(scriptId, path, forceOverwrite, checkFunc);
        },
        unRegisterScript: function (scriptId) {
            return methods.unregister(scriptId);
        },
        requires: function (scriptId, callback) {
            return methods.requires(scriptId, callback);
        },
        dumpRegistry: function () {
            var attr;
            try {
                for (attr in methods.registry) {
                    if (methods.registry.hasOwnProperty(attr)) {
                        console.log(attr + ' = [' + methods.registry[attr].path + ', loaded: ' + methods.registry[attr].loaded + ']');
                    }
                }
            } catch (ex) {}
        },
        loadConfig: function(configObj) {
            if (!configObj) {
                return false;
            }

            var attr, count = 0, item;
            for (attr in configObj) {
                if (configObj.hasOwnProperty(attr)) {
                    item = configObj[attr];
                    methods.register(attr, item.path, false, item.check);
                    count++;
                }
            }
            return count;
        }
    };

}());
if (!BB.Instances.tooltipData) {
    BB.Instances.tooltipData = {};
}
