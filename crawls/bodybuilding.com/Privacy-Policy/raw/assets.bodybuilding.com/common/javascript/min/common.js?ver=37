    'jquery': {
        path: '//assets.bodybuilding.com/common/javascript/min/jquery.min.js',
        check: function() {return (typeof jQuery !== 'undefined');}
    },
    'jquery-ui': {
        path: '//assets.bodybuilding.com/common/javascript/min/jquery-ui.js',
        check: function() {return (typeof jQuery !== 'undefined' && typeof jQuery.ui !== 'undefined');}
