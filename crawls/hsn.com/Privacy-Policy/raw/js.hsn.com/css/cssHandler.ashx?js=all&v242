function AutoSuggestControl(c,b,a){this.cur=-1;this.layer=null;this.provider=b;this.textbox=c;this.timeoutId=null;this.userText=c.value;this.numOfChar=a;this.init();this.suggesns=true}AutoSuggestControl.prototype.autosuggest=function(a,c,b){this.cur=-1;if(a.length>0){if(c){this.typeAhead(a[0])}if(b){this.showSuggestions(a)}}else{this.hideSuggestions()}};AutoSuggestControl.prototype.createDropDown=function(){this.layer=document.createElement("div");this.layer.className="suggestiveSearch";this.layer.style.visibility="hidden";this.layer.style.width=this.textbox.offsetWidth;document.body.appendChild(this.layer);var a=this;this.layer.onmousedown=this.layer.onmouseup=this.layer.onmouseover=function(b){var d=$("searchQuery").value;b=b||window.event;oTarget=b.target||b.srcElement;if(oTarget.nodeName!="B"){oTarget=b.target||b.srcElement}else{if(b.target!=null){oTarget=b.target.parentNode}else{oTarget=b.srcElement.parentElement}}if(b.type=="mousedown"){if(oTarget.innerText!=null&&oTarget.innerText!=""){a.textbox.value=oTarget.innerText}else{if(oTarget.id!="labelP"&&oTarget.id!="labelK"&&(navigator.userAgent.indexOf("Firefox")==-1)){var c=oTarget.parentNode.nextSibling.innerHTML.replace("<b>","").replace("</b>","").replace("<B>","").replace("</B>","").unescapeHTML();a.textbox.value=c}else{a.textbox.value=oTarget.innerHTML.replace("<b>","").replace("</b>","").replace("<B>","").replace("</B>","").unescapeHTML()}}if(a.textbox.value!=""){a.hideSuggestions();cmCreateManualLinkClickTag("cm_sp=global*search*suggestedsearch");a.textbox.ancestors().first().submit()}else{if(oTarget.id=="labelP"||oTarget.id=="labelK"){a.textbox.value=d;a.textbox.focus();return}}}else{if(b.type=="mouseover"){if(oTarget.id=="sscontainer"||oTarget.id=="sscontainerlast"){a.highlightSuggestion(oTarget)}}else{a.textbox.focus()}}}};AutoSuggestControl.prototype.getLeft=function(){var b=this.textbox;var a=0;if(b.offsetParent){do{a+=b.offsetLeft}while(b=b.offsetParent)}return a};AutoSuggestControl.prototype.getTo