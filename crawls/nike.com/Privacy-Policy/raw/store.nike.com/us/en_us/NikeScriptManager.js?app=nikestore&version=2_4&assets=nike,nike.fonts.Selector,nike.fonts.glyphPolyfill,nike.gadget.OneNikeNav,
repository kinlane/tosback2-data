var j=function(I,N){var P=I.find(".selectBox-scrollbar a"),B=I.find(".selectBox-options"),y=I.find(".selectBox-scrollbar"),z=y.offset(),A={bottom:z.top+y.height(),top:z.top},D=P.offset(),F={bottom:D.top+P.height(),top:D.top},Q=B.height()-I.height(),K=y.height()-P.height(),O,M,E,C;
