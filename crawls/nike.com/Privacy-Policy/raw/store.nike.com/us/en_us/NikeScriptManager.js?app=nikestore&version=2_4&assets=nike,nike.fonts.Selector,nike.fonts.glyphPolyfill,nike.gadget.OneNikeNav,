if(a){nike.request({type:"POST",url:a,data:{country:c.toUpperCase(),lang_locale:d,action:"updateShippingCountry",rt:"json"}},function(f){try{responseData=$.parseJSON(f.data)
}catch(g){nike.error("Error parsing cart response");
nike.error(g)
}if(responseData.status=="success"){nike.dispatchEvent(nike.Event.UPDATE_SHIPPING_COUNTRY_SUCCESS,{response:responseData})
}else{nike.dispatchEvent(nike.Event.UPDATE_SHIPPING_COUNTRY_FAIL,{response:responseData})
}},function(e){nike.log("Recieved failure response message from cart service");
var f={};
f.errorMessages=[e.toString()];
nike.dispatchEvent(nike.Event.UPDATE_SHIPPING_COUNTRY_FAIL,{response:f})
})
}else{nike.debug("Could not get url from yellowpages. jsonCartService");
nike.dispatchEvent(nike.Event.UPDATE_SHIPPING_COUNTRY_FAIL)
}};
nike.Cart.UserType={UNKNOWN:"UNKNOWN",DEFAULT_USER:"DEFAULT_USER",EMPLOYEE:"EMPLOYEE",AFFILIATE:"AFFILIATE"};
