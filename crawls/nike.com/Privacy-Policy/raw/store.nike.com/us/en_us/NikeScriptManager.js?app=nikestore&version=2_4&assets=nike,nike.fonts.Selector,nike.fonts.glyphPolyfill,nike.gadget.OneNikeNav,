nike.gadget.Gadget=Class.extend({gadgetElement:undefined,nikeBindings:{},init:function(a){this.gadgetElement=a
},bindListeners:function(){if(this.nikeBindings){this.bindNikeListeners()
}},unbindListeners:function(){this.gadgetElement.unbind();
