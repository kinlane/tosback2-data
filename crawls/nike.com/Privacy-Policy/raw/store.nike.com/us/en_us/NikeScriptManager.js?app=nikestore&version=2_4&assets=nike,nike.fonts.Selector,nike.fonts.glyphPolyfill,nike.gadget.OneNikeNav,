k.html(a.attr("data-bp"));
if(e.attr("data-eda")=="true"){e.html(e.attr("data-ep"))
}break;
case nike.Cart.UserType.AFFILIATE:j.html(a.attr("data-oap"));
k.html(a.attr("data-ap"));
if(e.attr("data-eda")=="true"){e.html(e.attr("data-ap"))
}break;
