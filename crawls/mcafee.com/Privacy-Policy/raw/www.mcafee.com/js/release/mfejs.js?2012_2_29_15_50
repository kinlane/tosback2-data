var ip2long=function(ip){var ips=ip.split(".");
var iplong=0;
with(Math){iplong=ips[0]*pow(256,3)+ips[1]*pow(256,2)+ips[2]*pow(256,1)+ips[3]*pow(256,0);
}return iplong;
};
