if(!results){classname="default";
}else{classname="";
}toInject+="<li class='"+classname+"'><a href='#'><img src='"+pp_images[i]+"' width='50' alt='' /></a></li>";
}toInject=settings.gallery_markup.replace(/{gallery}/g,toInject);
