var H4Val=1;
var ID1=document.getElementById("tab-OnDemand");
if(ID1!=null){H4Val=ID1.getElementsByTagName("h4").item(0).innerHTML.trim();
}for(TagName in arryList){containerContent="container-"+arryList[0];
getTabName="tab-";
getliName="li-";
getContentName="content-";
getHyperLinkName="Hyperlink-";
getTabName=getTabName+arryList[TagName];
getliName=getliName+arryList[TagName];
getContentName=getContentName+arryList[TagName];
getHyperLinkName=getHyperLinkName+arryList[TagName];
