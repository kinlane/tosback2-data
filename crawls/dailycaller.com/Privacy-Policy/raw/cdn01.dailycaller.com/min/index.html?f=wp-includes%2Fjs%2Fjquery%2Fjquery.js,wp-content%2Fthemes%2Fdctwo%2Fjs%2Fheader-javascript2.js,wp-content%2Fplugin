function ss_esl_emailAjax(cookie_exp_ms){var ajaxRequest;try{ajaxRequest=new XMLHttpRequest();}catch(e){try{ajaxRequest=new ActiveXObject("Msxml2.XMLHTTP");}catch(e){try{ajaxRequest=new ActiveXObject("Microsoft.XMLHTTP");}catch(e){alert("Your browser is not compatible with AJAX.");return false;}}}
