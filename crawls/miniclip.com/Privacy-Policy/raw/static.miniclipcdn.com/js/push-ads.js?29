var PushAd={push_ads:{},track_ads:[],counter:[],init:function(a){this.push_ads=a;this.firstRun()},firstRun:function(){$(".mc-push").each(function(a){var b=$(this);var c=b.attr("id").split("-");PushAd.counter[b.attr("id")]=c[1];PushAd.showAds(b.attr("id"))});PushAd.statTrack()},showAds:function(g){var e=$("#"+g);if(e.attr("id")==g){var b=e.attr("data-type");if(typeof PushAd.push_ads[b]=="undefined"){return}if(!PushAd.counter[g]||parseInt(PushAd.counter[g])+1>PushAd.push_ads[b].length){PushAd.counter[g]=0}if(PushAd.push_ads[b][PushAd.counter[g]]){var d=PushAd.push_ads[b][PushAd.counter[g]];if(d.token==b){var c="_self";var a=new RegExp(location.host+"|/games/");if(!a.test(d.url)){c="_blank"}PushAd.counter[g]=parseInt(PushAd.counter[g])+1;var f=d.token+"/"+d.image_file;e.html('<a href="'+d.url+'" data-track="'+f+'" id="ad-'+d.id+'" class="ad-image" target="'+c+'"><span class="icon-wrap"><img src="'+static_path+"content/push-ads/"+d.push_ad_type_id+"/"+d.image_file+'" alt="'+d.image_file+'" /></span></a>');PushAd.track(d.token,d.image_file);$(".ad-image").click(function(){statTracker("/push-adserver/click/"+$(this).attr("data-track"))})}}}setTimeout(function(){PushAd.showAds(g);PushAd.statTrack()},e.attr("data-refresh"));return false},statTrack:function(){for(var b in this.track_ads){var a="/push-adserver/view/"+this.track_ads[b].join(",")}this.track_ads=[]},track:function(b,a){if(typeof(this.track_ads[b])=="undefined"){this.track_ads[b]=[]}this.track_ads[b].push(a)}};