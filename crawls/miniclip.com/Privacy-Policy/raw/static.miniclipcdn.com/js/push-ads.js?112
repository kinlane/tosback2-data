var PushAd={push_ads:{},counter:{},timers:{},init:function(a){this.push_ads=a;this.firstRun()},firstRun:function(){$(".mc-push").each(function(b){var a=$(this);var c=a.attr("id").split("-");PushAd.counter[a.attr("id")]=c[1];PushAd.showAds(a.attr("id"))})},showAds:function(h){var a=$("#"+h);var f=false;if(a.attr("id")==h){var e=a.attr("data-type");if(typeof PushAd.push_ads[e]=="undefined"){return}if(!PushAd.counter[h]||parseInt(PushAd.counter[h])+1>PushAd.push_ads[e].length){PushAd.counter[h]=0}if(PushAd.push_ads[e][PushAd.counter[h]]){f=PushAd.push_ads[e][PushAd.counter[h]];if(f.token==e){var c="_self";var b=new RegExp(location.host+"|/games/");if(!b.test(f.url)){c="_blank"}var d=f.url+"#t-p-"+f.push_ad_type_id+"-"+f.push_ad_id;var g=f.token+"/"+f.image_file;a.html('<a href="'+d+'" data-track="'+g+'" id="ad-'+f.push_ad_id+'" class="ad-image" target="'+c+'"><span class="icon-wrap"><img src="'+static_path+"content/push-ads/"+f.push_ad_type_id+"/"+f.image_file+'" alt="'+f.image_file+'" /></span></a>');$(".ad-image").unbind("click");$("ad-image").click(function(){statTracker("/push-adserver/click/"+$(this).attr("data-track"))});if(f.is_tracked==1){statTracker("/push-adserver/view/"+g)}else{statTrackerSampled("/push-adserver/view/"+g)}if(f.keep_visible==1){PushAd.push_ads[e].splice(PushAd.counter[h],1);clearTimeout(this.timers[h])}else{PushAd.counter[h]=parseInt(PushAd.counter[h])+1}}}}if(!f||(f&&f.keep_visible==0)){this.timers[h]=setTimeout(function(){PushAd.showAds(h)},a.attr("data-refresh"))}return false}};$(function(){$(".mc-push-ab").each(function(){var a=$(this).find("img");statTrackerSampled("/push-ad-ab-test/view/"+a.attr("src"));$(this).click(function(){statTracker("/push-ad-ab-test/click/"+a.attr("src"))})})});