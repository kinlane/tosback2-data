
jQuery('#d-streaming-player').appear(function(){setTimeout(function(){xml='';processXml(xml);},3000);},{one:true});function processXml(xml){if("undefined"==typeof(ord))ord=Math.random()*10000000000000000;var pixel_url='http://ad.doubleclick.net/ad/allthingsd.com/front;tracker=dmedia;sz=1x1;ord='+ord;var pixel=new Image();jQuery(pixel).load(function(){jQuery(this).hide();jQuery('#d-pixel').append(this);}).error(function(){}).attr('src',pixel_url);var overlay=jQuery('#d-streaming-overlay');if(pageHasFlash){jQuery(overlay).fadeIn('slow');}
setTimeout(function(){jQuery(overlay).fadeOut('slow');},10000);}