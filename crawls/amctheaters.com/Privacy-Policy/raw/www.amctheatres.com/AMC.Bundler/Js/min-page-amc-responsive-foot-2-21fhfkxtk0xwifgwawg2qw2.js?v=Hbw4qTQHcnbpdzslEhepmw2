var amc=amc||{};$.extend(true,amc,{views:{TheatreListMenuViewModel:function(a){var b=this;b.options=$.extend({},a);b.hasCurrentLocation=ko.computed(function(){if(amc.user.info.location.unitNumber()){return true}else{return false}});b.noCurrentLocation=ko.computed(function(){return !amc.user.info.location.unitNumber()});b.currentUnitNumber=ko.computed(function(){return amc.user.info.location.unitNumber()});b.leftColumnTheatres=ko.computed(function(){var d=amc.user.info.location.theatres();var c=[];if(d){if(d[0]){c[0]=d[0]}if(d[3]){c[1]=d[3]}if(d[6]){c[2]=d[6]}}return c});b.middleColumnTheatres=ko.computed(function(){var d=amc.user.info.location.theatres();var c=[];if(d){if(d[1]){c[0]=d[1]}if(d[4]){c[1]=d[4]}if(d[7]){c[2]=d[7]}}return c});b.rightColumnTheatres=ko.computed(function(){var d=amc.user.info.location.theatres();var c=[];if(d){if(d[2]){c[0]=d[2]}if(d[5]){c[1]=d[5]}if(d[8]){c[2]=d[8]}}return c});b.showLocationModal=function(){amc.events.setLocation.trigger({},b.element)};b.setTheatreAsCurrent=function(d,c){c.stopPropagation();amc.user.info.location.unitNumber(d.UnitNumber())}}}});
