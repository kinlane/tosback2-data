.ValidationMessage_DateOfBirth=ko.computed(function(){if(!f.DateOfBirth_Month()||!f.DateOfBirth_Day()||!f.DateOfBirth_Year()){return"This field is required."}else{return"You must be 13 years of age or older and 125 years of age or younger."}});f.Validate_EmailOrSMS=function(){if(!f.EmailAddress()&&!f.MobileNumber()){return false}return true};f.Validate_EmailAddress=function(){if(!f.EmailAddress()){if(f.UserConfirmsSubscriptionEmail()){return false}else{return true}}return amc.validators.isValidEmailAddress(f.EmailAddress())};f.Validate_UserConfirmsSubscriptionEmail=function(){if(f.EmailAddress()&&!f.UserConfirmsSubscriptionEmail()){return false}return true};f.Validate_MobileNumber=function(){if(!f.MobileNumber()){if(f.UserConfirmsSubscriptionSMS()){return false}else{return true}}return amc.validators.isValidPhone(f.MobileNumber())};f.Validate_UserConfirmsSubscriptionSMS=function(){if(f.MobileNumber()&&!f.UserConfirmsSubscriptionSMS()){return false}return true};f.Validate_FirstName=function(){if(!f.FirstName()){return false}if(!f.FirstName().match(/^[A-z\s'-]*$/gi)){return false}return true};f.Validate_LastName=function(){if(!f.LastName()){return false}if(!f.LastName().match(/^[A-z\s'-]*$/gi)){return false}return true};f.Validate_PostalCode=function(){if(!f.PostalCode()){return false}if(!amc.validators.isValidPostalCode(f.PostalCode())){return false}return true};f.Validate_DateOfBirth=function(){if(!f.DateOfBirth_Month()||!f.DateOfBirth_Day()||!f.DateOfBirth_Year()){return false}var h=moment(f.DateOfBirth_Year()+"-"+f.DateOfBirth_Month()+"-"+f.DateOfBirth_Day());if(!h.isValid()||!amc.validators.isValidBirthdate(h.format("YYYY-MM-DD"),13)){return false}return true};f.GetDateOfBirth=function(){var h=moment(f.DateOfBirth_Year()+"-"+f.DateOfBirth_Month()+"-"+f.DateOfBirth_Day());if(h.isValid()){return h.format("YYYY-MM-DD")}return null};f.FormIsValid=ko.computed(function(){if(!f.Validate_EmailOrSMS()||!f.Validate_EmailAddress()||!f.Validate_UserConfirmsSubscriptionEmail()||!f.Validate_MobileNumber()||!f.Validate_UserConfirmsSubscriptionSMS()||!f.Validate_PostalCode()||!f.Validate_DateOfBirth()||!f.Validate_FirstName||!f.Validate_LastName){return false}return true});f.submitForm=function(){f.submitHasBeenClicked(true);if(!f.FormIsValid()){$.fancybox.update();return}f.loading(true);amc.services.directMarketingSubscribe.call({params:{FirstName:f.FirstName(),LastName:f.LastName(),EmailAddress:f.EmailAddress(),MobileNumber:f.MobileNumber(),PostalCode:f.PostalCode(),DateOfBirth:f.GetDateOfBirth()},cache:false,success:function(h){f.loading(false);f.SuccessMessage(h.message);f.ErrorMessage("");$.fancybox.update()},error:function(h){f.loading(false);f.SuccessMessage("");if(h&&h.message){f.ErrorMessage(h.message)}else{f.ErrorMessage("An error has occurred. Please try again.")}},target:f.element})};f.loading=ko.computed({read:function(){return this._loading||false},write:function(h){this._loading=h;if(h){$.fancybox.showLoading()}else{$.fancybox.hideLoading()}},owner:f}).extend({throttle:1});f.SuccessMessage=ko.observable("");f.ErrorMessage=ko.observable("");f.ShowSuccessMessage=ko.computed(function(){if(f.SuccessMessage()){return true}return false});f.ShowErrorMessage=ko.computed(function(){if(f.ErrorMessage()){return true}return false})}}});
