function toggle_nav(b){var a=xb_get_element(b).style.display;if(a=="none"){expand_nav(b)}else{collapse_nav(b)}}function highlight_nav(a){if(a.className.indexOf("_active")>-1){return}if(a.className.indexOf("_open")>-1){a.className="left_nav_cat_hover left_nav_cat_open"}else{a.className="left_nav_cat_hover left_nav_cat_closed"}}function unhighlight_nav(a){if(a.className.indexOf("_active")>-1){return}if(a.className.indexOf("_open")>-1){a.className="left_nav_cat_inactive left_nav_cat_open"}else{a.className="left_nav_cat_inactive left_nav_cat_closed"}}function collapse_nav(a){xb_get_element("trigger_"+a).className=xb_get_element("trigger_"+a).className.replace(/left_nav_cat_open/,"left_nav_cat_closed");hide(a)}function expand_nav(a){xb_get_element("trigger_"+a).className=xb_get_element("trigger_"+a).className.replace(/left_nav_cat_closed/,"left_nav_cat_open");unhide(a)};