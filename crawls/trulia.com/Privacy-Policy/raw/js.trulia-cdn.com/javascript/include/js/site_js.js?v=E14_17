oker.value;reg_phone=trim(typeof frm.phone!=="undefined"?frm.phone.value:"")}}else{if(context=="voices_embed"){frm.password_verify_field.value=frm.password_field.value}reg_email=trim($("#email_field"+suffix).val()).toLowerCase();reg_email_confirm=trim($("#email_confirm_field"+suffix).val()).toLowerCase();reg_password=$("#password_field"+suffix).val();if(context!="dynamic_popup"){reg_password_verify=$("#password_verify_field"+suffix).val();reg_name=trim($("#name_field"+suffix).val());reg_location=trim($("#location_field"+suffix).val());reg_auto_location=$("#autocomplete_location"+suffix).val();reg_status=$("#user_status"+suffix).val();reg_pro_status=$("#pro_status"+suffix).val();reg_broker=$("#broker"+suffix).val();reg_phone=$("#phone"+suffix).val()}}focus_field_ids={email:"email_field"+suffix,email_confirm:"email_confirm_field"+suffix,pass:"password_field"+suffix,pass_verify:"password_verify_field"+suffix,name:"name_field"+suffix,location:"location_field"+suffix,user_status:"user_status_field"+suffix,pro_status:"pro_status_field"+suffix,broker:"broker_field"+suffix,phone:"phone_field"+suffix};validate_error_message="";errors=new Array();error_fields=new Array();field_to_focus="";trulia.clear_registration_form_error("register");if(context=="voices_embed"||context=="dynamic_popup"){_.each(focus_field_ids,function(field){trulia.clear_registration_form_field_error(field)})}if(!is_valid_email(reg_email)){errors.push("Please enter a valid email address, such as name@domain.com.");error_fields.push(focus_field_ids.email);if(field_to_focus==""){field_to_focus=focus_field_ids.email}omni_errors.push("reg_email_invalid")}else{if(reg_email==""||reg_email==_REG_DEFAULT_FIELDS.email){errors.push("Please enter an email address.");error_fields.push(focus_field_ids.email);if(field_to_focus==""){field_to_focus=focus_field_ids.email}omni_errors.push("reg_email_none")}else{if(context=="dynamic_popup"){trulia.check_email_exists("register")}}}if(!is_valid_email(reg_email_confirm)){errors