function AdPrefs(){this.data={};for(var b=document.cookie.split("; "),a=0;a<b.length;a++)if("BG_AD_PREFS"===b[a].substr(0,11)){this.data=JSON.parse(unescape(b[a].substr(12)));break}this.save=function(a,b){adPrefs.data[a]=b;var d=escape(JSON.stringify(adPrefs.data)),f=new Date;f.setTime(f.getTime()+864E5);document.cookie="BG_AD_PREFS="+d+"; expires="+f.toGMTString()+"; path=/"}}var adPrefs=new AdPrefs;
function setUpdatePrefs(b,a,e,c){prefs[b]=a;var d,f,g,h,a="";d=/^(pslookup|keysearch)_(search|version)(\d+)/;for(b in prefs)if("string"===typeof prefs[b])if(b.match(d)){var i=b.substring(0,b.length-1),j=i+"@"+prefs[b];switch(i){case "pslookup_search":g=g?g+(">"+prefs[b]):j;break;case "pslookup_version":f=f?f+(">"+prefs[b]):j;break;case "keysearch_search":a&&(a+="&");a+=i+"@"+prefs[b];break;case "keysearch_version":h=h?h+(">"+prefs[b]):j}}else a&&(a+="&"),a+=b+"@"+prefs[b];a+="&"+g+"&"+f+"&undefined&"+
h;b=new Date;b.setYear(b.getFullYear()+1);document.cookie="BG_PREFS="+a+";expires="+b.toGMTString()+"; path=/;";if(e)try{_gaq.push(e)}catch(k){}c&&setTimeout(window.location.reload,100)}
