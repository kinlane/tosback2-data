h;b=new Date;b.setYear(b.getFullYear()+1);document.cookie="BG_PREFS="+a+";expires="+b.toGMTString()+"; path=/;";if(e)try{_gaq.push(e)}catch(k){}c&&setTimeout(window.location.reload,100)}
