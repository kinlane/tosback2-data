$("#footerToolbarContent-"+a).show(),$(".footerToolbarBarItem").removeClass("active"),$("#footerToolbarBarItem-"+a).addClass("active"),$(".footerToolbar").animate({height:toolbar.heightMaximized},toolbar.speedMinMax),toolbar.toolbarMaximized=!0,toolbar.currentContent=a}else toolbar.currentContent=null},minimize:function(a){a=a?a:null;toolbar.friendlistOpened?toolbar.friendlistHide(a):toolbar.toolbarMaximized?($(".footerToolbarMinimize").hide(),$(".footerToolbarClose").show(),$(".footerToolbar").animate({height:toolbar.heightMinimized},
toolbar.speedMinMax,function(){toolbar.hideContents();a&&a()}),toolbar.toolbarMaximized=!1):a&&a();if(!a)toolbar.currentContent=null},toggle:function(){toolbar.minimize();toolbar.maximize()},open:function(){window.setTimeout(function(){$(".footerToolbarOpen, .footerToolbarOpenMeta").fadeOut(toolbar.speedOpenCloseContent,function(){$(".footerToolbarClose, .footerToolbarBarContent, .footerToolbarMetaContent, .footerToolbarSpacerCloseMinimize").fadeIn()})},toolbar.speedOpenClose-toolbar.speedOpenCloseContent);
$(".footerToolbarContent").show();$(".footerToolbar").animate({width:"100%",borderTopLeftRadius:0,borderBottomLeftRadius:0},toolbar.speedOpenClose);$(".footerToolbarBar").animate({borderTopLeftRadius:0},toolbar.speedOpenClose);$(".footerToolbarMeta").animate({borderBottomLeftRadius:0},toolbar.speedOpenClose)},close:function(){$(".footerToolbarClose, .footerToolbarBarContent, .footerToolbarMetaContent, .footerToolbarSpacerCloseMinimize").fadeOut(toolbar.speedOpenCloseContent,function(){$(".footerToolbarOpen, .footerToolbarOpenMeta").fadeIn()});
$(".footerToolbarContent").hide();$(".footerToolbar").animate({width:"50px",borderTopLeftRadius:10,borderBottomLeftRadius:10},toolbar.speedOpenClose);$(".footerToolbarBar").animate({borderTopLeftRadius:10},toolbar.speedOpenClose);$(".footerToolbarMeta").animate({borderBottomLeftRadius:10},toolbar.speedOpenClose)},hideContents:function(){$(".footerToolbarContentData").hide()},scrollContentLeft:function(){var a=$(".footerToolbarContentData:visible");Math.abs(parseInt($(a).css("left")))>0&&$(a).css({left:"+="+
toolbar.scrollContentWidth+"px"})},scrollContentRight:function(){var a=$(".footerToolbarContentData:visible"),c=parseInt($(a).width())+parseInt($(a).css("paddingLeft"))+parseInt($(a).css("paddingRight"))+parseInt($(a).css("marginLeft"))+parseInt($(a).css("marginRight")),b=$(".footerToolbarContent").width();c-b-toolbar.scrollContentWidth>Math.abs(parseInt($(a).css("left")))&&$(a).css({left:"-="+toolbar.scrollContentWidth+"px"})},setContentSlideWidth:function(){$(".footerToolbarContentData").each(function(){var a=
0;$(this).children().each(function(){a+=parseInt($(this).width())+parseInt($(this).css("paddingLeft"))+parseInt($(this).css("paddingRight"))+parseInt($(this).css("marginLeft"))+parseInt($(this).css("marginRight"))});$(this).css("width",a)})},instanceNext:function(a){var c=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:visible"),b=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:visible").next();$(b).length||(b=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:first"));
$(c).hide();$(b).show()},instancePrev:function(a){var c=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:visible"),b=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:visible").prev();$(b).length||(b=$(a).parent().find(".footerToolbarContentItemInstanceselectSelection > ul > li:last"));$(c).hide();$(b).show()},getNews:function(){toolbar.maximize("news")},getEvents:function(){toolbar.maximize("events")},getFriends:function(){toolbar.isLoggedIn?
toolbar.friendlistLoad():toolbar.showLogin()},getGames:function(){toolbar.maximize("games")}};window.myAd={soi_site_script:"bigpoint.js",container_prefix:"ad-",loadScript:function(a){if(window.SOI_WERBUNG){var b="";switch(a){case "global":b="http://ad.71i.de/global_js/globalV6.js";break;case "site":this.soi_site_script&&(b="http://ad.71i.de/global_js/Sites/"+this.soi_site_script)}b&&document.write('<script src="'+b+'" type="text/javascript"><\/script>')}},insertAd:function(a){if(window.SOI_WERBUNG&&window.SoiAd){var b=!1;switch(a){case "popup1":b=window.SOI_PU1;break;case "fullbanner2":b=
window.SOI_FB2;break;case "skyscraper1":b=window.SOI_SC1;break;case "rectangle1":b=window.SOI_RT1}b&&SoiAd.write(a)}},finishAd:function(a,b){switch(b){case "move":this.moveAd(a)}this.adjustLayoutForAd(a,b)},moveAd:function(a){window.SOI_WERBUNG&&window.SoiAd&&SoiAd.moveAd(a,this.container_prefix+a+"-postponed",this.container_prefix+a)},adjustLayoutForAd:function(a){var b=document.getElementById(this.container_prefix+a);if(b){var d=window.SoiAd?SoiAd.exists(a):!1;if(d)if((a!="fullbanner2"||!SoiAd.isBillboard(a))&&
SoiAd.removeStyleAttribute(b),SoiAd.reserveSowefoSpace(a))b.style.backgroundColor="transparent",b.style.backgroundImage="none",b.style.borderColor="transparent";else if(SoiAd.isBlockpixel(a))b.style.display="none";if(a!="popup1"){var c=0,e=0;d&&(c=SoiAd.getWidth(a),e=SoiAd.getHeight(a));if(a=="fullbanner2"){if(d){SoiAd.isPowerbanner(a);var d=SoiAd.isPushdown(a),f=SoiAd.isWallpaper(a),g=SoiAd.isFireplace(a),h=SoiAd.isBillboard(a);b.style.width=c+"px";if(d)SoiAd.setAutoHeight(a,this.container_prefix+
a);else if(f||g)b.style.height=Math.max(e,90)+"px",b.style.backgroundImage="none";else if(h&&(b.style.backgroundImage="none",b=this.container_prefix+a+"-billboard",a=document.getElementById(b)))SoiAd.removeStyleAttribute(a),b=parseInt(SoiQuery.getCurrentStyle(b+"-outer","width"))||990,(b=Math.max(parseInt((c-b)/2),0))?a.style.marginLeft=-1*b+"px":a.style.width=c+"px"}}else if(a=="rectangle1"){if(d&&(SoiAd.isHalfpage(a)&&SoiAd.setAutoHeight(a,this.container_prefix+a),c&&c>300))b.style.width=c+"px"}else if(a==
"skyscraper1"&&d&&c&&c>160)b.style.width=c+"px"}}}};