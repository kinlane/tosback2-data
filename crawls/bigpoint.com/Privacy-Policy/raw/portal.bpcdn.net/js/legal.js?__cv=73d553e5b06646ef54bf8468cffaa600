f-c,0,e,f)+d},easeOutBounce:function(a,b,c,d,e){return(b/=e)<1/2.75?d*7.5625*b*b+c:b<2/2.75?d*(7.5625*(b-=1.5/2.75)*b+0.75)+c:b<2.5/2.75?d*(7.5625*(b-=2.25/2.75)*b+0.9375)+c:d*(7.5625*(b-=2.625/2.75)*b+0.984375)+c},easeInOutBounce:function(a,c,d,e,f){if(c<f/2)return b.easing.easeInBounce(a,c*2,0,e,f)*0.5+d;return b.easing.easeOutBounce(a,c*2-f,0,e,f)*0.5+e*0.5+d}})}(jQuery);
