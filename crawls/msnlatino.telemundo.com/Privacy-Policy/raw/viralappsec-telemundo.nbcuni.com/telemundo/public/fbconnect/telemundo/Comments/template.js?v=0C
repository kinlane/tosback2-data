	output += '								<div class="fbc_innerThumbLftContainer"><img width="50px" height="50px" src="<#= imgURL #><#= showName #>_Thump.jpg"></img></div>';
	output += '                       		<div class="arrow_placement"><img src="<#= imgURL #>LeftPointArrow.png"></img></div>';
