	output += '   <# } #>';
	output += '							<# if (typeof childComments.FIRSTNAME  != "undefined" && childComments.FIRSTNAME  != "" && childComments.FIRSTNAME  != null) { #>';
	output += '                           	<div><div id="connectedUserName<#= childComments.COMMENTID #>" class="fbc_postedBy"><#= childComments.FIRSTNAME #><span class="createdBy"> on <#= childComments.createdAt #></span></div><div class="dotDivider"><img width="450" src="<#= imgURL #>CommentLineSmall.jpg"></div><div class="flag_report"><img src="<#= imgURL #>Icon_flag_G.gif" onclick="teleflagComment(<#= childComments.COMMENTID #>,\'<#= escape(childComments.COMMENTTEXT) #>\');return false;"><# if (0) { #><a onclick="deleteComment(<#= childComments.COMMENTID #>,<#= childComments.mediaId #>);return false;" class="fbc_iccDelete" href="#">Delete</a><# } #></div>';
