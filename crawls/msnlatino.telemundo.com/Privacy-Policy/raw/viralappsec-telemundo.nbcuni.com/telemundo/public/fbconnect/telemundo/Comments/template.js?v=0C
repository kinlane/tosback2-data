  output += '<div id="showLess<#=commentIds[0]#>" class="showLess" style="display:none;"><a href="#" onClick="showComments(\'<#=commentIds#>\',\'none\',\'block\');return false;">&laquo; minimize</a></div>';
  output += ' <#}#>';
	return output;
}

function showComments(comments,action,nextAction)
{
var commentArray = comments.split(",");
  jQuery.each(commentArray, function(i){
  $("#"+"CommentContentDiv"+commentArray[i]).css("display",action);
  });
  $("#showMore"+commentArray[0]).css("display",nextAction);
  $("#showLess"+commentArray[0]).css("display",action);
//alert(document.getElementById('CommentContentDiv'+);
}
