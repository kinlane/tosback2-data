	//output += '				 <# if (anonymousUserReporting == "Y" || fbconnected != "N" ) {#> <div class="fbc_iccReport"><a href="#" class="fbc_iccReportLink" onclick="flagComment (<#= childComments.COMMENTID #>,\'<#= escape(childComments.COMMENTTEXT) #>\');return false;">Report</a><# } #></div>';
	output += '                       </div>';
	output += '                   </div> </div></div><div class="fbc_clearFloat"></div></div><div id="flag_comment_popup<#=childComments.COMMENTID #>"></div>';
	 output += ' <# if (i>=5){ #> ';
	output += '<# flag = 1;#>';  
  output += '<# commentIds[j] = childComments.COMMENTID ;#>';
  output += '<# j++;#>';
  output += ' <#}#>';
	output += '		<# 	}); #>';
	output += ' <# if (flag==1){ #> '; 
