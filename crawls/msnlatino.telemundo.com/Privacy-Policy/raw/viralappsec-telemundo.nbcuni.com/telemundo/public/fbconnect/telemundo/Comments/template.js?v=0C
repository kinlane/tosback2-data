	output += '			<div class="align_txtareaedit<#= browserType #>"><textarea name="text" class="fbc_commentText" id="fbc_editUserComment"></textarea><div id="fbc_edituserComment_errormsg" class="usereditComment_errormsg"><#=commentTextEntry #></div><div id="fbc_edituserComment_errormsg2" class="userComment_errormsg"><#=profanityMessage #></div></div>';
	output += '		<div class="fbc_form_multi_buttons">';
	output += '			<button class="fbc_cancel_btn" type="button" onclick="closeeditForm();return false;"></button>';
	output += '			<button class="fbc_submit_btn"  id="fbc_editSaveComment" type="button" onclick="editComment(\'telemundoSaveComments\');return false;"></button>';
	output += '		</div>';
	output += '		</div>';
	output += '	</form></div>';
	return output;
}
function displayChildComments(comments) {
