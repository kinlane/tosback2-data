this.win.appendChild(close);var close2=document.createElement('a');close2.href="#";close2.innerHTML="Close";close2.className="novis";close2.onclick=function(){that.close();return false};document.getElementById('ShowBox-bottom').appendChild(close2);this.animate(0,1);window.onresize=function(){that.placeWin();that.placeBg()};window.onscroll=function(){that.placeWin();that.placeBg()}};ShowBoxClass.prototype.close=function(){var that=this;this.animate(1,0,function(){that.win.parentNode.removeChild(that.win)});that.overlay.parentNode.removeChild(that.overlay);window.onresize=null;window.onscroll=null;return false};ShowBoxClass.prototype.placeBg=function(){var bg=this.overlay;var scrolled=(window.pageXOffset==undefined)?{x:document.documentElement.scrollLeft,y:document.documentElement.scrollTop}:{x:window.pageXOffset,y:window.pageYOffset};bg.style.width=0;bg.style.height=0;var winsize=(window.innerHeight==undefined)?{x:document.documentElement.clientWidth+'px',y:document.documentElement.clientHeight+'px'}:{x:'100%',y:'100%'};
bg.style.top=scrolled.y+'px';bg.style.left=scrolled.x+'px';bg.style.width=winsize.x;bg.style.height=winsize.y};ShowBoxClass.prototype.placeWin=function(){var img=this.win;var bg=this.overlay;var middle={x:bg.offsetWidth/2,y:bg.offsetHeight/2};var size={x:img.offsetWidth/2,y:img.offsetHeight/2};var x=bg.offsetLeft+middle.x-size.x;var y=bg.offsetTop+middle.y-size.y;if(x<0)x=0;if(y<0)y=0;img.style.top=y+"px";img.style.left=x+"px"};ShowBoxClass.prototype.animate=function(start,stop,after){var div=(start<stop)? 0.2 : -0.2;var a=this.win;a.style.opacity=start;a.style.filter='alpha(opacity='+(start*100)+')';var i=0;var b=start;var that=this;var f=function(){i++;if(b!=stop){b=start+(div*i);a.style.opacity=b;a.style.filter='alpha(opacity='+(b*100)+')'}else{window.clearInterval(that.animateTimer);that.animateTimer=null;if(typeof(after)=='function')
after()}};this.animateTimer=window.setInterval(f,50)}

