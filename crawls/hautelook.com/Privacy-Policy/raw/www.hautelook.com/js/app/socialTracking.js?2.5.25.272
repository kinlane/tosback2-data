$Halo.bigbro.subscribe("Facebook_ApiReady",function(){if(FB&&FB.Event&&FB.Event.subscribe){FB.Event.subscribe("edge.create",function(){_gaq.push(["_trackSocial","facebook","like"])});FB.Event.subscribe("edge.remove",function(){_gaq.push(["_trackSocial","facebook","unlike"])});FB.Event.subscribe("message.send",function(){_gaq.push(["_trackSocial","facebook","send"])});FB.XFBML.parse()}});
twttr.ready(function(b){b.events.bind("tweet",function(){_gaq.push(["_trackSocial","twitter","tweet"])});b.events.bind("follow",function(a){a&&_gaq.push(["_trackEvent","twitter","follow",a.data.user_id+" ("+a.data.screen_name+")"])})});
