        { href: "http://sellonbuy.com/\" target=\"_blank", src: akPath + "/v9/header/slides/slide_sell_mp.png", onclick: "header_slider_SellMP" }        
    ];
} else if (buyHeaderNum == 2) {
    var header_slider_items =
    [
        { href: "http://sellonbuy.com/\" target=\"_blank", src: akPath + "/v9/header/slides/slide_sell_mp.png", onclick: "header_slider_SellMP" },
