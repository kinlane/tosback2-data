//set akamai image path for secure and non-secure
if (location.href.indexOf("https://") != -1) {
    var akPath = "https://a248.e.akamai.net/f/248/845/10h/images.buy.com/buy_assets";
} else {
    var akPath = "http://ak.buy.com/buy_assets";
}
//set random number for rotating promos
rnd.today = new Date();
rnd.seed = rnd.today.getTime();

function rnd() {
    rnd.seed = (rnd.seed * 9301 + 49297) % 233280;
    return rnd.seed / (233280.0);
};

function rand(number) {
    return Math.ceil(rnd() * number);
};


var buyHeaderNum = rand(2);

if (buyHeaderNum == 1) {
    var header_slider_items =
    [
        { href: "http://www.buy.com/loc/black-friday-store/71859.html?omadtrack=blackfriday_headerslider", src: akPath + "/campaigns/2012/blackout/header_slider_BFday.jpg", onclick: "header_slider_bf" },
        { href: "http://www.buy.com/loc/holiday-store/71860.html", src: akPath + "/campaigns/2012/holiday_store/238x34.png", onclick: "header_slider_holiday" }
    ];
} else if (buyHeaderNum == 2) {
    var header_slider_items =
    [
        { href: "http://www.buy.com/loc/black-friday-store/71859.html?omadtrack=blackfriday_headerslider", src: akPath + "/campaigns/2012/blackout/header_slider_BFday.jpg", onclick: "header_slider_bf" },
        { href: "http://www.buy.com/loc/holiday-store/71860.html", src: akPath + "/campaigns/2012/holiday_store/238x34.png", onclick: "header_slider_holiday" }		
    ];
} else {
    var header_slider_items =
    [
