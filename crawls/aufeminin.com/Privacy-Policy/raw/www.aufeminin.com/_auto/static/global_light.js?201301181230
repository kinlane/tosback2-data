function af_navSubOpen(el,pageid) {if (pub_SmartDomain == 'network.aufeminin.com') {afNavOpenPixSpan=document.createElement('span');afNavOpenPixSpan.style.position='absolute';afNavOpenPixSpan.innerHTML='<img src="http://' + pub_SmartDomain+'/call/pubimppixel/'+(typeof(afNavLight)!='undefined'?7451718:7357193)+'/'+Math.floor(Math.random()*999999)+'?" border=0 width=1 height=1>';af_nav2012.appendChild(afNavOpenPixSpan);// af_navSubOpen.img=new Image();// af_navSubOpen.img.src='http://'+pub_SmartDomain+'/call/pubimppixel/'+7357193+'/'+Math.floor(Math.random()*999999)+'?';}af_nav2012.style.position=(afNavFixed ? 'fixed' : 'relative');af_nav2012.style.zIndex=5500000;/* close old opened tab */if (af_navCurrentOpenedTab != null)af_navCurrentOpenedTab.className = af_navCurrentOpenedTab.className.replace(' af_nav_hover', '');/* open new one */af_navCurrentOpenedTab = el;af_navCurrentOpenedTab.className = af_navCurrentOpenedTab.className + ' af_nav_hover';af_accToggleDisplay(false);if (typeof(af_pubTagsShowHide)!='undefined' && typeof(af_pubTagsShowHide_disabled)=='undefined')af_pubTagsShowHide(false);/* ad: get menu subsection (formatid 15135) */if (pageid && el.id && typeof(afAdScripts)=='object' && afAdScripts.menuSubSection)afAdScripts.menuSubSection(pageid,el.id);
}function af_navSubIn(el, event) {if (!el.tagName) {if (!event) var event = top.event;el=event.srcElement ? event.srcElement : event.target;while (el && el.tagName != 'LI') el = el.parentElement;}var param_rub = el.id;if (param_rub && param_rub.indexOf("pub")<0) {if (!afDdData[param_rub].isCreated) {initNavOptimizeLoadingTab(param_rub); // preload rub picturesvar links=adsPlaceholders='';if (param_rub=='af_nav_people' || param_rub=='af_nav_news') {/* build people and news section */for (var i=0; i<afDdData[param_rub].sousRubLinks.length; i++) {var iRef=afDdData[param_rub].sousRubLinks[i];links += '<tr><td><a href="'+iRef.url+'" class="navPeopleTitre"><h4>' + iRef.sectTexte + '</h4></a>'+iRef.teaserDateParution+'</td>'+'<td width="52"><a href="'+iRef.teaserUrl+'" class="navPeopleImg"><img src="'+iRef.teaserImgUrl+'" alt="" border="0"/></a></td></tr>';}var content = '<div class="af_overTab">'+'<div class="af_overTabCadre" onmousedown="afnav_trck((typeof(afNavLight)!=\'undefined\'?7451715:7354075));"><a href="' + afDdData[param_rub].url + '"><span>' + afDdData[param_rub].name + '</span></a></div>'+'<div class="hide_shadow">&nbsp;</div></div>'+'<table onmousedown="af_navTracking(7355608);this.onmousedown=null;" cellpadding="10" cellspacing="0" border="0" width="100%" height="100%" class="af_dropDown_' + afDdData[param_rub].item + '">'+'<tr><td class="af_dropDown_td" valign="top" align="left">'+'<table cellpadding="0" cellspacing="0" border="0" width="100%" class="navPeopleSpe">' + links + '</table>';if (param_rub=='af_nav_people') {content += '<table cellpadding="0" cellspacing="0" border="0" width="100%">'+'<tr><td class="navPeopleLinkPlus"><a href="http://news.aufeminin.com/people/">+ de People</a></td>'+'<td class="navPeopleLinkAlbums" width="30%"><a href="http://news.aufeminin.com/people/">Albums people</a></td>'+'<td class="navPeopleLinkForum" width="30%"><a href="http://www.aufeminin.com/forum/show0_28/cine-tv-stars-musique.html">Foru