}function af_navBigTeaser(param_rub, l, themeID, lnk) {// console.log('> af_navBigTeaser('+param_rub+','+l+','+themeID')');if (param_rub!='af_nav_forum' && l=='tab' ) l=0;/* nav usage tracking */var navtrck = (typeof(afNavLight)!='undefined'?7451720:7368710); //modifiedif (l=='tab'||l==0) {navtrck=(typeof(afNavLight)!='undefined'?7451721:7368721);} //originalvar teaserBig='';if (param_rub=='af_nav_beaute' && l==af_idx_clubInBeaute) { // club expertesteaserBig = afDdData['af_nav_club'].sousRubLinks[af_idx_clubExpertes].iHTML;} else if (param_rub!='af_nav_people' && param_rub!='af_nav_news') { // news and people are static tabsif (l=='pub_15135' && themeID) { // ad: call menu subsection (formatid 15135)var e = document.getElementById('pub_15135_'+themeID+'_0');if (e) teaserBig = e.innerHTML;} else if (param_rub=='af_nav_forum' || l=='tab' || themeID>0) {if (l=='tab')teaserBig = afDdData['af_nav_forum'].community_mosaic;else { // #rub# forum links or real forum linksif (themeID>0) {for (t in afDdData['af_nav_forum'].sousRubLinks)if (afDdData['af_nav_forum'].sousRubLinks[t].themeID==themeID)teaserBig = (afDdData['af_nav_forum'].sousRubLinks[t].iHTML ? afDdData['af_nav_forum'].sousRubLinks[t].iHTML : '');} elseteaserBig = (afDdData['af_nav_forum'].sousRubLinks[l].iHTML ? afDdData['af_nav_forum'].sousRubLinks[l].iHTML : '');}} else if (l=='forum') {teaserBig = '<table cellspacing=0 cellpadding=0 border=0><tr><td height=310 valign=top><table cellspacing=0 cellpadding=0 border=0><tr><td valign=top><a href="'+lnk+'"><h3>'+afDdData[param_rub].community.forum_title+'</h3></a></td></tr><tr><td valign=top><a href="'+lnk+'"><img src="/world/static/edito/nav2012/forum.jpg" class=teasernav></a><p>Partagez vos expériences, vos avis, vos interrogations et vos centres 	d’intérêts sur les forums !</p></td></tr></table></td></tr></table>';} else if (l=='album') {teaserBig = '<table cellspacing=0 cellpadding=0 border=0><tr><td height=310 valign=top><table cell