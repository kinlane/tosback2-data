/*************************
 ****** utilitaires ******
 ************************/

 function af_getCookie(name) {
	var i, x, y;		
	var cookiesTab = document.cookie.split(';');
	for (i = 0; i < cookiesTab.length; i++) {
		x = cookiesTab[i].substr(0, cookiesTab[i].indexOf('='));
		y = cookiesTab[i].substr(cookiesTab[i].indexOf('=') + 1);
		x = x.replace(/^\s+|\s+$/g,'');
		if (x == 'afperso') {
			var j,a,cookiesValuesTab = y.split('&');
			for (j = 0; j < cookiesValuesTab.length; j++) {
				a = cookiesValuesTab[j].substr(0,cookiesValuesTab[j].indexOf('='));
				if (a==name) {
					return unescape(cookiesValuesTab[j].substr(cookiesValuesTab[j].indexOf('=')+1));
				}
			}
		}
	}
	return '';
}

// AJAX GET request
function af_requestGet(url, param, callback, json) {
	var xhr  = af_getXMLHttpRequest();
	callback = callback || null;
	json     = json || true;
	if (url.substr(0,1) == '/') {
		// Make relative URL absolute (to avoid BASE tag rewrite issues)
		url = document.location.protocol + '//' + document.location.host + url
	}
	xhr.onreadystatechange = function() {
		if (xhr.readyState == 4 && (xhr.status == 200 || xhr.status == 0)) {
			if (xhr.responseText) {
				data = json ? eval('(' + xhr.responseText+ ')') : xhr.responseText;
			} else {
				data = null;
			}
			if (callback) callback(data);
		}
	};
	xhr.open('GET', url+'?'+param, true);
	xhr.send(null);
}

// AJAX POST request
function af_requestPost(url, param, callback, json) {
	var xhr  = af_getXMLHttpRequest();
	callback = callback || null;
	json     = json || true;
	param   += '&personalKey=' + af_personalHashValue;
	if (url.substr(0,1) == '/') {
		// Make relative URL absolute (to avoid BASE tag rewrite issues)
		url = document.location.protocol + '//' + document.location.host + url
	}
	xhr.onreadystatechange = function() {
		if (xhr.readyState == 4 && (xhr.status == 200 || xhr.status == 0)) {
			if (xhr.responseText) {
				data = json ? eval('(' + xhr.responseText+ ')') : xhr.responseText;
			} else {
				data = null;
			}
			if (callback) callback(data);
		}
	};
	xhr.open('POST', url, true);
	xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
	xhr.setRequestHeader('X-Requested-With', 'XMLHttpRequest');
	xhr.send(param);
}

function af_getXMLHttpRequest() {
	var xhr = null;
	if(window.XMLHttpRequest || window.ActiveXObject) {
		if (window.ActiveXObject) {
			try {
				xhr = new ActiveXObject("Msxml2.XMLHTTP");
			} catch(e) {
				xhr = new ActiveXObject("Microsoft.XMLHTTP");
			}
		} else {
			xhr = new XMLHttpRequest(); 
		}
	}
	return xhr;
}

function af_urlencode(str) {
    str = (str + '').toString();
    return encodeURIComponent(str).replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');
}

/***********************************************
 ************* end utilitaires *****************
 ***********************************************/
 
 /**************************
 ****** lazy loading ******
 **************************/
function af_findPos(o){
	if (!o || o==document.body) return { l:0, t:0 };
	var l = t = 0, b = document.body, w = o.offsetWidth, h = o.offsetHeight;
	if (o.offsetParent) {
		l = o.offsetLeft - o.scrollLeft;
		t = o.offsetTop - o.scrollTop;
		while (o = o.offsetParent) {
			l += o.offsetLeft - (o!=b ? o.scrollLeft : 0);
			t += o.offsetTop - (o!=b ? o.scrollTop : 0);
		}
	}
	return { l:l, t:t, r:l+w, b:t+h, w:w, h:h };
}

function af_windowSize() {
	var i = window, e = document.documentElement, b = document.body;
	var w = ( i.innerWidth ? i.innerWidth : ( e && e.clientWidth ? e.clientWidth : (b &&  b.clientWidth ? b.clientWidth : 0) ) );
	var h = ( i.innerHeight ? i.innerHeight : ( e && e.clientHeight ? e.clientHeight : (b &&  b.clientHeight ? b.clientHeight : 0) ) );
	var l = ( i.pageXOffset ? i.pageXOffset : ( e && e.scrollLeft ? e.scrollLeft : (b &&  b.scrollLeft ? b.scrollLeft : 0) ) );
	var t = ( i.pageYOffset ? i.pageYOffset : ( e && e.scrollTop ? e.scrollTop : (b &&  b.scrollTop ? b.scrollTop : 0) ) );
	return { l:l, t:t, r:l+w, b:t+h, w:w, h:h };
}

/* images lazyloading */
var af_lazyloading_timeout = null;
var af_lazyloading_dist = 200;
var af_lazyloading_imgs = document.getElementsByTagName('img');

//TODO : utiliser une classe pour cibler les images a bencher ?
function af_lazyloading() {
	if (af_lazyloading_timeout == null) {
		af_lazyloading_timeout = setTimeout(af_lazyloading_real, 50);
	}
}
function af_lazyloading_real() {
	var w = af_windowSize();
	var len = af_lazyloading_imgs.length;
	for (var i = 0; i < len; i++) {
		var img = af_lazyloading_imgs[i];
		var d = img.getAttribute('dataorigin');
		if (d && d != '') {
			if (w.b + af_lazyloading_dist >= af_findPos(img).b) {
				img.src = d;
				img.setAttribute('dataorigin', '');
			}
		}
	}
	af_lazyloading_timeout = null;
}
if (window.addEventListener) {
	window.addEventListener('load', af_lazyloading, false);
	window.addEventListener('scroll', af_lazyloading, false);
} else if(window.attachEvent) {
	window.attachEvent('onload', af_lazyloading);
	window.attachEvent('onscroll', af_lazyloading);
} else {
	var onloadorig = window.onload;
	window.onload = function() {
		if (onloadorig) onloadorig();
		af_lazyloading();
	}
	var onscrollorig = window.onscroll;
	window.onscroll = function() {
		if (onscrollorig) onscrollorig();
		af_lazyloading();
	}
}

/**************************
 ****** end lazy loading ***
 **************************/



function af_gaqRePush() {
	var u = (''+document.location).replace(/^http:\/\/.*?\//gi,'/virtual/');
	
	if (typeof _gaq != 'undefined') {
		_gaq.push(['_trackPageview',u]);
	} else if (typeof pageTracker != 'undefined') {
		pageTracker._trackPageview(u);
	}
}



/*******************************
 *** Dynamic display SeeAlso ***
 *******************************/
var af_dynamicSA = {
	'onScroll': function() {
		af_dynamicSA.update();
	},
	
	'update': function() {
		// No SeeAlso block, or job already done
		if (this.links === undefined) {
			this.links = this.getFirstElementWithClassName('afsa_relLinks', 'div');
		}
		if (this.links == null) {
			return;
		}
		
		// Init
		if (this.startHeight === undefined) {
			//var forumElement =  this.getFirstElementWithClassName('aff_bestofdiv', 'div');
			//if (forumElement != null) {
			//	this.startHeight = Math.round(document.body.offsetHeight * 0.75);
			//} else {
				var p = af_findPos(this.links);
				this.startHeight = p.t - 150;
			//}
		}
		
		// Maintain position
		if (this.generated == true) {
			this.updatePosition();
		} else {
			// Check visibility
			var w = af_windowSize();
			if (w.b >= this.startHeight) {
				// Display block
				this.generated = true;
				this.generate();
				this.display();
			}
		}
	},
	
	'getFirstElementWithClassName': function(classname, objectType) {
		if (document.getElementsByClassName) {
			var e = document.getElementsByClassName(classname);
			if (e && e.length > 0) {
				return e[0];
			}
		} else {
			var e = document.getElementsByTagName(objectType);
			for (var i = 0; i < e.length; i++) {
				if (e[i].className == classname) {
					return e[i];
				}
			}
		}
		return null;
	},
	
	'generate': function() {
		var displayTeaser, trackingHome, trackingLinks, divID;
		//var r = Math.random();
		//if (r < 0.3) {
		//	displayTeaser = false;
		//	trackingHome = 7232315;
		//	trackingLinks = 7232307;
		//	divID = 'af_dynamic_seealso';
		//} else if (r < 0.6) {
		//	displayTeaser = false;
		//	trackingHome = 7257519;
		//	trackingLinks = 7257517;
		//	divID = 'af_dynamic_seealso2';
		//} else {
			displayTeaser = true;
			trackingHome = 7258372;
			trackingLinks = 7258367;
			divID = 'af_dynamic_seealso3';
		//}
	
		this.mainDiv = document.createElement('div');
		this.mainDiv.id = divID;
		
		var html = '<a href="#" class="close" title="Fermer" onclick="' +
						'af_dynamicSA.mainDiv.style.display=\'none\';' +
						'setAfXperso(\'afperso\', \'stopDySA\', \'1\');' +
						'return false;">x</a>';
		var mainTmstp = Math.round(Math.random()*99999);
		html = html + '<img src="http://www.smartadserver.com/call/adimppixel/'+trackingLinks+'/'+mainTmstp+'?" width=1 height=1 border=0 style="position:absolute;">';
		html = html + '<b>Sur le même thème :</b>';
		html = html + '<ul>';
		var l = this.links.getElementsByTagName('a');
		for (var i = 0; i < l.length; i++) {
			if (l[i].href.replace('http://','').split('/')[1] == '') {
				var homeTmstp = Math.round(Math.random()*99999);
				html = html + '<li class=homepage>' +
									'<img src="http://www.smartadserver.com/call/adimppixel/'+trackingHome+'/'+homeTmstp+'?" width=1 height=1 border=0 style="position:absolute;">' +
							 		'<a href="'+l[i].href+'" title="'+l[i].title+'"' +
									' onmousedown="this.href=\'http://www.smartadserver.com/call/cliccommand/'+trackingHome+'/'+homeTmstp+'?\'+escape(this.href);this.onmousedown=null;" ' +
									'>'
				if (displayTeaser) {
					html = html + 	'<img src="/world/semantic/link2img.asp?link='+escape(l[i].href)+'" width=30 height=30 border=0 class="teaser">'
				}
				html = html +		l[i].innerHTML+'</a></li>';
			} else {
				html = html + '<li>' +
									'<a href="'+l[i].href+'" title="'+l[i].title+'"' +
									' onmousedown="this.href=\'http://www.smartadserver.com/call/cliccommand/'+trackingLinks+'/'+mainTmstp+'?\'+escape(this.href);this.onmousedown=null;" ' +
									'>'
				if (displayTeaser) {
					html = html + 	'<img src="/world/semantic/link2img.asp?link='+escape(l[i].href)+'" width=30 height=30 border=0 class="teaser">'
				}
				html = html +		l[i].innerHTML+'</a></li>';
			}
		}
		html = html + '</ul>';
		
		this.mainDiv.innerHTML = html;
		document.body.appendChild(this.mainDiv);
	},
	
	'display': function() {
		if (this.isIE()) {
			this.displayAbsolute();
		} else {
			this.displayFixed();
		}
	},
	
	'displayFixed': function() {
		this.bottom = -300;
		this.mainDiv.style.position = 'fixed';
		this.mainDiv.style.bottom = this.bottom + 'px';
		
		this.displayInterval = setInterval(this.displayFixedCallback, 20);
	},
	'displayFixedCallback': function() {
		af_dynamicSA.bottom += 20;
		af_dynamicSA.mainDiv.style.bottom = af_dynamicSA.bottom + 'px';
		
		if (af_dynamicSA.bottom > 0) {
			clearInterval(af_dynamicSA.displayInterval);
		}
	},
	
	'displayAbsolute': function() {
		this.top = 0;
		this.mainDiv.style.position = 'absolute';
		var windowHeight = af_windowSize().b;
		this.mainDiv.style.top = (windowHeight - this.top) + 'px';
		
		this.displayInterval = setInterval(this.displayAbsoluteCallBack, 20);
	},
	'displayAbsoluteCallBack': function() {
		af_dynamicSA.top += 20;
		var windowHeight = af_windowSize().b;
		af_dynamicSA.mainDiv.style.top = (windowHeight - af_dynamicSA.top) + 'px';
		
		var maxTop = af_dynamicSA.mainDiv.offsetHeight;
		if (af_dynamicSA.top > maxTop) {
			clearInterval(af_dynamicSA.displayInterval);
		}
	},
	
	'updatePosition': function() {
		var windowSize = af_windowSize();
		var windowHeight = windowSize.b;
		
		if (this.isIE()) {
			if (windowHeight >= this.startHeight) {
				this.mainDiv.style.top = (windowHeight - this.top) + 'px';
			}
		} else {
			if (windowHeight < this.startHeight) {
				// Switch to scrollable position...
				if (this.mainDiv.style.position == 'fixed') {
					this.mainDiv.style.position = 'absolute';
					this.mainDiv.style.bottom = (0 - windowSize.t + this.bottom) + 'px';
				}
			} else {
				this.mainDiv.style.position = 'fixed';
				this.mainDiv.style.bottom = this.bottom + 'px';
			}
		}
	},
	
	'isIE': function() {
		return navigator.appName == 'Microsoft Internet Explorer';
	}
}

if (typeof(SITEID) != 'undefined' && SITEID == 1) {
	if (af_getCookie('stopDySA') != '1') {
		if (window.addEventListener) {
			window.addEventListener('scroll', af_dynamicSA.onScroll, false);
		} else if(window.attachEvent) {
			window.attachEvent('onscroll', af_dynamicSA.onScroll);
		}
	}
}