this.display_roadblock_to_logged_in=function(stocks){currentStocks=stocks?stocks.split(","):[];for(var i=0,s;s=currentStocks[i];i++)currentStocks[i]=s.toUpperCase();AlphaBox.Loading({showHeader:true,Locked:true,background:"white"})
var content=[];var needToAdd=5-currentStocks.length;var s=(needToAdd==1?"":"s");content.push("<div style='width:432px; padding:23px;' class='customize_pop_bordeaux mail_sent' id='display_roadblock_to_logged_in'>");content.push("<div style='color:#721314;font-size:16px;line-height:22px;'>To use the Seeking Alpha home page, please add 5 stocks to your portfolio:</div>");if(needToAdd<5)content.push("<div style='color:#4f4f4f;font-size:14px;margin-top:12px;'>You are subscribed to: "+currentStocks.join(", ")+". Please add "+needToAdd+" stock"+(needToAdd==1?"":"s")+":</div>");content.push("<div><input type='text' style='margin-top:5px;width:416px;height:25px;' id='display_roadblock_to_logged_in_text' empty='Enter stocks, separate with a comma'/></div>");content.push("<div style='margin-top:23px;font-size:12px;line-height:17px;'><div style='float:left;'><input type='checkbox' id='display_roadblock_to_logged_in_rta' checked/></div><div>Send me email alerts when news and articles are published on my stocks"+(typeof(emailSubscrCount)=="undefined"?"":"-<strong> "+emailSubscrCount+"</strong> Seeking Alpha users get these alerts")+".</div></div>");content.push("<div style='font-size:14px;color:#4F4F4F;margin-top:15px;'>Your email address is "+SA.Data.User.email()+" [<span style='font-size:14px;color:#024999;cursor:pointer' onclick='logout();'>Sign out</span>]</div>");content.push('<div class="pro_blue_button mt10" style="width:60px;float:left" id="display_roadblock_to_logged_in_click_go">GO!</div>');content.push('<div class="fs12 fl red" style="margin:15px 0 0 5px" id="display_roadblock_to_logged_in_error"></div>');content.push('<div id="roadblock_loader" class="fl mt15 small_loader" style="display:none"></div>');content.push("</div>");AlphaBox.AddHtml(content.join(""));applyTest();var stockText=SA.Utils.Expansion.Input($("display_roadblock_to_logged_in_text"),{autocomplete:{stocks:true}});var error=$("display_roadblock_to_logged_in_error");var button=new SA.Utils.Button({element:$('display_roadblock_to_logged_in_click_go'),onclick:function(){stockText.removeClassName('bcred');error.innerHTML='';if(!stockText.Changed()){stockText.addClassName('bcred');Mone.event("Roadblock - Home Page","Roadblock Add Stocks","Error : Please enter at least "+needToAdd+" stocks.");error.innerHTML="Please enter at least "+needToAdd+" stock"+s+".";return;}
var stocks=SA.Models.StockModel.stringToStocksArray(stockText.value);stocks[0].length>0?stockText.value=stocks[0].join(", "):stockText.Reset();if(stocks[1].length>0){error.innerHTML="*Symbol(s) not recognized: "+SA.Utils.joinWithAnd(stocks[1],", ");Mone.event("Roadblock - Home Page"," Roadblock Add Stocks "," Error : *Symbol(s) not recognized");stockText.addClassName('error');return;}
if(stocks[0].concat(currentStocks).uniq().length<5){stockText.addClassName('error');error.innerHTML="Please enter at least "+needToAdd+" stock"+s+".";Mone.event("Roadblock - Home Page"," Roadblock Add Stocks "," Error : Please enter at least "+needToAdd+" stocks");return;}
var rta=$('display_roadblock_to_logged_in_rta').checked?"rta_market_currents,rta_articles":"";var p=new SA.Models.PortfolioModel();if($('display_roadblock_to_logged_in_rta').checked)
Mone.event("Roadblock - Home Page"," Roadblock Add Stocks ","Add stocks conversion alerts",{data:{before:currentStocks.join(","),after:stocks[0].join(",")}});else Mone.event("Roadblock - Home Page"," Roadblock Add Stocks ","Add stocks conversion no alerts",{data:{before:currentStocks.join(","),after:stocks[0].join(",")}});button.Lock();p.createPortfolio('portfolio',stocks[0].join(","),function(){top.location.reload(true);},function(){button.UnLock()},rta,"Homepage Roadblock Create Portfolio");},loaderObj:$("roadblock_loader")});}
function openPopup(content,special){if(special){AlphaBox.Loading({background:"white",showHeader:true,Locked:true})
AlphaBox.AddHtml(content.join(''),{force:true});}else{AlphaBox.Loading();AlphaBox.AddHtml(content.join(''));}
applyTest();}
function closer(){return"<div class='close_pop_bordeaux' onclick='AlphaBox.Close();'></div>";}
function rta_closer(){return'<div class="rta_pop_article_close" onclick="AlphaBox.Close()">X</div>';}
function cleaner(){return'<div class="cleaner"></div>';}}})();