(function($,h,c){var a=$([]),e=$.resize=$.extend($.resize,{}),i,k="setTimeout",j="resize",d=j+"-special-event",b="delay",f="throttleWindow";e[b]=250;e[f]=true;$.event.special[j]={setup:function(){if(!e[f]&&this[k]){return false}var l=$(this);a=a.add(l);$.data(this,d,{w:l.width(),h:l.height()});if(a.length===1){g()}},teardown:function(){if(!e[f]&&this[k]){return false}var l=$(this);a=a.not(l);l.removeData(d);if(!a.length){clearTimeout(i)}},add:function(l){if(!e[f]&&this[k]){return false}var n;function m(s,o,p){var q=$(this),r=$.data(this,d);r.w=o!==c?o:q.width();r.h=p!==c?p:q.height();n.apply(this,arguments)}if($.isFunction(l)){n=l;return m}else{n=l.handler;l.handler=m}}};function g(){i=h[k](function(){a.each(function(){var n=$(this),m=n.width(),l=n.height(),o=$.data(this,d);if(m!==o.w||l!==o.h){n.trigger(j,[o.w=m,o.h=l])}});g()},e[b])}})(jQuery,this);

/*
 * jQuery mousehold plugin - fires an event while the mouse is clicked down.
 * Additionally, the function, when executed, is passed a single
 * argument representing the count of times the event has been fired during
 * this session of the mouse hold.
 *
 * @author Remy Sharp (leftlogic.com)
 * @date 2006-12-15
 * @example $("img").mousehold(200, function(i){  })
 * @desc Repeats firing the passed function while the mouse is clicked down
 *
 * @name mousehold
 * @type jQuery
 * @param Number timeout The frequency to repeat the event in milliseconds
 * @param Function fn A function to execute
 * @cat Plugin
 * Original path: http://remysharp.com/demo/mousehold.js
 */
jQuery.fn.mousehold=function(c,b){if(c&&typeof c=="function"){b=c;c=100}if(b&&typeof b=="function"){var d=0;var a=0;return this.each(function(){jQuery(this).mousedown(function(){a=1;var f=0;var e=this;d=setInterval(function(){f++;b.call(e,f);a=2},c)});clearMousehold=function(){clearInterval(d);if(a==1){b.call(this,1)}a=0};jQuery(this).mouseout(clearMousehold);jQuery(this).mouseup(clearMousehold)})}};

/* Copyright (c) 2011 Brandon Aaron (http://brandonaaron.net)
 * Licensed under the MIT License (LICENSE.txt).
 *
 * Thanks to: http://adomas.org/javascript-mouse-wheel/ for some pointers.
 * Thanks to: Mathias Bank(http://www.mathias-bank.de) for a scope bug fix.
 * Thanks to: Seamus Leahy for adding deltaX and deltaY
 *
 * Version: 3.0.6
 * 
 * Requires: 1.2.2+
 * Original path: https://raw.github.com/brandonaaron/jquery-mousewheel/master/jquery.mousewheel.js
 */
 (function(a){function d(b){var c=b||window.event,d=[].slice.call(arguments,1),e=0,f=true,g=0,h=0;b=a.event.fix(c);b.type="mousewheel";if(c.wheelDelta){e=c.wheelDelta/120}if(c.detail){e=-c.detail/3}h=e;if(c.axis!==undefined&&c.axis===c.HORIZONTAL_AXIS){h=0;g=-1*e}if(c.wheelDeltaY!==undefined){h=c.wheelDeltaY/120}if(c.wheelDeltaX!==undefined){g=-1*c.wheelDeltaX/120}d.unshift(b,e,g,h);return(a.event.dispatch||a.event.handle).apply(this,d)}var b=["DOMMouseScroll","mousewheel"];if(a.event.fixHooks){for(var c=b.length;c;){a.event.fixHooks[b[--c]]=a.event.mouseHooks}}a.event.special.mousewheel={setup:function(){if(this.addEventListener){for(var a=b.length;a;){this.addEventListener(b[--a],d,false)}}else{this.onmousewheel=d}},teardown:function(){if(this.removeEventListener){for(var a=b.length;a;){this.removeEventListener(b[--a],d,false)}}else{this.onmousewheel=null}}};a.fn.extend({mousewheel:function(a){return a?this.bind("mousewheel",a):this.trigger("mousewheel")},unmousewheel:function(a){return this.unbind("mousewheel",a)}})})(jQuery);
 
 /*
* A jQuery extension to provide scrollbars to a container
* 
* @author Andrew Lowndes
* @date 08/12/10
*
* @requires jquery.js
* @requires jquery.event.drag.js
* @requires jquery.resize.js
* @requires jquery.mousehold.js
* @requires jquery.mousewheel.js
* BREAK NOTES:	This plugin has been modified slightly. This version of aplweb.scrollbars.js plugin
*				has a bug in which vertical scrollbars don't work when horizontal bars are disabled. 
*				Calculation used to check if horizontal scrollbars has been commented out.
*				Original line within updateScrollers() function: 
*					var amountWidth = $scrollcontent.innerWidth() / $scrollwrap.outerWidth();
*				commented out and replaced with this
*					var amountWidth = 0.5;
*				Basically it tricks the plugin into thinking the content width is wider than the container
*				hence requiring the horizontal bars.
*				3-14-2012 RM
*/
$.fn.scrollbars=function(){return $(this).each(function(){var b=$(this);b.addClass("scrollable");var d=$('<div class="scrollcontent"></div>');var c=$('<div class="scrollwrap"></div>');b.wrapInner(c).wrapInner(d);d=b.find(".scrollcontent");c=b.find(".scrollwrap");var j=$('<div class="scroller vscroller"></div>');var k=$('<div class="scrollblock"></div>');var l=$('<div class="scrollarea"></div>');var m=$('<div class="scrollbtn scrolltop">?</div>');var n=$('<div class="scrollbtn scrolldown">?</div>');l.append(k);j.append(l).append(m).append(n);var e=$('<div class="scroller hscroller"></div>');var a=$('<div class="scrollblock"></div>');var g=$('<div class="scrollarea"></div>');var f=$('<div class="scrollbtn scrollleft">?</div>');var i=$('<div class="scrollbtn scrollright">?</div>');g.append(a);e.append(g).append(f).append(i);var o=$('<div class="filler"></div>');b.append(j).append(o).append(e);h();d.add(c).resize(function(){h()});m.mousehold(100,function(){d.scrollTop(d.scrollTop()-40);h()});n.mousehold(100,function(){d.scrollTop(d.scrollTop()+40);h()});f.mousehold(100,function(){d.scrollLeft(d.scrollLeft()-40);h()});i.mousehold(100,function(){d.scrollLeft(d.scrollLeft()+40);h()});k.drag("start",function(q,p){p.origTop=$(this).position().top}).drag(function(r,p){var s=$(this).parent().innerHeight()-$(this).outerHeight();var q=Math.max(0,Math.min(s,p.origTop+p.deltaY));$(this).css({top:q});d.scrollTop((q/s)*(c.outerHeight()-d.innerHeight()))});a.drag("start",function(q,p){p.origLeft=$(this).position().left}).drag(function(s,p){var q=$(this).parent().innerWidth()-$(this).outerWidth();var r=Math.max(0,Math.min(q,p.origLeft+p.deltaX));$(this).css({left:r});d.scrollLeft((r/q)*(c.outerWidth()-d.innerWidth()))});b.mousewheel(function(p,q){d.scrollTop(d.scrollTop()-(q*30));h();return false});l.mousedown(function(q){var r=$(this).innerHeight()-k.outerHeight();var p=Math.max(0,Math.min(r,q.pageY-$(this).offset().top-(k.outerHeight()/2)));k.css({top:p});d.scrollTop((p/r)*(c.outerHeight()-d.innerHeight()))});g.mousedown(function(r){var p=$(this).innerWidth()-a.outerWidth();var q=Math.max(0,Math.min(p,r.pageX-$(this).offset().left-(a.outerWidth()/2)));a.css({left:q});d.scrollLeft((q/p)*(c.outerWidth()-d.innerWidth()))});d.scroll(function(){h()});function h(){var x=d.innerHeight()/c.outerHeight();if(x>=1){b.addClass("no_scroll_v")}else{b.removeClass("no_scroll_v");var r=x*l.innerHeight();var q=c.outerHeight()-d.innerHeight();var s=d.scrollTop()/q;var t=s*(l.innerHeight()-r);k.css({top:t,height:r})}var u=0.5;if(u>=1){b.addClass("no_scroll_h")}else{b.removeClass("no_scroll_h");var v=u*g.innerWidth();var y=c.outerWidth()-d.innerWidth();var w=d.scrollLeft()/y;var p=w*(g.innerWidth()-v);a.css({left:p,width:v})}}})};

/*==================================================================
 *------------END Plugins required for custom scrollbars-----------/
 *==================================================================
*/

