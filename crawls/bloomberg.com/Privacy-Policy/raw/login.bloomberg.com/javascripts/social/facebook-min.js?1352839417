var global_fb_logged_in=false;var button_fb_logged_in=false;window.fbAsyncInit=function(){FB.init({appId:"208624989171045",status:true,cookie:true,xfbml:true,oauth:true});FB.Event.subscribe("auth.login",function(a){if(a.authResponse){BLOOMBERG.user.createCookie("fb_session_token",a.authResponse.accessToken,null,"bloomberg.com",true);if(button_fb_logged_in){loginOrLinkFBUser()}global_fb_logged_in=true}});FB.Event.subscribe("auth.logout",function(a){logout()});FB.getLoginStatus(function(a){if(a.authResponse){BLOOMBERG.user.createCookie("fb_session_token",a.authResponse.accessToken,null,"bloomberg.com",true);global_fb_logged_in=true}})};function logout(){}function loginOrLinkFBUser(){FB.api("/me",function(a){BLOOMBERG.user.loginOrLinkSocialUser("facebook",a.email,a.first_name,a.last_name)})}(function(){}());