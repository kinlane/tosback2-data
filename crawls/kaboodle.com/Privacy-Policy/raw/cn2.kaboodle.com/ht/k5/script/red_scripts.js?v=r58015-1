function fold(a){$(a.target).parents(".item").toggleClass("unfolded")}function overlay(a){$(a.target).next(".item_overlay").toggleClass("hidden")}function initDocReady(){$("select.styled").each(function(){var b=$(this).attr("title");if($("option:selected",this).val()!=""){b=$("option:selected",this).text()}$(this).css({"z-index":10,opacity:0,"-khtml-appearance":"none","-webkit-appearance":"none"}).after('<div class="select">'+b+"</div>").change(function(){val=$("option:selected",this).text();$(this).next().text(val)})});$(".hover_popup").mouseenter(function(b){$(".overlay_box.overlay_popup").hide();$(this).children(".overlay_box.overlay_popup").show()});$(".hover_popup").mouseleave(function(b){$(this).children(".overlay_box.overlay_popup").hide()});$(window).scroll(function(){$("body").toggleClass("fixed",$(this).scrollTop()>document.getElementById("ScrollableHeaderPart").scrollHeight)});$("#SideNavBlock .caption").click(function(b){$(b.target).parents(".item").toggleClass("unfolded");return false});var a=0;$(".selectbox .text").click(function(b){a=1;if(!$(this).next(".options").is(":visible")){$(".selectbox").children(".options").hide();$(this).next(".options").show();$(this).next(".options").children(".item").each(function(c,d){$(this).click(function(){$(b.target).text($(d).text());$(b.target).next(".options").hide()})})}else{$(this).next(".options").hide()}});$(".action_popup").click(function(b){a=2;$(".overlay_box.overlay_popup").hide();$(this).next(".overlay_box.overlay_popup").show();$(this).next(".overlay_box.overlay_popup").find(".menu a").each(function(c,d){$(this).click(function(){$(b.target).next(".overlay_box.overlay_popup").hide()})})});$(".adv_search_box .overlay_popup li a").click(function(b){var c=$(this).parents(".adv_search_box").children(".searchbox_input");if(c.parents().is(".search_no_results")){c.attr("placeholder","Please type a new word or phrase to search for "+$(this).text())}else{c.attr("placeholder","Search for "+$(this).text())}return true});$(".popup_box").click(function(b){a=3;$(".overlay_box").hide();var c=$(this).next(".overlay_box").show();c.find(".buttons").children().each(function(d,e){$(this).click(function(){$(b.target).next(".overlay_box").hide()})})});$(".folding_box .fold_control").click(function(c){if($(this).parents(".folding_box").hasClass("unique")){if(!$(this).parents(".folding_box").children(".folded_items").hasClass("expanded")){var b=$(this).parents(".folding_box").parent();$(b).children(".folding_box.unique").each(function(d){$(this).children(".folded_items").removeClass("expanded");$(this).children(".caption").children(".fold_control").each(function(e){this.innerText="Expand"})});$(this).parents(".folding_box").children(".folded_items").toggleClass("expanded");if($(this).parents(".folding_box").children(".folded_items").hasClass("expanded")){this.innerText="Collapse"}else{this.innerText="Expand"}}}else{$(this).parents(".folding_box").children(".folded_items").toggleClass("expanded");if($(this).parents(".folding_box").children(".folded_items").hasClass("expanded")){this.innerText="Collapse"}else{this.innerText="Expand"}}});$(".folding_box .control.clear_checks").click(function(b){$(this).parents(".folding_box").children(".folded_items").find('input[type="checkbox"]').each(function(c,d){d.checked=false;$(this).siblings("label").removeClass("checked")})});$('input[type="checkbox"]').change(function(b){$(this).siblings("label").toggleClass("checked")});$(window).click(function(b){if(a!=1){$(".selectbox").children(".options").hide()}if(a!=2){$(".action_popup").next(".overlay_box.overlay_popup").hide();$(".search_popup").children(".overlay_box.overlay_popup").hide()}if(a!=4){$(".overlay_box.poll_zoom").hide()}a=0});$("#inputField").bind("input",function(){var b=$(this).attr("maxLength")-$(this).val().length;$(this).parent().siblings("#charCounter").text(b+" characters remaining")});$(".input_field").bind("input",function(){var b=$(this).attr("maxLength")-$(this).val().length;$(this).parent().siblings(".char_counter").text(b+" characters remaining")});$(".input_field").keypress(function(b){var c=$(this).attr("maxLength")-$(this).val().length;$(this).parent().siblings(".char_counter").text(c+" characters remaining")});$(".zoom_popup_action").click(function(c){a=4;$(".overlay_box").hide();var b=$(this).nextAll(".overlay_box.poll_zoom");if(b.length==0){b=$(this).parent().nextAll(".overlay_box.poll_zoom")}b.css("left",-(b.width()-b.parent().width())/2+"px");b.css("top",-(b.height()-b.parent().height())/2+"px");b.show()});$(".question").click(function(b){$(this).parent().toggleClass("expanded")})};