function adn_ta_call_functions()
	{
		
		
		
		var r = 0 + Math.floor(Math.random() * (adn_top_autos.length));
				
		var adn_selected_top_auto_src = adn_top_autos[r];
		var functions_file = 'http://www.adn.com/includes/classified/top_autos/top_autos_functions.level2.js?2';
		document.write('<scr' + 'ipt language="JavaScript" src="'+ adn_selected_top_auto_src +'" type="text/javascript"></scr' + 'ipt>');
		document.write('<scr' + 'ipt language="JavaScript" src="'+ functions_file + '" type="text/javascript"></scr' + 'ipt>');

	}
