    s.prop18 = ""; // clear out previous link names
    s.linkTrackVars = "server,zip,prop2,prop4,prop26,prop27,prop28,prop18,eVar2,products,events,purchaseID,prop41,prop17,prop24,prop45,eVar12,prop24";

    s.tl(o_this, 'o', pagename_description.toLowerCase());

    /* Clear omniture tracking properties */
