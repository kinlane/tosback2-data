function reSubmitOmniture(pagename, detailedpagename, leadid, category, manufacturerid, modelid, vehicleid, yearid, compareids) {
    s = s_gi(s_account);
    s.pageName = pagename.toLowerCase();
    if (detailedpagename != "") {
        s.prop2 = detailedpagename.toLowerCase();
    }
    if (leadid != undefined && leadid != "") {
        s.prop24 = leadid.toLowerCase();
    }
    if (category != undefined) {
        s.prop3 = category.toLowerCase();
    }
    if (yearid != undefined) {
        s.prop4 = yearid;
    }
    if (manufacturerid != undefined) {
        s.prop26 = manufacturerid;
    }
    if (modelid != undefined) {
        s.prop27 = modelid;
    }
    if (vehicleid != undefined) {
        s.prop28 = vehicleid;
    }
    if (compareids != undefined) {
        s.prop29 = compareids;
    }

    s.prop17 = IncrementSession();

    s.dynamicAccountList = s_account;
    s.t();

    /* Clear omniture tracking properties */
    if (KBB && KBB.get('omniture.reset')) {
        KBB.omniture.reset(s);
    }
}

function submitOmniture() {
    s = s_gi(s_account);
    s.dynamicAccountList = s_account;
    s.t();

    /* Clear omniture tracking properties */
    if (KBB && KBB.get('omniture.reset')) {
        KBB.omniture.reset(s);
    }
}

function pageView(pagename, detailedpagename) {
    vars = new Object();
    vars.pageName = pagename.toLowerCase(); ;
    if (detailedpagename != "") {
        vars.prop2 = detailedpagename.toLowerCase();
        vars.prop17 = IncrementSession();
    }
    s.t(vars);

    /* Clear omniture tracking properties */
    if (KBB && KBB.get('omniture.reset')) {
        KBB.omniture.reset(s);
    }
}

function IncrementSession() {

    return (Math.random() * 10000000000000000).toFixed(0);
}

