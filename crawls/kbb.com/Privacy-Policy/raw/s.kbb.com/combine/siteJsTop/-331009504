        }
    }
})();

var KbbErrorHandler = KbbErrorHandler || (function () {
    return {
        onerror: function (m, u, n) {
            KBB.log('Message: ' + m + ' Line: ' + n + ' Source: ' + u);
        },
        handle: function (e) {
            KbbErrorHandler.onerror(e.message, e.fileName || e.name, e.lineNumber || e.number);
        }
    }
})();

