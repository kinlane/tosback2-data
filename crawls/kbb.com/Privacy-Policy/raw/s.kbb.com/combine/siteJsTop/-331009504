KBB.ads.mHideByClassHistory = new Array();

var mHideByClass = function (className) {
    var module = jQuery("." + className);
    if (module != null) {
        jQuery(module).hide();
        if (className == 'mrecshowcase') {
            $("#Mrec-container").removeClass("with-showcase");
        }
        $(document).trigger('AdCollapsed', { adClass: className });
        KBB.ads.mHideByClassHistory.push(className);
    }
};;


/* Comment Generated by Combres - Resource '~/js/kbb/jquery.errorHandler.js' (Mode: Static) */
/* This little guy will, not only allow jQuery to continue with .ready() calls (after one has failed).. 
but, when JS error logging is enable it will log them too */

(function ($) {
    var jQueryReady = $.ready;
    $.ready = function () {
        try {
            return jQueryReady.apply(this, arguments);
        } catch (e) {
            if (KbbErrorHandler) {
                KbbErrorHandler.handle(e);
            }
            /* This line is useful if you would like the degugger to raise the event. */
            //throw e;
        }
    };
    if (KbbErrorHandler) {
        $(document).ajaxError(function (event, xhr, ajaxOptions, thrownError) {
            KbbErrorHandler.handle({
                message: '[Ajax Error] Error: "{0} [{1}]" AjaxUrl: {2} AjaxData: {3}'.format(thrownError,
                                                                                             xhr.status,
                                                                                             ajaxOptions.url,
                                                                                             JSON ? JSON.stringify(ajaxOptions.data) : '')
            });
        });
    }
} (jQuery));


/* Comment Generated by Combres - Resource '~/js/kbb/KBB.cookies.js' (Mode: Static) */
var KBB = KBB || {};

if (typeof Array.prototype.map !== 'function') {
    Array.prototype.map = function (callback, thisArg) {
        for (var i = 0, n = this.length, a = []; i < n; i++) {
            if (i in this) a[i] = callback.call(thisArg, this[i]);
        }
        return a;
    };
}

KBB.cookies = (function (undefined) {
    var format = '{0}={1};expires={2};path={3};domain={4}',
        isCookiesSupported;

    function getCookies() {
        var c = document.cookie,
            v = 0,
            cookies = {};

        if (document.cookie.match(/^\s*\$Version=(?:"1"|1);\s*(.*)/)) {
            c = RegExp.$1;
            v = 1;
        }
        if (v === 0) {
            c.split(/[,;]/).map(function (cookie) {
                var parts = cookie.split(/=/, 2),
                name = decodeURIComponent(parts[0].replace(/^\s+/, '')),
                value = parts.length > 1 ? decodeURIComponent(parts[1].replace(/\s+$/, '')) : null;
                cookies[name] = value;
            });
        } else {
            c.match(/(?:^|\s+)([!#$%&'*+\-.0-9A-Z^`a-z|~]+)=([!#$%&'*+\-.0-9A-Z^`a-z|~]*|"(?:[\x20-\x7E\x80\xFF]|\\[\x00-\x7F])*")(?=\s*[,;]|$)/g).map(function ($0, $1) {
                var name = $0,
                value = $1.charAt(0) === '"'
                          ? $1.substr(1, -1).replace(/\\(.)/g, "$1")
                          : $1;
                cookies[name] = value;
            });
        }
        return cookies;
    }

    return {
        set: function (name, value, days, path, domain) {
            var date = new Date();

            days = days || 30;
            path = path || '/';
            domain = domain || '.kbb.com';

            date.setTime(date.getTime() + (days * 86400000));
            this.setWithDateExpiration(name, value, date.toGMTString(), path, domain);
        },
        setWithDateExpiration: function(name, value, expirationDateStr, path, domain) {
            path = path || '/';
            domain = domain || '.kbb.com';
            document.cookie = format.format(name, escape(value), expirationDateStr, path, domain);
        },
        all: function () {
            return getCookies();
        },
        get: function (name) {
            if (!name || !name.length || !document.cookie) return undefined;
            return this.all()[name];
        },
        remove: function (name, path, domain) {
            if (this.get(name)) {
                try {
                    path = path || '/';
                    domain = domain || '.kbb.com';
                    document.cookie = format.format(name, '', "Thu, 01-Jan-1970 00:00:01 GMT", path, domain);
                } catch (e) { }
            }
        },
        enabled: function () {
            if (isCookiesSupported === undefined) {
                try {
                    var value = "testing", key = "kbb_test_cookie";
                    KBB.cookies.set(key, value, 1);
                    isCookiesSupported = value === KBB.cookies.get(key);
                    KBB.cookies.remove(key);
                } catch (e) { /* Gulp! */ }
            }
            return isCookiesSupported;
        }
    }
})();;


