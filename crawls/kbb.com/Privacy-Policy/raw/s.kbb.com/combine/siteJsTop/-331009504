    s.products = products;
    s.linkTrackVars = "server,zip,prop2,prop4,prop26,prop27,prop28,prop18,eVar2,products,events,purchaseID,prop41,prop45,eVar12,prop17";

    s.tl(o_this, 'o', s.pageName.toLowerCase());

