function AddPartner(partner, pagename_description, ec_partnercode) {
    if (partner && partner.length > 0) {
        if (s.products && s.products.length > 0) {
            s.products += ",";
        }
        if (ec_partnercode && ec_partnercode.indexOf("kbbmain") < 0) {
            ec_partnercode = "";
        }
        s.products += (ec_partnercode + ";" + partner + ":" + pagename_description + ";1;0.00").toLowerCase();
    }
}



function AddPartnerImpressionBatch(partner, pagename_description, ec_partnercode) {


    if (partner && partner.length > 0) {
        if (s.tempProducts && s.tempProducts.length > 0) {
            s.tempProducts += ",";
        }
        if (ec_partnercode && ec_partnercode.indexOf("kbbmain") < 0) {
            ec_partnercode = "";
        }
        s.tempProducts += ec_partnercode.toLowerCase() + ";" + partner.toLowerCase() + ":" + pagename_description.toLowerCase() + ";1;0.00";
    }

};


/* Comment Generated by Combres - Resource '~/js/kbb/KBB.cache.js' (Mode: Static) */
var KBB = KBB || {};

KBB.cache = (function (undefined) {
    var _userCache = (function () { try { if (localStorage.setItem) { return localStorage; } } catch (e) { return {}; } } ()) || {},
        _localCache = {},
        isLocalStorage = typeof _userCache.setItem === 'function',
        persistMethods = { LocalStorage: 0, LocalVariable: 1 };

    return {
        persistMethods: persistMethods,
        isLocalStorage: isLocalStorage,
        add: function (key, value, persistMethod) {
            if (!key) return;
            try {
                var cache = (persistMethod && persistMethod === persistMethods.LocalStorage) ? _localCache : _userCache;
                cache[key] = JSON.stringify(value);
            } catch (err) {
                KbbErrorHandler.handle(err);
            }
            return true;
        },
        get: function (key) {
            var item = _userCache[key] || _localCache[key];
            if (item === 'undefined' || item === undefined || item === null)
                return undefined;
            return JSON.parse(item);
        },
        remove: function (key) {
            _userCache[key] = undefined;
            _localCache[key] = undefined;
            return true;
        },
        containsKey: function (key) {
            return _userCache[key] !== undefined || _localCache[key] !== undefined;
        },
        clear: function () {
            for (var i in _localCache) {
                this.remove(i);
            }
            if (isLocalStorage) {
                _userCache.clear();
                KBB.log('Cleared cache stored in localStorage');
            }
            else {
                for (var i in _userCache) {
                    this.remove(i);
                }
                KBB.log('Cleared cache from local array');
            }
        },
        length: function () {
            return (_userCache.length || 0) + (_localCache.length || 0);
        },
        ajaxGet: function (url, data, cb, persistMethod) {
            if (!$.isFunction(cb)) {
                KBB.log('A valid callback was not supplied to KBB.cache.ajaxGet: {0}'.format(url));
                return;
            }

            var cacheKey = url;
            for (var key in data) {
                cacheKey += '|{0}-{1}'.format(key, data[key]);
            }

            var item = KBB.cache.get(cacheKey);

            if (!(item === undefined || item === null)) {
                KBB.log('[CACHE] Ajax call prevented {0}'.format(url));
                cb(item);
            } else {
                $.get(url, data, function (_data) {
                    KBB.cache.add(cacheKey, _data, persistMethod);
                    cb(_data);
                });
            }
        }
    }
} ());

/* This guy checks to see if we have any cache stored for a prior data or assembly version */
(function () {
    if (!KBB.cache.isLocalStorage || !KBB.versions.assembly) return;
    if (KBB.cache.get('kbb.assemblyversion') != KBB.versions.assembly) {
        KBB.cache.clear();
        KBB.cache.add('kbb.assemblyversion', KBB.versions.assembly);
    }
} ());;


/* Comment Generated by Combres - Resource '~/js/omniture.extension.js' (Mode: Static) */
/// <reference path="KBB\KBB.js" />
/* 
Omniture Extensions for JavaScript.  
              
----------------------------
--Partner Types Definition--
----------------------------
0 - partner click (Default)
1 - partner click and impression
2 - partner click and impression on click
3 - partner impression
4 - partner impression on click
99 - partner impression batch

------------------
--Explicit Calls--
------------------

[Click event is attached, via jQuery, to the body (Using .delegate(fn))]

<a href="#" data-omn-partner="nx" 
data-omn-page="foo_${s.prop2}_lnk" />   -- Partner Click
                
<a href="#" data-omn-partner="nx" 
data-omn-page="foo_${s.prop2}_lnk" 
data-omn-type="1"
data-omn-onclick="MyFunction();" />     -- Client Impression & Partner Click
- w/ a custom event that will fire AFTER the PartnerClick event.

------------------
--Implicit calls--
------------------

KBB.omniture.impression
KBB.omniture.PartnerClick 

The following example shows an Impression call from within a jQuery handler:
Assuming the data-omn-* attributes have been applied to the element:        
...
$('a[data-omn-partner]').click(function() 
{ 
KBB.omniture.impression.call(this); 
return false; 
});
...
Otherwise,     
...
$('a[data-omn-partner]').click(function() 
{ 
var omnitureType = 0, // Partner Click
omnitureDetails = new KBB.omniture.OmnitureDetails('nx', 'pagename', omnitureType);

KBB.omniture.impression.call(this, omnitureDetails); 
return false; 
});
...

-----------------
--jQuery Plugin--
-----------------

This is useful for instances when you already have a click event defined, which in turn 
may; return false, stopPropagation, redirect etc.. anything that may stop the event from
bubbling up to the attached delegate. This will prevent the partner click from being called.

To solve this issue, provide a data-omn-onclick attribute with the element. Example:

<a href="#" data-omn-partner="nx" 
data-omn-page="foo_${s.prop2}_lnk" 
data-omn-onclick="MyMethod();" />
*/

var KBB = KBB || {};

KBB.omniture = (function (kbb) {

    function getPartnerLinkDetails() {
        var _this = $(this);

        if (!_this || !_this.length) return;

        var partner = $(this).data('omn-partner'),
               page = $(this).data('omn-page'),
               type = $(this).data('omn-type'),
               events = $(this).data('omn-events');

        return new KBB.omniture.OmnitureDetails(page, partner, type, events);
    }

    function onClickComplete(omnitureDetails, e) {
        var runScript = $(this).data('omn-onclick');

        if (typeof (runScript) !== 'undefined' && runScript !== false) {
            (new Function(runScript)).call({ obj: this, omniture: omnitureDetails, event: e });
        }
    }

    return {
        /* Object to hold our omniture variables */
        OmnitureDetails: function (page, partner, type, events, mobileProp) {
            if (!page || !page.length) return;
            this.page = $.tmpl(page).text().toLowerCase();
            this.partner = (partner || 'nx').toLowerCase();
            this.type = type || 0;
            this.events = (events ? events.toLowerCase() : '');
        },
        PartnerImpression: function (event) {
            var e = event,
                o = typeof event !== 'undefined' && event instanceof KBB.omniture.OmnitureDetails
                         ? event : getPartnerLinkDetails.call(this);
            if (!o) return;
            KBB.omniture.impression(o.page, o.partner);
        },
        PartnerClick: function (event) {
            var e = event,
                o = typeof event !== 'undefined' && event instanceof KBB.omniture.OmnitureDetails
                         ? event : getPartnerLinkDetails.call(this);

            if (o.type > 3 && o.type < 99) { /* Are we firing an impression? */
                KBB.omniture.impression(o.page, o.partner);
            }
            if (o.type < 3 || o.type == 99) { /* Are we firing a partnerlink? */
                KBB.omniture.link(o.page, o.partner, this, '', o.events);
            }
            if ($(this).attr('data-omn-onclick')) {
                onClickComplete.call(this, o, e);
            }
        },
        AddPartnerImpressionBatch: function (event) {
            var e = event,
            o = typeof event != 'undefined' && event instanceof KBB.omniture.OmnitureDetails
                         ? event : getPartnerLinkDetails.call(this);

            AddPartnerImpressionBatch(o.partner, o.page, '');
        },
        PartnerBatchImpressionSubmit: function () {

            var batchPartnerImpressions = $('[data-omn-type="99"]:visible');
            if (batchPartnerImpressions != undefined && batchPartnerImpressions.length > 0) {
                s.tempProducts = "";

                $('[data-omn-type="99"]:visible').each(function (e) {
                    batchPartners = true;
                    KBB.omniture.AddPartnerImpressionBatch.call(this, e);
                });
            }
        },
        resubmit: function (pagename, detailedpagename, leadid, category, manufacturerid, modelid, vehicleid, yearid, compareids) {
            reSubmitOmniture(pagename || s.pageName, detailedpagename || s.prop2, leadid, category, manufacturerid, modelid, vehicleid, yearid, compareids);
            KBB.omniture.reset();
        },
        submit: function () {
            submitOmniture();
            KBB.omniture.reset();
        },
        impression: function (pagename_description, partner, o_this, ec_partnercode) {
            AddPartnerClientImpression(partner || 'nx', pagename_description || s.prop2, ec_partnercode || '', o_this || this);
            KBB.omniture.reset();
        },
        link: function (page, partner, o_this, om_events) {
            PartnerLink(partner || 'nx', page || s.prop2, '', o_this || this, om_events || '');
            KBB.omniture.reset();
        },
        reset: function (omnitureObject) {
            var obj = omnitureObject || s;

            if (obj.dynamicAccountList) obj.dynamicAccountList = '';
            if (obj.linkName) obj.linkName = '';
            if (obj.events) obj.events = '';
            if (obj.products) obj.products = '';
            if (obj.prop18) obj.prop18 = '';
            if (obj.eVar2) obj.eVar2 = ''; ;
            if (obj.linkTrackVars) obj.linkTrackVars = '';
            if (obj.linkTrackEvents) obj.linkTrackEvents = '';
        }
    };
} (KBB));

(function ($) {
    $(document).ready(function () {
        /* Wire up handler for all elements tagged with 'data-omn-page' */
        $('body').undelegate('.partnerLinks')
                 .delegate('[data-omn-page]', 'click.partnerLinks', KBB.omniture.PartnerClick);

        /* Fire our impressions */
        if (KBB.config.enableOmniture) {
            $('[data-omn-type="1"]:visible, [data-omn-type="3"]:visible').each(function (e) {
                KBB.omniture.PartnerImpression.call(this);
            });
        }
        KBB.omniture.PartnerBatchImpressionSubmit.call(this);
    });
} (jQuery));;


