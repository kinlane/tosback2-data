    return {
        enableOmniture: true,
        enableLogging: _isDevelopment
    };
})();

KBB.zip = (function () {
    var regexValidZip = /^(\d{5}-\d{4})|(\d{5})$/,
        lookupCache = {};
    return {
        clear: function () {
            $(window).trigger('ZipCodeChanging');

            KBB.cookies.remove('PersistentZipCode');
            KBB.cookies.remove('ZipCode');

            $(window).trigger('ZipCodeChanged', ['']);
        },
        get: function () {
            return KBB.cookies.get('PersistentZipCode') || KBB.cookies.get('ZipCode');
        },
        set: function (zip) {
            var args = $.makeArray(arguments);

            zip = jQuery.trim(zip);

            $(window).trigger('ZipCodeChanging', args);

            if (!this.validateFormat(zip) || !this.validate(zip)) {
                return false;
            }
            KBB.cookies.set('PersistentZipCode', zip, 365);
            KBB.cookies.set('ZipCode', zip);

            $(window).trigger('ZipCodeChanged', args);
            return true;
        },
        validate: function (zip, isAysnc) {
            var validZip = false,
                deferred,
                cachebusterrnd = Math.random() * 10000000000000000
            deferred = jQuery.ajax({
                cache: false,
                type: 'POST',
                url: '/zipcode/validate',
                data: { zip: zip, cbrnd: cachebusterrnd },
                async: isAysnc || false,
                success: function (data) { validZip = data; }
            });
            return isAysnc ? deferred : validZip;
        },
        validateFormat: function (zip) {
            return !!zip && !!zip.length && regexValidZip.test(zip);
        },
        getZipInfo: function (zip, isAysnc) {
            var zipInfo = false,
                deferred;
            deferred = jQuery.ajax({
                type: 'POST',
                url: '/zipcode/GetZipInfo',
                data: { zip: zip || KBB.zip.get() },
                async: isAysnc || false,
                success: function (data) {
                    zipInfo = data;
                    if (isAysnc) {
                        if (!data || !data.IsValid) {
                            $.window.trigger('GeoCodeFailed', ['ZipFail', data && !data.IsValid ? 'Invalid zip code' : 'no data']);
                        }
                        $.window.trigger('GeoCodeComplete', [data]);
                    }
                },
                error: function (e) { $.window.trigger('GeoCodeFailed', ['ZipFail', e]); }
            });
            return isAysnc ? deferred : zipInfo;
        },
        lookup: function (coords) {
            if (!coords || !coords.latitude || !coords.longitude) throw TypeError();
            var cacheKey = (coords.latitude + ' ') + coords.longitude,
                success = function (data) {
                    if (!data || !data.IsValid) {
                        $.window.trigger('GeoCodeFailed', ['ZipFail', data && !data.IsValid ? 'Invalid zip code' : 'no data']);
                    } else {
                        KBB.zip.set(data.Zip, coords);
                    }
                    $.window.trigger('GeoCodeComplete', [data]);
                };

            if (cacheKey in lookupCache) {
                return jQuery.Deferred()
                             .done(success)
                             .resolve(lookupCache[cacheKey]);
            }

            return jQuery.post('/zipcode/getzipinfobylatlong', coords)
                         .fail(function (e) { $.window.trigger('GeoCodeFailed', ['ZipFail', e]); })
                         .done(success)
                         .done(function (data) {
                             if (!(cacheKey in lookupCache)) {
                                 /* Coords will likely change often, so let's just store the last lookup */
                                 lookupCache = {};
                                 lookupCache[cacheKey] = data;
                             }
                         });
        }
    }
})();

