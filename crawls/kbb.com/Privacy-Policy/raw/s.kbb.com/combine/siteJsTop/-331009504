    s.linkTrackEvents = "event10,event11,event51,event52,event53,event54,event55,event56,event57,event58,event59,event61,event62,event71,event72,event73";
    s.tl(o_this, 'o', action);

    /* Clear omniture tracking properties */
    if (KBB && KBB.get('omniture.reset')) {
        KBB.omniture.reset(s);
    }
}

