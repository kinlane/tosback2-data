var encHTML = function (html) {
    return html.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/"/g, '&quot;').replace(/'/g, '&#39;');
}

var encURL = function (url) {
    return url.replace(/-/g, '_').replace(/ /g, '-').replace(/[^a-zA-Z0-9-_)]+/g, '').toLowerCase();
}

