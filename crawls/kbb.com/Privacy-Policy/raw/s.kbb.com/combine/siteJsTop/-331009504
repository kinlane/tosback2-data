Modernizr.addTest('safari',function(){return!navigator.userAgent.match(/Chrome/i)&&!navigator.userAgent.match(/iPhone|iPad|iPod/i)&&!!navigator.userAgent.match(/Safari/i)});;


/* Comment Generated by Combres - Resource '~/js/kbb/ads/KBB.ads.zipoverlay.js' (Mode: Static) */
/* TODO: Move this to KBB.ads.zipOverlayController
KBB.ads.zipOverlayController = (function () {
    return {
        init: function () {
        }
    }
})();
*/
function zipCodeBackOverlay(loc, sid, ref) {
    var ps = loc.indexOf("#");
    if (ps > 0) {
        var pos = loc.lastIndexOf("zipo=");
        if (pos > 0) {
            var dsrest = loc.substr(pos).split('&');
            var zipo = dsrest[0].split('=');
            if (zipo[1] == sid) {
                location.href = ref;
                return;
            }
        }
    }
    else {
        location.href = location.href + "#zipo=" + sid;
    }
}

// zip code overlay ad

function zipAdExpandInit() {
    jQuery("#Zip-code-ad .hide-for-expand").hide();
    jQuery("#Zip-code-ad .show-for-expand").show();
    jQuery("#Vehicle-image").css("left", "10px");
    var input = jQuery("#Zip-code-ad input");
    input.val('ENTER ZIP').on({
        focus: function () {
            jQuery(this).val('');
        },
        blur: function () {
            if (jQuery(this).val() == '') {
                jQuery(this).val('ENTER ZIP');
            }
        }
    });
    

    var topPos = jQuery("#zipCode").offset().top;

    jQuery("#Zip-code-ad").css({
        "top": (topPos + jQuery("#zipCode").height() - 32) + "px"
        
    }).show();
}

function zipOverlayAdInit() {
    /* shift overlay for ZipCodeSponsorship ad */
    jQuery('#zipCode').css('top', '24px');
};


