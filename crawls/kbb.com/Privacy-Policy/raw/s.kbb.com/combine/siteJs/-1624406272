    try {
        var adMessage = JSON.parse(e.data);
        if (adMessage.dyn && (adMessage.type === 'OverlayAd'))
            showToolbarAdOverlay(adMessage.dyn);
    } catch (error) {
    }
}, self.location.protocol + '//' + self.location.host);
